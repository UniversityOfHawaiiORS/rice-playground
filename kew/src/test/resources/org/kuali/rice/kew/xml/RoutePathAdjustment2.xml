<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType overwriteMode="true">
      <name>LoadRoutePathOnlyAdjustsDocument</name>
      <blanketApproveWorkgroupName>KR-WKFLW:WorkflowAdmin</blanketApproveWorkgroupName>
      <routePaths>
        <routePath>
          <start name="First" nextNode="Second" />
          <requests name="Second" />
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="First">
          <exceptionWorkgroupName>KR-WKFLW:TestWorkgroup</exceptionWorkgroupName>
          <activationType>S</activationType>
        </start>
        <requests name="Second">
          <activationType>S</activationType>
          <ruleTemplate>TestRuleTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
      </routeNodes>
    </documentType>
  </documentTypes>
</data>