<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
	<groups xmlns="ns:workflow/Group" xsi:schemaLocation="ns:workflow/Group resource:Group">
      <group>
        <name>AIWG1</name>
        <namespace>KR-WKFLW</namespace>
        <description>AIWG1</description>
        <members>
          <principalName>ewestfal</principalName>
          <principalName>rkirkend</principalName>
        </members>
      </group>
      <group>
        <name>AIWG2</name>
        <namespace>KR-WKFLW</namespace>
        <description>AIWG2</description>
        <members>
          <principalName>jhopf</principalName>
          <principalName>bmcgough</principalName>
        </members>
      </group>
      <group>
        <name>AIWGNested1</name>
        <namespace>KR-WKFLW</namespace>
        <description>AIWGNested1</description>
        <members>
          <principalName>natjohns</principalName>
          <groupName>
            <name>AIWG1</name>
            <namespace>KR-WKFLW</namespace>
          </groupName>
        </members>
      </group>
      <group>
        <name>AIWGNested2</name>
        <namespace>KR-WKFLW</namespace>
        <description>AIWGNested1</description>
        <members>
          <principalName>pmckown</principalName>
          <groupName>
            <name>AIWG2</name>
            <namespace>KR-WKFLW</namespace>
          </groupName>
          <groupName>
            <name>AIWGNested1</name>
            <namespace>KR-WKFLW</namespace>
          </groupName>
        </members>
      </group>
    </groups>
    <ruleTemplates xmlns="ns:workflow/RuleTemplate" xsi:schemaLocation="ns:workflow/RuleTemplate resource:RuleTemplate">
		<ruleTemplate>
			<name>RuleTemplate1</name>
			<description>RuleTemplate1</description>
		</ruleTemplate>
		<ruleTemplate>
			<name>RuleTemplate2</name>
			<description>RuleTemplate1</description>
		</ruleTemplate>
		<ruleTemplate>
			<name>RuleTemplate3</name>
			<description>RuleTemplate1</description>
		</ruleTemplate>
	</ruleTemplates>
	<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
		<documentType>
			<name>ActionItemDocumentType</name>
			<description>TestDocumentType</description>
			<label>TestDocumentType</label>
			<postProcessorName>org.kuali.rice.kew.postprocessor.DefaultPostProcessor</postProcessorName>
			<superUserWorkgroupName>KR-WKFLW:TestWorkgroup</superUserWorkgroupName>
			<docHandler>_blank</docHandler>
			<blanketApproveWorkgroupName>KR-WKFLW:TestWorkgroup</blanketApproveWorkgroupName>
			<defaultExceptionWorkgroupName>KR-WKFLW:TestWorkgroup</defaultExceptionWorkgroupName>
			<policies>
				<policy>
					<name>PRE_APPROVE</name>
					<value>false</value>
				</policy>
				<policy>
					<name>DEFAULT_APPROVE</name>
					<value>false</value>
				</policy>
			</policies>
			<routePaths>
				<routePath>
					<start name="AdHoc" nextNode="Split" />
					<split name="Split" nextNode="RuleTemplate3">
						<branch name="B1">
							<requests name="RuleTemplate1" nextNode="Join" />
						</branch>
						<branch name="B2">
							<requests name="RuleTemplate2" nextNode="Join" />
						</branch>
						<join name="Join" />
					</split>
					<requests name="RuleTemplate3" />
				</routePath>
			</routePaths>
			<routeNodes>
				<start name="AdHoc">
					<activationType>P</activationType>
				</start>
				<split name="Split" />
				<join name="Join" />
				<requests name="RuleTemplate1">
					<activationType>S</activationType>
					<ruleTemplate>RuleTemplate1</ruleTemplate>
				</requests>
				<requests name="RuleTemplate2">
					<activationType>S</activationType>
					<ruleTemplate>RuleTemplate2</ruleTemplate>
				</requests>
				<requests name="RuleTemplate3">
					<activationType>S</activationType>
					<ruleTemplate>RuleTemplate3</ruleTemplate>
				</requests>
			</routeNodes>
		</documentType>
		<documentType>
			<name>BAOrphanedRequestDocumentType</name>
			<label>Document Type used to test that Blanket Approval is not orphaning Action Requests</label>
			<postProcessorName>org.kuali.rice.kew.postprocessor.DefaultPostProcessor</postProcessorName>
			<superUserWorkgroupName>KR-WKFLW:TestWorkgroup</superUserWorkgroupName>
			<docHandler>none</docHandler>
			<blanketApproveWorkgroupName>KR-WKFLW:TestWorkgroup</blanketApproveWorkgroupName>
			<defaultExceptionWorkgroupName>KR-WKFLW:TestWorkgroup</defaultExceptionWorkgroupName>
			<policies>
				<policy>
					<name>PRE_APPROVE</name>
					<value>false</value>
				</policy>
				<policy>
					<name>DEFAULT_APPROVE</name>
					<value>false</value>
				</policy>
			</policies>
			<routePaths>
				<routePath>
					<start name="AdHoc" nextNode="RuleTemplate1" />
					<requests name="RuleTemplate1" nextNode="RuleTemplate2" />
					<requests name="RuleTemplate2" nextNode="RuleTemplate3"/>
					<requests name="RuleTemplate3" />
				</routePath>
			</routePaths>
			<routeNodes>
				<start name="AdHoc">
					<activationType>P</activationType>
				</start>
				<requests name="RuleTemplate1">
					<activationType>S</activationType>
					<ruleTemplate>RuleTemplate1</ruleTemplate>
				</requests>
				<requests name="RuleTemplate2">
					<activationType>S</activationType>
					<ruleTemplate>RuleTemplate2</ruleTemplate>
				</requests>
				<requests name="RuleTemplate3">
					<activationType>S</activationType>
					<ruleTemplate>RuleTemplate3</ruleTemplate>
				</requests>
			</routeNodes>
		</documentType>
	</documentTypes>
	<rules xmlns="ns:workflow/Rule" xsi:schemaLocation="ns:workflow/Rule resource:Rule">
		<rule>
			<documentType>ActionItemDocumentType</documentType>
			<ruleTemplate>RuleTemplate1</ruleTemplate>
			<description>RuleTemplate1</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<workgroup>KR-WKFLW:TestWorkgroup</workgroup>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>ActionItemDocumentType</documentType>
			<ruleTemplate>RuleTemplate2</ruleTemplate>
			<description>RuleTemplate2</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<workgroup>KR-WKFLW:WorkflowAdmin</workgroup>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>

		<!-- Rules for the Orphaned request test -->
		<rule>
			<documentType>BAOrphanedRequestDocumentType</documentType>
			<ruleTemplate>RuleTemplate1</ruleTemplate>
			<description>RuleTemplate1</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>rkirkend</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>BAOrphanedRequestDocumentType</documentType>
			<ruleTemplate>RuleTemplate2</ruleTemplate>
			<description>RuleTemplate2</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<user>user1</user>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
		<rule>
			<documentType>BAOrphanedRequestDocumentType</documentType>
			<ruleTemplate>RuleTemplate3</ruleTemplate>
			<description>RuleTemplate3</description>
			<ignorePrevious>false</ignorePrevious>
			<responsibilities>
				<responsibility>
					<workgroup>KR-WKFLW:TestWorkgroup</workgroup>
					<actionRequested>A</actionRequested>
					<priority>1</priority>
				</responsibility>
			</responsibilities>
		</rule>
	</rules>
</data>