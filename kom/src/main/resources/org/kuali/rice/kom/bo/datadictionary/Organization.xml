<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="Organization" parent="Organization-parentBean" />

  <bean id="Organization-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kom.bo.Organization" />
    <property name="inquiryDefinition" >
      <ref bean="Organization-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="Organization-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="id" />
    <property name="objectLabel" value="Organization" />
    <property name="collections" >
      <list>
        <bean parent="CollectionDefinition"
              p:shortLabel="OrganizationsContexts"
              p:summary="Organizations Contexts"
              p:elementLabel="Contexts"
              p:name="organizationsContexts"
              p:label="Organization Contexts"
              p:description="Contexts of this Organization" />
      </list>
    </property>
    <property name="attributes" >
      <list>
        <ref bean="Organization-id" />
        <ref bean="Organization-shortName" />
        <ref bean="Organization-name" />
        <ref bean="Organization-organizationCategory.name" />
        <ref bean="Organization-organizationsContexts.contextName" />
        <ref bean="Organization-parent.name" />
        <ref bean="Organization-organizationsContexts.active" />
        <ref bean="Organization-active" />
        <ref bean="Organization-parentOrganizationId" />
        <ref bean="Organization-categoryId" />
        <ref bean="Organization-versionNumber" />
        <ref bean="Organization-objectId" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="Organization-id" parent="Organization-id-parentBean" />

  <bean id="Organization-id-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="name" value="id" />
    <property name="label" value="ID" />
  </bean>
  <bean id="Organization-shortName" parent="Organization-shortName-parentBean" />

  <bean id="Organization-shortName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Short Name" />
    <property name="maxLength" value="255" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="shortName" />
    <property name="label" value="Short Name" />
  </bean>
  <bean id="Organization-name" parent="Organization-name-parentBean" />

  <bean id="Organization-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="255" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="name" />
    <property name="label" value="Organization Name" />
  </bean>
  <bean id="Organization-organizationCategory.name" parent="Organization-organizationCategory.name-parentBean" />

  <bean id="Organization-organizationCategory.name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="255" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="organizationCategory.name" />
    <property name="label" value="Category" />
  </bean>
  <bean id="Organization-organizationsContexts.contextName" parent="Organization-organizationsContexts.contextName-parentBean" />

  <bean id="Organization-organizationsContexts.contextName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="255" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="organizationsContexts.contextName" />
    <property name="label" value="Category" />
  </bean>
  <bean id="Organization-parent.name" parent="Organization-parent.name-parentBean" />

  <bean id="Organization-parent.name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Parent" />
    <property name="maxLength" value="255" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="parent.name" />
    <property name="label" value="Parent" />
  </bean>
  <bean id="Organization-organizationsContexts.active" parent="Organization-organizationsContexts.active-parentBean" />

  <bean id="Organization-organizationsContexts.active-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Con Active" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.IndicatorYNOnlyValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="name" value="organizationsContexts.active" />
    <property name="label" value="Context Active" />
  </bean>
  <bean id="Organization-active" parent="Organization-active-parentBean" />

  <bean id="Organization-active-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Active" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.IndicatorYNOnlyValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="name" value="active" />
    <property name="label" value="Active" />
  </bean>
  <bean id="Organization-parentOrganizationId" parent="Organization-parentOrganizationId-parentBean" />

  <bean id="Organization-parentOrganizationId-parentBean" abstract="true" parent="Organization-id">
    <property name="shortLabel" value="Parent" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:businessObjectClass="org.kuali.rice.kom.bo.Organization"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
            p:includeKeyInLabel="false"
            p:labelAttribute="name" />
    </property>
    <property name="name" value="parentOrganizationId" />
    <property name="label" value="Parent Organization" />
  </bean>
  <bean id="Organization-categoryId" parent="Organization-categoryId-parentBean" />

  <bean id="Organization-categoryId-parentBean" abstract="true" parent="OrganizationCategory-id">
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:businessObjectClass="org.kuali.rice.kom.bo.OrganizationCategory"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
            p:includeKeyInLabel="false"
            p:keyAttribute="id"
            p:labelAttribute="name" />
    </property>
    <property name="name" value="categoryId" />
    <property name="label" value="Category" />
  </bean>
  <bean id="Organization-versionNumber" parent="Organization-versionNumber-parentBean" />

  <bean id="Organization-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
  <bean id="Organization-objectId" parent="Organization-objectId-parentBean" />

  <bean id="Organization-objectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="Organization-inquiryDefinition" parent="Organization-inquiryDefinition-parentBean" />

  <bean id="Organization-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="id" />
              <bean parent="FieldDefinition"
                    p:attributeName="shortName" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationCategory.name" />
              <bean parent="FieldDefinition"
                    p:attributeName="parentOrganizationId" />
            </list>
          </property>
          <property name="title" value="Header Text" />
        </bean>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="InquiryCollectionDefinition">
                <property name="attributeName" value="organizationsContexts" />
                <property name="businessObjectClass" value="org.kuali.rice.kom.bo.OrganizationsContexts" />
                <property name="numberOfColumns" value="1" />
                <property name="inquiryFields" >
                  <list>
                    <bean parent="FieldDefinition"
                          p:attributeName="id" />
                    <bean parent="FieldDefinition"
                          p:attributeName="active" />
                    <bean parent="FieldDefinition"
                          p:attributeName="organizationContext.name" />
                    <bean parent="FieldDefinition"
                          p:attributeName="organizationContext.description" />
                  </list>
                </property>
              </bean>
            </list>
          </property>
          <property name="title" value="Organizations Contexts" />
        </bean>
      </list>
    </property>
    <property name="title" value="Organization Inquiry" />
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="Organization-lookupDefinition" parent="Organization-lookupDefinition-parentBean" />

  <bean id="Organization-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="instructions" value="Lookup a Organization" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="id" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="shortName" />
        <bean parent="FieldDefinition"
              p:attributeName="categoryId" />
        <bean parent="FieldDefinition"
              p:attributeName="parentOrganizationId" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="id" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="shortName" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCategory.name" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationsContexts" />
        <bean parent="FieldDefinition"
              p:attributeName="parent.name" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="title" value="Organization Lookup" />
  </bean>
</beans>
