<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="Group" parent="Group-parentBean" />

  <bean id="Group-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Group" />
    <property name="inquiryDefinition" >
      <ref bean="Group-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="Group-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="name" />
    <property name="objectLabel" value="Group" />
    <property name="collections" >
      <list>
        <bean parent="CollectionDefinition"
              p:shortLabel="SubGroups"
              p:summary="Member Groups"
              p:name="memberGroups"
              p:label="Member Groups"
              p:description="Sub Groups of this group" />
        <bean parent="CollectionDefinition"
              p:shortLabel="ParentGroups"
              p:summary="Parent Groups"
              p:name="parentGroups"
              p:label="Parent Groups"
              p:description="Parents of this group" />
        <bean parent="CollectionDefinition"
              p:shortLabel="Roles"
              p:summary="Group Roles"
              p:name="groupRoles"
              p:label="Group Roles"
              p:description="Roles of this group" />
        <bean parent="CollectionDefinition"
              p:shortLabel="Attributes"
              p:summary="Group Attributes"
              p:name="groupAttributes"
              p:label="Group Attributes"
              p:description="Attributes of this group" />
      </list>
    </property>
    <property name="attributes" >
      <list>
        <ref bean="Group-id" />
        <ref bean="Group-name" />
        <ref bean="Group-description" />
        <ref bean="Group-groupTypeId" />
        <ref bean="Group-newCollectionRecord" />
        <ref bean="Group-versionNumber" />
        <ref bean="Group-objectId" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="Group-id" parent="Group-id-parentBean" />

  <bean id="Group-id-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="name" value="id" />
    <property name="label" value="Id" />
  </bean>
  <bean id="Group-name" parent="Group-name-parentBean" />

  <bean id="Group-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="500" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="name" />
    <property name="label" value="Name" />
  </bean>
  <bean id="Group-description" parent="Group-description-parentBean" />

  <bean id="Group-description-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="4000" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextareaControlDefinition"
            p:cols="50"
            p:rows="10" />
    </property>
    <property name="name" value="description" />
    <property name="label" value="Description" />
  </bean>
  <bean id="Group-groupTypeId" parent="Group-groupTypeId-parentBean" />

  <bean id="Group-groupTypeId-parentBean" abstract="true" parent="GroupType-id">
    <property name="shortLabel" value="Type" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:businessObjectClass="org.kuali.rice.kim.bo.GroupType"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
            p:includeKeyInLabel="false"
            p:keyAttribute="id"
            p:labelAttribute="name" />
    </property>
    <property name="name" value="groupTypeId" />
    <property name="label" value="Type" />
  </bean>
  <bean id="Group-newCollectionRecord" parent="Group-newCollectionRecord-parentBean" />

  <bean id="Group-newCollectionRecord-parentBean" abstract="true" parent="AttributeReferenceDummy-newCollectionRecord">
  </bean>
  <bean id="Group-versionNumber" parent="Group-versionNumber-parentBean" />

  <bean id="Group-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
  <bean id="Group-objectId" parent="Group-objectId-parentBean" />

  <bean id="Group-objectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="Group-inquiryDefinition" parent="Group-inquiryDefinition-parentBean" />

  <bean id="Group-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="id" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
              <bean parent="FieldDefinition"
                    p:attributeName="description" />
              <bean parent="FieldDefinition"
                    p:attributeName="groupType.name" />
            </list>
          </property>
          <property name="title" value="Group Info" />
        </bean>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="InquiryCollectionDefinition">
                <property name="attributeName" value="parentGroups" />
                <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Group" />
                <property name="numberOfColumns" value="1" />
                <property name="inquiryFields" >
                  <list>
                    <bean parent="FieldDefinition"
                          p:attributeName="id" />
                    <bean parent="FieldDefinition"
                          p:attributeName="name" />
                  </list>
                </property>
              </bean>
            </list>
          </property>
          <property name="title" value="Parent Group" />
        </bean>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="InquiryCollectionDefinition">
                <property name="attributeName" value="memberGroups" />
                <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Group" />
                <property name="numberOfColumns" value="1" />
                <property name="inquiryFields" >
                  <list>
                    <bean parent="FieldDefinition"
                          p:attributeName="id" />
                    <bean parent="FieldDefinition"
                          p:attributeName="name" />
                  </list>
                </property>
              </bean>
            </list>
          </property>
          <property name="title" value="Sub Groups" />
        </bean>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="InquiryCollectionDefinition">
                <property name="attributeName" value="groupRoles" />
                <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Role" />
                <property name="numberOfColumns" value="1" />
                <property name="inquiryFields" >
                  <list>
                    <bean parent="FieldDefinition"
                          p:attributeName="id" />
                    <bean parent="FieldDefinition"
                          p:attributeName="name" />
                  </list>
                </property>
              </bean>
            </list>
          </property>
          <property name="title" value="Group Roles" />
        </bean>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="InquiryCollectionDefinition">
                <property name="attributeName" value="groupAttributes" />
                <property name="businessObjectClass" value="org.kuali.rice.kim.bo.GroupAttribute" />
                <property name="numberOfColumns" value="1" />
                <property name="inquiryFields" >
                  <list>
                    <bean parent="FieldDefinition"
                          p:attributeName="id" />
                    <bean parent="FieldDefinition"
                          p:attributeName="attributeTypeId" />
                    <bean parent="FieldDefinition"
                          p:attributeName="attributeName" />
                    <bean parent="FieldDefinition"
                          p:attributeName="value" />
                  </list>
                </property>
              </bean>
            </list>
          </property>
          <property name="title" value="Group Attributes" />
        </bean>
      </list>
    </property>
    <property name="title" value="Group Inquiry" />
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="Group-lookupDefinition" parent="Group-lookupDefinition-parentBean" />

  <bean id="Group-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="instructions" value="Lookup a Group" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="id"
              p:forceLookup="true" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="groupTypeId" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="id" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
      </list>
    </property>
    <property name="title" value="Group Lookup" />
  </bean>
</beans>
