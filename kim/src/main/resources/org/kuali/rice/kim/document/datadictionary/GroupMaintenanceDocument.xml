<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="KIMGroupMaintenanceDocument" parent="KIMGroupMaintenanceDocument-parentBean" />

  <bean id="KIMGroupMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Group" />
    <property name="maintainableClass" value="org.kuali.rice.kim.document.maintenance.GroupMaintainable" />
    <property name="maintainableSections" >
      <list>
        <ref bean="KIMGroupMaintenanceDocument-EditGroup" />
        <ref bean="KIMGroupMaintenanceDocument-ParentGroups" />
        <ref bean="KIMGroupMaintenanceDocument-MemberGroups" />
        <ref bean="KIMGroupMaintenanceDocument-MemberPrincipals" />
        <ref bean="KIMGroupMaintenanceDocument-AssignedRoles" />
        <ref bean="KIMGroupMaintenanceDocument-GroupTypeAttributes" />
        <ref bean="KIMGroupMaintenanceDocument-GroupAttributes" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>id</value>
      </list>
    </property>
    <property name="shortLabel" value="Group" />
    <property name="summary" value="Group Maintenance Document" />
    <property name="documentTypeName" value="KIMGroupMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="KGMD" />
    <property name="webScriptFiles" >
      <list>
        <value>../kim/scripts/group.js</value>
      </list>
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
          <property name="action" value="initiate" />
        </bean>
      </list>
    </property>
    <property name="label" value="Group" />
    <property name="description" value="Maintenance document which allows you to create new Group,
			or modify existing Group." />
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="KIMGroupMaintenanceDocument-EditGroup" parent="KIMGroupMaintenanceDocument-EditGroup-parentBean" />

  <bean id="KIMGroupMaintenanceDocument-EditGroup-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="id"
              p:readOnly="true" 
              p:required="true"
              p:defaultValueFinderClass="org.kuali.rice.kim.lookup.valuefinder.NextGroupIdFinder" />
        <bean parent="MaintainableFieldDefinition"
              p:required="true"
              p:name="name" />
        <bean parent="MaintainableFieldDefinition"
              p:required="true"
              p:name="groupTypeId" 
              p:defaultValueFinderClass="org.kuali.rice.kim.lookup.valuefinder.GroupTypeDefaultValueFinder" />
        <bean parent="MaintainableFieldDefinition"
              p:required="true"
              p:name="description" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
    <property name="id" value="Edit Group" />
    <property name="title" value="Edit Group" />
  </bean>
  <bean id="KIMGroupMaintenanceDocument-ParentGroups" parent="KIMGroupMaintenanceDocument-ParentGroups-parentBean" />

  <bean id="KIMGroupMaintenanceDocument-ParentGroups-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="sourceClassName" value="org.kuali.rice.kim.bo.Group" />
          <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Group" />
          <property name="includeAddLine" value="false" />
          <property name="alwaysAllowCollectionDeletion" value="true" />
          <property name="summaryTitle" value="Parent Groups" />
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:template="id"
                    p:name="id"
                    p:readOnly="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:template="name"
                    p:name="name"
                    p:readOnly="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="newCollectionRecord" />
            </list>
          </property>
          <property name="name" value="parentGroups" />
        </bean>
      </list>
    </property>
    <property name="id" value="Parent Groups" />
    <property name="title" value="Parent Groups" />
  </bean>
  <bean id="KIMGroupMaintenanceDocument-MemberGroups" parent="KIMGroupMaintenanceDocument-MemberGroups-parentBean" />

  <bean id="KIMGroupMaintenanceDocument-MemberGroups-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="sourceClassName" value="org.kuali.rice.kim.bo.Group" />
          <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Group" />
          <property name="includeAddLine" value="false" />
          <property name="alwaysAllowCollectionDeletion" value="true" />
          <property name="summaryTitle" value="Member Groups" />
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:template="id"
                    p:name="id"
                    p:readOnly="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:template="name"
                    p:name="name"
                    p:readOnly="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="newCollectionRecord" />
            </list>
          </property>
          <property name="name" value="memberGroups" />
        </bean>
      </list>
    </property>
    <property name="id" value="Member Groups" />
    <property name="title" value="Member Groups" />
  </bean>
  <bean id="KIMGroupMaintenanceDocument-MemberPrincipals" parent="KIMGroupMaintenanceDocument-MemberPrincipals-parentBean" />

  <bean id="KIMGroupMaintenanceDocument-MemberPrincipals-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="sourceClassName" value="org.kuali.rice.kim.bo.Principal" />
          <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Principal" />
          <property name="includeAddLine" value="false" />
          <property name="alwaysAllowCollectionDeletion" value="true" />
          <property name="summaryTitle" value="Principal" />
          <property name="summaryFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="name" />
            </list>
          </property>
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:template="id"
                    p:name="id"
                    p:readOnly="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:template="name"
                    p:name="name"
                    p:readOnly="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="newCollectionRecord" />
            </list>
          </property>
          <property name="name" value="memberPrincipals" />
        </bean>
      </list>
    </property>
    <property name="id" value="Member Principals" />
    <property name="title" value="Member Principals" />
  </bean>
  <bean id="KIMGroupMaintenanceDocument-AssignedRoles" parent="KIMGroupMaintenanceDocument-AssignedRoles-parentBean" />

  <bean id="KIMGroupMaintenanceDocument-AssignedRoles-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="sourceClassName" value="org.kuali.rice.kim.bo.Role" />
          <property name="businessObjectClass" value="org.kuali.rice.kim.bo.RoleQualificationForGroup" />
          <property name="includeAddLine" value="false" />
          <property name="alwaysAllowCollectionDeletion" value="true" />
          <property name="summaryTitle" value="Assigned Role" />
          <property name="summaryFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="name" />
            </list>
          </property>
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:readOnlyAfterAdd="true"
                    p:template="id"
                    p:name="id" />
              <bean parent="MaintainableFieldDefinition"
                    p:template="name"
                    p:name="name"
                    p:readOnly="true" />
            </list>
          </property>
          <property name="maintainableCollections" >
            <list>
              <bean parent="MaintainableCollectionDefinition">
                <property name="businessObjectClass" value="org.kuali.rice.kim.bo.GroupQualifiedRoleAttribute" />
                <property name="alwaysAllowCollectionDeletion" value="true" />
                <property name="summaryTitle" value="Qualification" />
                <property name="summaryFields" >
                  <list>
                    <bean parent="MaintainableFieldDefinition"
                          p:name="attributeName" />
                  </list>
                </property>
                <property name="maintainableFields" >
                  <list>
                    <bean parent="MaintainableFieldDefinition"
			              p:name="id"
			              p:required="true"
			              p:readOnly="true"
			              p:defaultValueFinderClass="org.kuali.rice.kim.lookup.valuefinder.NextGroupQualifiedRoleAttributeIdFinder">
			        </bean>
                    <bean parent="MaintainableFieldDefinition"
                          p:required="true"
                          p:name="attributeName" />
                    <bean parent="MaintainableFieldDefinition"
                          p:required="true"
                          p:name="attributeValue" />
                    <bean parent="MaintainableFieldDefinition"
                          p:name="groupId"
                          p:readOnly="true" />
                    <bean parent="MaintainableFieldDefinition"
                          p:name="roleId"
                          p:readOnly="true" />
                    <bean parent="MaintainableFieldDefinition"
                          p:name="versionNumber"
                          p:readOnly="true" />
                    <bean parent="MaintainableFieldDefinition"
                          p:name="objectId"
                          p:readOnly="true" />
                  </list>
                </property>
                <property name="name" value="qualifiedRoleAttributes" />
              </bean>
            </list>
          </property>
          <property name="name" value="roleQualificationsForGroup" />
        </bean>
      </list>
    </property>
    <property name="id" value="Assigned Roles" />
    <property name="title" value="Assigned Roles" />
  </bean>
  <bean id="KIMGroupMaintenanceDocument-GroupTypeAttributes" parent="KIMGroupMaintenanceDocument-GroupTypeAttributes-parentBean" />

  <bean id="KIMGroupMaintenanceDocument-GroupTypeAttributes-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="businessObjectClass" value="org.kuali.rice.kim.bo.GroupAttribute" />
          <property name="includeAddLine" value="false" />
          <property name="alwaysAllowCollectionDeletion" value="false" />
          <property name="summaryTitle" value="Group Type Attributes" />
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
		            p:name="id"
		            p:readOnly="true" 
		            p:required="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="attributeName" 
                    p:readOnly="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="attributeTypeId" 
                    p:readOnly="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="value" />
            </list>
          </property>
          <property name="name" value="groupTypeAttributes" />
        </bean>
      </list>
    </property>
    <property name="id" value="Group Type Attributes" />
    <property name="title" value="Group Type Attributes" />
  </bean>
  <bean id="KIMGroupMaintenanceDocument-GroupAttributes" parent="KIMGroupMaintenanceDocument-GroupAttributes-parentBean" />

  <bean id="KIMGroupMaintenanceDocument-GroupAttributes-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="businessObjectClass" value="org.kuali.rice.kim.bo.GroupAttribute" />
          <property name="alwaysAllowCollectionDeletion" value="true" />
          <property name="summaryTitle" value="Group Attributes" />
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
		            p:name="id"
		            p:readOnly="true" 
		            p:required="true"
		            p:defaultValueFinderClass="org.kuali.rice.kim.lookup.valuefinder.NextGroupAttributeIdFinder" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="attributeName" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="attributeTypeId" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="value" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="newCollectionRecord" />
            </list>
          </property>
          <property name="name" value="nonGroupTypeAttributes" />
        </bean>
      </list>
    </property>
    <property name="id" value="Group Attributes" />
    <property name="title" value="Group Attributes" />
  </bean>
</beans>
