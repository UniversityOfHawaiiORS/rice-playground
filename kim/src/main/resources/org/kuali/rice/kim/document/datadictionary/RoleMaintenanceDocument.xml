<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="KIMRoleMaintenanceDocument" parent="KIMRoleMaintenanceDocument-parentBean" />

  <bean id="KIMRoleMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Role" />
    <property name="maintainableClass" value="org.kuali.rice.kim.document.maintenance.RoleMaintainable" />
    <property name="maintainableSections" >
      <list>
        <ref bean="KIMRoleMaintenanceDocument-RoleInformation" />
        <ref bean="KIMRoleMaintenanceDocument-PermissionsforRole" />
        <ref bean="KIMRoleMaintenanceDocument-AssignedPrincipals" />
        <ref bean="KIMRoleMaintenanceDocument-AssignedGroups" />
        <ref bean="KIMRoleMaintenanceDocument-RoleAttributes" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>id</value>
      </list>
    </property>
    <property name="shortLabel" value="Role" />
    <property name="summary" value="Role Maintenance Document" />
    <property name="documentTypeName" value="KIMRoleMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="KRMD" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
          <property name="action" value="initiate" />
        </bean>
      </list>
    </property>
    <property name="label" value="Role" />
    <property name="description" value="Maintenance document which allows you to create new Role, or modify existing Roles." />
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="KIMRoleMaintenanceDocument-RoleInformation" parent="KIMRoleMaintenanceDocument-RoleInformation-parentBean" />

  <bean id="KIMRoleMaintenanceDocument-RoleInformation-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="id"
              p:required="true"
              p:readOnly="true"
              p:defaultValueFinderClass="org.kuali.rice.kim.lookup.valuefinder.NextRoleIdFinder">
        </bean>
        <bean parent="MaintainableFieldDefinition"
              p:required="true"
              p:name="name" />
        <bean parent="MaintainableFieldDefinition"
              p:required="true"
              p:name="description" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
    <property name="id" value="Role Information" />
    <property name="title" value="Role Information" />
  </bean>
  <bean id="KIMRoleMaintenanceDocument-PermissionsforRole" parent="KIMRoleMaintenanceDocument-PermissionsforRole-parentBean" />

  <bean id="KIMRoleMaintenanceDocument-PermissionsforRole-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="sourceClassName" value="org.kuali.rice.kim.bo.Permission" />
          <property name="sourceAttributeName" value="permissions" />
          <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Permission" />
          <property name="includeAddLine" value="false" />
          <property name="summaryTitle" value="Permissions for Role" />
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:readOnlyAfterAdd="true"
                    p:template="id"
                    p:name="id" />
              <bean parent="MaintainableFieldDefinition"
                    p:template="name"
                    p:name="name"
                    p:readOnly="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:template="namespaceId"
                    p:name="namespaceId"
                    p:readOnly="true" />
            </list>
          </property>
          <property name="name" value="permissions" />
        </bean>
      </list>
    </property>
    <property name="id" value="Permissions for Role" />
    <property name="title" value="Permissions for Role" />
  </bean>
  <bean id="KIMRoleMaintenanceDocument-AssignedPrincipals" parent="KIMRoleMaintenanceDocument-AssignedPrincipals-parentBean" />

  <bean id="KIMRoleMaintenanceDocument-AssignedPrincipals-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="sourceClassName" value="org.kuali.rice.kim.bo.Principal" />
          <property name="sourceAttributeName" value="principalQualifiedRoles" />
          <property name="businessObjectClass" value="org.kuali.rice.kim.bo.PrincipalQualifiedRole" />
          <property name="includeAddLine" value="false" />
          <property name="summaryTitle" value="Assigned Principal" />
          <property name="summaryFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="name" />
            </list>
          </property>
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:readOnlyAfterAdd="true"
                    p:template="id"
                    p:name="id" />
              <bean parent="MaintainableFieldDefinition"
                    p:template="name"
                    p:name="name"
                    p:readOnly="true" />
            </list>
          </property>
          <property name="maintainableCollections" >
            <list>
              <bean parent="MaintainableCollectionDefinition">
                <property name="businessObjectClass" value="org.kuali.rice.kim.bo.PrincipalQualifiedRoleAttribute" />
                <property name="summaryTitle" value="Qualification" />
                <property name="summaryFields" >
                  <list>
                    <bean parent="MaintainableFieldDefinition"
                          p:name="attributeName" />
                  </list>
                </property>
                <property name="maintainableFields" >
                  <list>
                    <bean parent="MaintainableFieldDefinition"
			              p:name="id"
			              p:required="true"
			              p:readOnly="true"
			              p:defaultValueFinderClass="org.kuali.rice.kim.lookup.valuefinder.NextPrincipalQualifiedRoleAttributeIdFinder">
			        </bean>
                    <bean parent="MaintainableFieldDefinition"
                          p:required="true"
                          p:name="attributeName" />
                    <bean parent="MaintainableFieldDefinition"
                          p:required="true"
                          p:name="attributeValue" />
                    <bean parent="MaintainableFieldDefinition"
                          p:name="principalId"
                          p:readOnly="true" />
                    <bean parent="MaintainableFieldDefinition"
                          p:name="roleId"
                          p:readOnly="true" />
                    <bean parent="MaintainableFieldDefinition"
                          p:name="versionNumber"
                          p:readOnly="true" />
                    <bean parent="MaintainableFieldDefinition"
                          p:name="objectId"
                          p:readOnly="true" />
                  </list>
                </property>
                <property name="name" value="qualifiedRoleAttributes" />
              </bean>
            </list>
          </property>
          <property name="name" value="principalQualifiedRoles" />
        </bean>
      </list>
    </property>
    <property name="id" value="Assigned Principals" />
    <property name="title" value="Assigned Principals" />
  </bean>
  <bean id="KIMRoleMaintenanceDocument-AssignedGroups" parent="KIMRoleMaintenanceDocument-AssignedGroups-parentBean" />

  <bean id="KIMRoleMaintenanceDocument-AssignedGroups-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="sourceClassName" value="org.kuali.rice.kim.bo.Group" />
          <property name="sourceAttributeName" value="groupQualifiedRoles" />
          <property name="businessObjectClass" value="org.kuali.rice.kim.bo.GroupQualifiedRole" />
          <property name="includeAddLine" value="false" />
          <property name="summaryTitle" value="Assigned Group" />
          <property name="summaryFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="name" />
            </list>
          </property>
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:readOnlyAfterAdd="true"
                    p:template="id"
                    p:name="id" />
              <bean parent="MaintainableFieldDefinition"
                    p:template="name"
                    p:name="name"
                    p:readOnly="true" />
            </list>
          </property>
          <property name="maintainableCollections" >
            <list>
              <bean parent="MaintainableCollectionDefinition">
                <property name="businessObjectClass" value="org.kuali.rice.kim.bo.GroupQualifiedRoleAttribute" />
                <property name="summaryTitle" value="Qualification" />
                <property name="summaryFields" >
                  <list>
                    <bean parent="MaintainableFieldDefinition"
                          p:name="attributeName" />
                  </list>
                </property>
                <property name="maintainableFields" >
                  <list>
                    <bean parent="MaintainableFieldDefinition"
			              p:name="id"
			              p:required="true"
			              p:readOnly="true"
			              p:defaultValueFinderClass="org.kuali.rice.kim.lookup.valuefinder.NextGroupQualifiedRoleAttributeIdFinder">
			        </bean>
                    <bean parent="MaintainableFieldDefinition"
                          p:required="true"
                          p:name="attributeName" />
                    <bean parent="MaintainableFieldDefinition"
                          p:required="true"
                          p:name="attributeValue" />
                    <bean parent="MaintainableFieldDefinition"
                          p:name="groupId"
                          p:readOnly="true" />
                    <bean parent="MaintainableFieldDefinition"
                          p:name="roleId"
                          p:readOnly="true" />
                    <bean parent="MaintainableFieldDefinition"
                          p:name="versionNumber"
                          p:readOnly="true" />
                    <bean parent="MaintainableFieldDefinition"
                          p:name="objectId"
                          p:readOnly="true" />
                  </list>
                </property>
                <property name="name" value="qualifiedRoleAttributes" />
              </bean>
            </list>
          </property>
          <property name="name" value="groupQualifiedRoles" />
        </bean>
      </list>
    </property>
    <property name="id" value="Assigned Groups" />
    <property name="title" value="Assigned Groups" />
  </bean>
  <bean id="KIMRoleMaintenanceDocument-RoleAttributes" parent="KIMRoleMaintenanceDocument-RoleAttributes-parentBean" />

  <bean id="KIMRoleMaintenanceDocument-RoleAttributes-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="sourceClassName" value="org.kuali.rice.kim.bo.RoleAttribute" />
          <property name="sourceAttributeName" value="roleAttributes" />
          <property name="businessObjectClass" value="org.kuali.rice.kim.bo.RoleAttribute" />
          <property name="summaryTitle" value="Role Attributes" />
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
	                p:name="id"
	                p:required="true"
	                p:readOnly="true"
	                p:defaultValueFinderClass="org.kuali.rice.kim.lookup.valuefinder.NextRoleAttributeIdFinder">
	          </bean>
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="attributeTypeId" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="attributeName" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="value" />
            </list>
          </property>
          <property name="name" value="roleAttributes" />
        </bean>
      </list>
    </property>
    <property name="id" value="Role Attributes" />
    <property name="title" value="Role Attributes" />
  </bean>
</beans>
