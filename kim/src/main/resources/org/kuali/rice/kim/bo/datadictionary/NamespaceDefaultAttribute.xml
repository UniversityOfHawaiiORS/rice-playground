<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "https://test.kuali.org/dtds/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.rice.kim.bo.NamespaceDefaultAttribute">

        <inquiry>
            <title>Namespace Default Attribute Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="id" />
                <field attributeName="namespaceId" />
                <field attributeName="attributeTypeId" />
                <field attributeName="attributeName" />
                <field attributeName="description" />
                <field attributeName="required" />
                <field attributeName="active" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Namespace Default Attribute Lookup</title>

            <instructions>Lookup a Namespace Default Attribute</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="description" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="id" required="false" />
                <lookupField attributeName="namespaceId" required="false" />
                <lookupField attributeName="attributeTypeId" required="false" />
                <lookupField attributeName="attributeName" required="false" />
                <lookupField attributeName="description" required="false" />
                <lookupField attributeName="required" required="false" />
                <lookupField attributeName="active" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="id" />
                <field attributeName="namespaceId" />
               <field attributeName="attributeTypeId" />
               <field attributeName="attributeName" />
               <field attributeName="description" />
               <field attributeName="required" />
               <field attributeName="active" />
            </resultFields>
        </lookup>

        <titleAttribute>id</titleAttribute>

        <attributes>

            <attribute name="id" forceUppercase="true">
                <label>ID</label>
                <shortLabel>id</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="8" />
                </control>

			</attribute>

           <attribute name="attributeName" forceUppercase="false">
                <label>Attribute Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>500</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <text size="100" />
                </control>

            </attribute>
           <attribute name="description" forceUppercase="false">
                <label>Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>4000</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <text size="100" />
                </control>

            </attribute>
           <attribute name="required" forceUppercase="true">
                <label>Required</label>

                <maxLength>3</maxLength>
               <control>
                <select valuesFinderClass="org.kuali.core.lookup.keyvalues.IndicatorYNOnlyValuesFinder"/>
               </control>

             </attribute>
           <attribute name="active" forceUppercase="true">
                <label>Active</label>
                <shortLabel>Active</shortLabel>

                <maxLength>3</maxLength>
               <control>
                <select valuesFinderClass="org.kuali.core.lookup.keyvalues.IndicatorYNOnlyValuesFinder"/>
               </control>

            </attribute>

           <attributeReference name="namespaceId"
            sourceClassName="org.kuali.rice.kim.bo.Namespace"
            sourceAttributeName="id">
            <label>Namespace ID</label>
            <required>true</required>
            <control>
                <select
                    valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
                    businessObjectClass="org.kuali.rice.kim.bo.Namespace"
                    keyAttribute="id" labelAttribute="name"
                    includeKeyInLabel="true" />
            </control>
           </attributeReference>

           <attributeReference name="attributeTypeId"
           	sourceClassName="org.kuali.rice.kim.bo.AttributeType"
           	sourceAttributeName="id">
           	<label>Attribute Type ID</label>
           	<required>true</required>
           	<control>
           		<select
           			valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
           			businessObjectClass="org.kuali.rice.kim.bo.AttributeType"
           			keyAttribute="id" labelAttribute="attributeTypeName"
           			includeKeyInLabel="false" />
           	</control>
           </attributeReference>

           <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
           <attributeReference name="objectId" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="objectId" />
        </attributes>

        <objectLabel>Namespace Default Attribute</objectLabel>
    </businessObject>
</dictionaryEntry>
