<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="KIMNamespaceMaintenanceDocument" parent="KIMNamespaceMaintenanceDocument-parentBean" />

  <bean id="KIMNamespaceMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Namespace" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="KIMNamespaceMaintenanceDocument-NamespaceInformation" />
        <ref bean="KIMNamespaceMaintenanceDocument-DefaultNamespaceAttributes" />
        <ref bean="KIMNamespaceMaintenanceDocument-NamespacePermissions" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>id</value>
      </list>
    </property>
    <property name="shortLabel" value="Namespace Maintenence" />
    <property name="summary" value="Namespace Document" />
    <property name="documentTypeName" value="KIMNamespaceMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="KNSD" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
          <property name="action" value="initiate" />
        </bean>
      </list>
    </property>
    <property name="label" value="Namespace Document" />
    <property name="description" value="This document is used to create or update Namespaces." />
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="KIMNamespaceMaintenanceDocument-NamespaceInformation" parent="KIMNamespaceMaintenanceDocument-NamespaceInformation-parentBean" />

  <bean id="KIMNamespaceMaintenanceDocument-NamespaceInformation-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="id"
              p:required="true"
              p:readOnly="true"
              p:defaultValueFinderClass="org.kuali.rice.kim.lookup.valuefinder.NextNamespaceIdFinder">
        </bean>
        <bean parent="MaintainableFieldDefinition"
              p:required="true"
              p:name="name" />
        <bean parent="MaintainableFieldDefinition"
              p:required="true"
              p:name="description" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
    <property name="id" value="Namespace Information" />
    <property name="title" value="Namespace Information" />
  </bean>
  <bean id="KIMNamespaceMaintenanceDocument-DefaultNamespaceAttributes" parent="KIMNamespaceMaintenanceDocument-DefaultNamespaceAttributes-parentBean" />

  <bean id="KIMNamespaceMaintenanceDocument-DefaultNamespaceAttributes-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="businessObjectClass" value="org.kuali.rice.kim.bo.NamespaceDefaultAttribute" />
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="id"
                    p:readOnly="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="attributeName" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="attributeTypeId" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="description" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="required" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="active" />
            </list>
          </property>
          <property name="name" value="namespaceAttributes" />
        </bean>
      </list>
    </property>
    <property name="id" value="Default Namespace Attributes" />
    <property name="title" value="Default Namespace Attributes" />
  </bean>
  <bean id="KIMNamespaceMaintenanceDocument-NamespacePermissions" parent="KIMNamespaceMaintenanceDocument-NamespacePermissions-parentBean" />

  <bean id="KIMNamespaceMaintenanceDocument-NamespacePermissions-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Permission" />
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="id"
                    p:readOnly="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="name" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="description" />
            </list>
          </property>
          <property name="name" value="namespacePermissions" />
        </bean>
      </list>
    </property>
    <property name="id" value="Namespace Permissions" />
    <property name="title" value="Namespace Permissions" />
  </bean>
</beans>
