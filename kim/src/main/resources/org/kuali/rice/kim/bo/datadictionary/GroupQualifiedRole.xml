<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="GroupQualifiedRole" parent="GroupQualifiedRole-parentBean" />

  <bean id="GroupQualifiedRole-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.web.form.GroupQualifiedRole" />
    <property name="titleAttribute" value="id" />
    <property name="objectLabel" value="Group Qualified Role" />
    <property name="collections" >
      <list>
        <bean parent="CollectionDefinition"
              p:shortLabel="Qualifications"
              p:summary="Qualification Attributes"
              p:elementLabel="Qualification"
              p:name="qualifiedRoleAttributes"
              p:label="Qualifications"
              p:description="Qualification Attributes" />
      </list>
    </property>
    <property name="attributes" >
      <list>
        <ref bean="GroupQualifiedRole-id" />
        <ref bean="GroupQualifiedRole-name" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="GroupQualifiedRole-id" parent="GroupQualifiedRole-id-parentBean" />

  <bean id="GroupQualifiedRole-id-parentBean" abstract="true" parent="Group-id">
    <property name="shortLabel" value="Group" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:businessObjectClass="org.kuali.rice.kim.bo.Group"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
            p:includeKeyInLabel="true"
            p:keyAttribute="id"
            p:labelAttribute="name" />
    </property>
    <property name="label" value="Group" />
  </bean>
  <bean id="GroupQualifiedRole-name" parent="GroupQualifiedRole-name-parentBean" />

  <bean id="GroupQualifiedRole-name-parentBean" abstract="true" parent="Group-name">
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
</beans>
