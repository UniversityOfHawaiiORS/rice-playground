<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="Principal" parent="Principal-parentBean" />

  <bean id="Principal-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Principal" />
    <property name="inquiryDefinition" >
      <ref bean="Principal-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="Principal-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="name" />
    <property name="objectLabel" value="Principal" />
    <property name="collections" >
      <list>
        <bean parent="CollectionDefinition"
              p:shortLabel="Groups"
              p:summary="Groups"
              p:name="groups"
              p:label="Groups"
              p:description="Groups of this Principal" />
        <bean parent="CollectionDefinition"
              p:shortLabel="Roles"
              p:summary="Roles"
              p:name="roles"
              p:label="Roles"
              p:description="Roles of this Principal" />
        <bean parent="CollectionDefinition"
              p:shortLabel="Principal Qualified Roles"
              p:summary="These are roles and their qualifications for this principal."
              p:name="roleQualificationsForPrincipal"
              p:label="Qualified Roles for Principal"
              p:description="Role and their Qualifications for this Principal." />
        <bean parent="CollectionDefinition"
              p:shortLabel="Principal Qualified Role Attributes"
              p:summary="These are principals that have this role along with their qualification attributes."
              p:name="principalQualifiedRoleAttributes"
              p:label="Principals with Qualified Role Attributes"
              p:description="Principal Qualified Role Attributes for this Role." />
      </list>
    </property>
    <property name="attributes" >
      <list>
        <ref bean="Principal-id" />
        <ref bean="Principal-name" />
        <ref bean="Principal-entityId" />
        <ref bean="Principal-versionNumber" />
        <ref bean="Principal-objectId" />
        <ref bean="Principal-newCollectionRecord" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="Principal-id" parent="Principal-id-parentBean" />

  <bean id="Principal-id-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Principal Id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="name" value="id" />
    <property name="label" value="Id" />
  </bean>
  <bean id="Principal-name" parent="Principal-name-parentBean" />

  <bean id="Principal-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Principal Name" />
    <property name="maxLength" value="500" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="name" />
    <property name="label" value="Name" />
  </bean>
  <bean id="Principal-entityId" parent="Principal-entityId-parentBean" />

  <bean id="Principal-entityId-parentBean" abstract="true" parent="Entity-id">
    <property name="shortLabel" value="Entity" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:businessObjectClass="org.kuali.rice.kim.bo.Entity"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
            p:includeKeyInLabel="true"
            p:keyAttribute="id"
            p:labelAttribute="EntityType.name" />
    </property>
    <property name="name" value="entityId" />
    <property name="label" value="Entity" />
  </bean>
  <bean id="Principal-newCollectionRecord" parent="Principal-newCollectionRecord-parentBean" />

  <bean id="Principal-newCollectionRecord-parentBean" abstract="true" parent="AttributeReferenceDummy-newCollectionRecord">
  </bean>
  <bean id="Principal-versionNumber" parent="Principal-versionNumber-parentBean" />

  <bean id="Principal-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
  <bean id="Principal-objectId" parent="Principal-objectId-parentBean" />

  <bean id="Principal-objectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="Principal-inquiryDefinition" parent="Principal-inquiryDefinition-parentBean" />

  <bean id="Principal-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="id" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
            </list>
          </property>
          <property name="title" value="Header Text" />
        </bean>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="InquiryCollectionDefinition">
                <property name="attributeName" value="groups" />
                <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Group" />
                <property name="numberOfColumns" value="1" />
                <property name="inquiryFields" >
                  <list>
                    <bean parent="FieldDefinition"
                          p:attributeName="id" />
                    <bean parent="FieldDefinition"
                          p:attributeName="name" />
                  </list>
                </property>
              </bean>
            </list>
          </property>
          <property name="title" value="Group Membership" />
        </bean>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="InquiryCollectionDefinition">
                <property name="attributeName" value="roles" />
                <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Role" />
                <property name="numberOfColumns" value="1" />
                <property name="inquiryFields" >
                  <list>
                    <bean parent="FieldDefinition"
                          p:attributeName="id" />
                    <bean parent="FieldDefinition"
                          p:attributeName="name" />
                  </list>
                </property>
              </bean>
            </list>
          </property>
          <property name="title" value="Roles" />
        </bean>
      </list>
    </property>
    <property name="title" value="Principal Inquiry" />
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="Principal-lookupDefinition" parent="Principal-lookupDefinition-parentBean" />

  <bean id="Principal-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="instructions" value="Lookup a Principal" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="id" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="id" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
      </list>
    </property>
    <property name="title" value="Principal Lookup" />
  </bean>
</beans>
