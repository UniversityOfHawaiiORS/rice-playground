<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "https://test.kuali.org/dtds/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.rice.kim.bo.Namespace">

        <inquiry>
        	<title>Namespace Inquiry</title>

        	<inquirySections>
        		<inquirySection title="Header Text">
        			<inquiryFields>
        				<field attributeName="id" />
        				<field attributeName="name" />
        				<field attributeName="description" />
        			</inquiryFields>
        		</inquirySection>
        		<inquirySection title="Default Namespace Attributes">
        			<inquiryFields>
        				<inquiryCollection name="namespaceAttributes"
        					businessObjectClass="org.kuali.rice.kim.bo.NamespaceDefaultAttribute">
        					<field attributeName="id" />
        					<field attributeName="attributeTypeId" />
        					<field attributeName="attributeName" />
                            <field attributeName="description" />
                            <field attributeName="required" />
        					<field attributeName="active" />
        				</inquiryCollection>
        			</inquiryFields>
        		</inquirySection>
            <inquirySection title="Namespace Permissions">
                    <inquiryFields>
                        <inquiryCollection name="namespacePermissions"
                            businessObjectClass="org.kuali.rice.kim.bo.Permission">
                            <field attributeName="id" />
                            <field attributeName="name" />
                            <field attributeName="description" />
                         </inquiryCollection>
                    </inquiryFields>
                </inquirySection>
        	</inquirySections>
        </inquiry>

        <lookup>
            <title>Namespace Lookup</title>

            <instructions>Lookup a Namespace</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="name" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="id" required="false" />
                <lookupField attributeName="name" required="false" />
                <lookupField attributeName="description" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="id" />
                <field attributeName="name" />
                <field attributeName="description" />
            </resultFields>
        </lookup>

        <titleAttribute>id</titleAttribute>

        <attributes>

            <attribute name="id" forceUppercase="true">
                <label>ID</label>
                <shortLabel>id</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                 <hidden/>
                </control>

            </attribute>

            <attribute name="name" forceUppercase="true">
                <label>Namespace Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>500</maxLength>
                <validationPattern><anyCharacter allowWhitespace="false" /></validationPattern>

                <control>
                    <text size="100" />
                </control>

            </attribute>

            <attribute name="description" forceUppercase="false">
                <label>Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>4000</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <text size="100" />
                </control>

            </attribute>
             <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
            <attributeReference name="objectId" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="objectId" />

        </attributes>

        <collections>
          <collection name="namespaceAttributes">
            <label>Namespace Default Attributes</label>
            <shortLabel>Attributes</shortLabel>
            <summary>Namespace Default Attributes Attributes</summary>
            <description>Attributes of this namespace</description>
          </collection>
          <collection name="namespacePermissions">
            <label>Namespace Permissions</label>
            <shortLabel>Permissions</shortLabel>
            <summary>Namespace Permissions</summary>
            <description>Permissions of this namespace</description>
          </collection>
        </collections>

        <objectLabel>Namespace</objectLabel>
    </businessObject>
</dictionaryEntry>
