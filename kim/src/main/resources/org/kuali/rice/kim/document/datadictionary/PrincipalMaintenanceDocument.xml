<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="KIMPrincipalMaintenanceDocument" parent="KIMPrincipalMaintenanceDocument-parentBean" />

  <bean id="KIMPrincipalMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Principal" />
    <property name="maintainableClass" value="org.kuali.rice.kim.document.maintenance.PrincipalMaintainable" />
    <property name="maintainableSections" >
      <list>
        <ref bean="KIMPrincipalMaintenanceDocument-PrincipalInformation" />
        <ref bean="KIMPrincipalMaintenanceDocument-GroupMembership" />
        <ref bean="KIMPrincipalMaintenanceDocument-AssignedRoles" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>id</value>
      </list>
    </property>
    <property name="shortLabel" value="Principal" />
    <property name="summary" value="Principal Maintenance Document" />
    <property name="documentTypeName" value="KIMPrincipalMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="KPMD" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="authorizedGroups" >
            <set>
              <value>Kuali Identity Managers</value>
            </set>
          </property>
          <property name="action" value="initiate" />
        </bean>
      </list>
    </property>
    <property name="label" value="Principal" />
    <property name="description" value="Maintenance document which allows you to create new Principal, or modify existing Principal." />
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="KIMPrincipalMaintenanceDocument-PrincipalInformation" parent="KIMPrincipalMaintenanceDocument-PrincipalInformation-parentBean" />

  <bean id="KIMPrincipalMaintenanceDocument-PrincipalInformation-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="id"
              p:required="true"
              p:readOnly="true"
              p:defaultValueFinderClass="org.kuali.rice.kim.lookup.valuefinder.NextPrincipalIdFinder">
        </bean>
        <bean parent="MaintainableFieldDefinition"
              p:required="true"
              p:name="name" />
        <bean parent="MaintainableFieldDefinition"
              p:required="true"
              p:name="entityId" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
    <property name="id" value="Principal Information" />
    <property name="title" value="Principal Information" />
  </bean>
  <bean id="KIMPrincipalMaintenanceDocument-GroupMembership" parent="KIMPrincipalMaintenanceDocument-GroupMembership-parentBean" />

  <bean id="KIMPrincipalMaintenanceDocument-GroupMembership-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="sourceClassName" value="org.kuali.rice.kim.bo.Group" />
          <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Group" />
          <property name="includeAddLine" value="false" />
          <property name="alwaysAllowCollectionDeletion" value="true" />
          <property name="summaryTitle" value="Group" />
          <property name="summaryFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="name" />
            </list>
          </property>
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:readOnly="true"
                    p:template="id"
                    p:name="id" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:template="name"
                    p:name="name"
                    p:readOnly="true" />
            </list>
          </property>
          <property name="name" value="groups" />
        </bean>
      </list>
    </property>
    <property name="id" value="Group Membership" />
    <property name="title" value="Group Membership" />
  </bean>
  <bean id="KIMPrincipalMaintenanceDocument-AssignedRoles" parent="KIMPrincipalMaintenanceDocument-AssignedRoles-parentBean" />

  <bean id="KIMPrincipalMaintenanceDocument-AssignedRoles-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="sourceClassName" value="org.kuali.rice.kim.bo.Role" />
          <property name="businessObjectClass" value="org.kuali.rice.kim.web.form.RoleQualificationForPrincipal" />
          <property name="includeAddLine" value="false" />
          <property name="alwaysAllowCollectionDeletion" value="true" />
          <property name="summaryTitle" value="Assigned Role" />
          <property name="summaryFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="name" />
            </list>
          </property>
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:readOnlyAfterAdd="true"
                    p:template="id"
                    p:name="id" />
              <bean parent="MaintainableFieldDefinition"
                    p:template="name"
                    p:name="name"
                    p:readOnly="true" />
            </list>
          </property>
          <property name="maintainableCollections" >
            <list>
              <bean parent="MaintainableCollectionDefinition">
                <property name="businessObjectClass" value="org.kuali.rice.kim.bo.PrincipalQualifiedRoleAttribute" />
                <property name="alwaysAllowCollectionDeletion" value="true" />
                <property name="summaryTitle" value="Qualification" />
                <property name="summaryFields" >
                  <list>
                    <bean parent="MaintainableFieldDefinition"
                          p:name="attributeName" />
                  </list>
                </property>
                <property name="maintainableFields" >
                  <list>
                    <bean parent="MaintainableFieldDefinition"
			              p:name="id"
			              p:required="true"
			              p:readOnly="true"
			              p:defaultValueFinderClass="org.kuali.rice.kim.lookup.valuefinder.NextPrincipalQualifiedRoleAttributeIdFinder">
			        </bean>
                    <bean parent="MaintainableFieldDefinition"
                          p:required="true"
                          p:name="attributeName" />
                    <bean parent="MaintainableFieldDefinition"
                          p:required="true"
                          p:name="attributeValue" />
                    <bean parent="MaintainableFieldDefinition"
                          p:name="principalId"
                          p:readOnly="true" />
                    <bean parent="MaintainableFieldDefinition"
                          p:name="roleId"
                          p:readOnly="true" />
                    <bean parent="MaintainableFieldDefinition"
                          p:name="versionNumber"
                          p:readOnly="true" />
                    <bean parent="MaintainableFieldDefinition"
                          p:name="objectId"
                          p:readOnly="true" />
                  </list>
                </property>
                <property name="name" value="qualifiedRoleAttributes" />
              </bean>
            </list>
          </property>
          <property name="name" value="roleQualificationsForPrincipal" />
        </bean>
      </list>
    </property>
    <property name="id" value="Assigned Roles" />
    <property name="title" value="Assigned Roles" />
  </bean>
</beans>
