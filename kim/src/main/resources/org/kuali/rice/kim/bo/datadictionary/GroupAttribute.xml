<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="GroupAttribute" parent="GroupAttribute-parentBean" />

  <bean id="GroupAttribute-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.GroupAttribute" />
    <property name="inquiryDefinition" >
      <ref bean="GroupAttribute-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="GroupAttribute-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="attributeName" />
    <property name="objectLabel" value="Group Attribute" />
    <property name="attributes" >
      <list>
        <ref bean="GroupAttribute-id" />
        <ref bean="GroupAttribute-attributeName" />
        <ref bean="GroupAttribute-value" />
        <ref bean="GroupAttribute-attributeTypeId" />
        <ref bean="GroupAttribute-groupId" />
        <ref bean="GroupAttribute-newCollectionRecord" />
        <ref bean="GroupAttribute-versionNumber" />
        <ref bean="GroupAttribute-objectId" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="GroupAttribute-id" parent="GroupAttribute-id-parentBean" />

  <bean id="GroupAttribute-id-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="name" value="id" />
    <property name="label" value="Id" />
  </bean>
  <bean id="GroupAttribute-attributeName" parent="GroupAttribute-attributeName-parentBean" />

  <bean id="GroupAttribute-attributeName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="500" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="attributeName" />
    <property name="label" value="Name" />
  </bean>
  <bean id="GroupAttribute-value" parent="GroupAttribute-value-parentBean" />

  <bean id="GroupAttribute-value-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Value" />
    <property name="maxLength" value="4000" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextareaControlDefinition"
            p:cols="40"
            p:rows="10" />
    </property>
    <property name="name" value="value" />
    <property name="label" value="Group Attribute Value" />
  </bean>
  <bean id="GroupAttribute-attributeTypeId" parent="GroupAttribute-attributeTypeId-parentBean" />

  <bean id="GroupAttribute-attributeTypeId-parentBean" abstract="true" parent="AttributeType-id">
    <property name="shortLabel" value="Type" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:businessObjectClass="org.kuali.rice.kim.bo.AttributeType"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
            p:includeKeyInLabel="true"
            p:keyAttribute="id"
            p:labelAttribute="attributeTypeName" />
    </property>
    <property name="name" value="attributeTypeId" />
    <property name="label" value="Type" />
  </bean>
  <bean id="GroupAttribute-groupId" parent="GroupAttribute-groupId-parentBean" />

  <bean id="GroupAttribute-groupId-parentBean" abstract="true" parent="Group-id">
    <property name="shortLabel" value="Group" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:businessObjectClass="org.kuali.rice.kim.bo.Group"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
            p:includeKeyInLabel="true"
            p:keyAttribute="id"
            p:labelAttribute="name" />
    </property>
    <property name="name" value="groupId" />
    <property name="label" value="Group" />
  </bean>
  <bean id="GroupAttribute-newCollectionRecord" parent="GroupAttribute-newCollectionRecord-parentBean" />

  <bean id="GroupAttribute-newCollectionRecord-parentBean" abstract="true" parent="AttributeReferenceDummy-newCollectionRecord">
  </bean>
  <bean id="GroupAttribute-versionNumber" parent="GroupAttribute-versionNumber-parentBean" />

  <bean id="GroupAttribute-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
  <bean id="GroupAttribute-objectId" parent="GroupAttribute-objectId-parentBean" />

  <bean id="GroupAttribute-objectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="GroupAttribute-inquiryDefinition" parent="GroupAttribute-inquiryDefinition-parentBean" />

  <bean id="GroupAttribute-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="id" />
              <bean parent="FieldDefinition"
                    p:attributeName="attributeName" />
              <bean parent="FieldDefinition"
                    p:attributeName="group.name" />
              <bean parent="FieldDefinition"
                    p:attributeName="attributeType.attributeTypeName" />
              <bean parent="FieldDefinition"
                    p:attributeName="value" />
            </list>
          </property>
          <property name="title" value="" />
        </bean>
      </list>
    </property>
    <property name="title" value="Group Attribute Inquiry" />
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="GroupAttribute-lookupDefinition" parent="GroupAttribute-lookupDefinition-parentBean" />

  <bean id="GroupAttribute-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="instructions" value="Lookup a Group Attribute" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="id" />
        <bean parent="FieldDefinition"
              p:attributeName="attributeName" />
        <bean parent="FieldDefinition"
              p:attributeName="attributeTypeId" />
        <bean parent="FieldDefinition"
              p:attributeName="groupId" />
        <bean parent="FieldDefinition"
              p:attributeName="value" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="id" />
        <bean parent="FieldDefinition"
              p:attributeName="attributeName" />
        <bean parent="FieldDefinition"
              p:attributeName="group.name" />
        <bean parent="FieldDefinition"
              p:attributeName="attributeType.attributeTypeName" />
        <bean parent="FieldDefinition"
              p:attributeName="value" />
      </list>
    </property>
    <property name="title" value="Group Attribute Lookup" />
  </bean>
</beans>
