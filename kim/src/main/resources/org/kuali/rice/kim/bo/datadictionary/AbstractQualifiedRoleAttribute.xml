<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AbstractQualifiedRoleAttribute" parent="AbstractQualifiedRoleAttribute-parentBean" />

  <bean id="AbstractQualifiedRoleAttribute-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.AbstractQualifiedRoleAttribute" />
    <property name="titleAttribute" value="attributeName" />
    <property name="objectLabel" value="Abstract Qualified Role Attribute" />
    <property name="attributes" >
      <list>
        <ref bean="AbstractQualifiedRoleAttribute-id" />
        <ref bean="AbstractQualifiedRoleAttribute-attributeName" />
        <ref bean="AbstractQualifiedRoleAttribute-attributeValue" />
        <ref bean="AbstractQualifiedRoleAttribute-roleId" />
        <ref bean="AbstractQualifiedRoleAttribute-versionNumber" />
        <ref bean="AbstractQualifiedRoleAttribute-objectId" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="AbstractQualifiedRoleAttribute-id" parent="AbstractQualifiedRoleAttribute-id-parentBean" />

  <bean id="AbstractQualifiedRoleAttribute-id-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="name" value="id" />
    <property name="label" value="Id" />
  </bean>
  <bean id="AbstractQualifiedRoleAttribute-attributeName" parent="AbstractQualifiedRoleAttribute-attributeName-parentBean" />

  <bean id="AbstractQualifiedRoleAttribute-attributeName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="255" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="attributeName" />
    <property name="label" value="Name" />
  </bean>
  <bean id="AbstractQualifiedRoleAttribute-attributeValue" parent="AbstractQualifiedRoleAttribute-attributeValue-parentBean" />

  <bean id="AbstractQualifiedRoleAttribute-attributeValue-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Value" />
    <property name="maxLength" value="255" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="attributeValue" />
    <property name="label" value="Value" />
  </bean>
  <bean id="AbstractQualifiedRoleAttribute-roleId" parent="AbstractQualifiedRoleAttribute-roleId-parentBean" />

  <bean id="AbstractQualifiedRoleAttribute-roleId-parentBean" abstract="true" parent="Role-id">
    <property name="shortLabel" value="Role Id" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="name" value="roleId" />
    <property name="label" value="Role Id" />
  </bean>
  <bean id="AbstractQualifiedRoleAttribute-versionNumber" parent="AbstractQualifiedRoleAttribute-versionNumber-parentBean" />

  <bean id="AbstractQualifiedRoleAttribute-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
  <bean id="AbstractQualifiedRoleAttribute-objectId" parent="AbstractQualifiedRoleAttribute-objectId-parentBean" />

  <bean id="AbstractQualifiedRoleAttribute-objectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
  </bean>
</beans>
