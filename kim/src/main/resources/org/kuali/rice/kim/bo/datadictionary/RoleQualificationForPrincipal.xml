<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RoleQualificationForPrincipal" parent="RoleQualificationForPrincipal-parentBean" />

  <bean id="RoleQualificationForPrincipal-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.RoleQualificationForPrincipal" />
    <property name="titleAttribute" value="id" />
    <property name="objectLabel" value="Role Qualification for Principal" />
    <property name="collections" >
      <list>
        <bean parent="CollectionDefinition"
              p:shortLabel="Qualifications"
              p:summary="Qualification Attributes"
              p:elementLabel="Qualification"
              p:name="qualifiedRoleAttributes"
              p:label="Qualifications"
              p:description="Qualification Attributes" />
      </list>
    </property>
    <property name="attributes" >
      <list>
        <ref bean="RoleQualificationForPrincipal-id" />
        <ref bean="RoleQualificationForPrincipal-name" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="RoleQualificationForPrincipal-id" parent="RoleQualificationForPrincipal-id-parentBean" />

  <bean id="RoleQualificationForPrincipal-id-parentBean" abstract="true" parent="Role-id">
    <property name="shortLabel" value="Id" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:businessObjectClass="org.kuali.rice.kim.bo.Role"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
            p:includeKeyInLabel="false"
            p:keyAttribute="id"
            p:labelAttribute="name" />
    </property>
    <property name="label" value="Id" />
  </bean>
  <bean id="RoleQualificationForPrincipal-name" parent="RoleQualificationForPrincipal-name-parentBean" />

  <bean id="RoleQualificationForPrincipal-name-parentBean" abstract="true" parent="Role-name">
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
</beans>
