<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="GroupTypeDefaultAttribute" parent="GroupTypeDefaultAttribute-parentBean" />

  <bean id="GroupTypeDefaultAttribute-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.GroupTypeDefaultAttribute" />
    <property name="titleAttribute" value="attributeName" />
    <property name="objectLabel" value="Group Type Default Attribute" />
    <property name="attributes" >
      <list>
        <ref bean="GroupTypeDefaultAttribute-id" />
        <ref bean="GroupTypeDefaultAttribute-attributeName" />
        <ref bean="GroupTypeDefaultAttribute-description" />
        <ref bean="GroupTypeDefaultAttribute-attributeTypeId" />
        <ref bean="GroupTypeDefaultAttribute-groupTypeId" />
        <ref bean="GroupTypeDefaultAttribute-required" />
        <ref bean="GroupTypeDefaultAttribute-active" />
        <ref bean="GroupTypeDefaultAttribute-versionNumber" />
        <ref bean="GroupTypeDefaultAttribute-objectId" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="GroupTypeDefaultAttribute-id" parent="GroupTypeDefaultAttribute-id-parentBean" />

  <bean id="GroupTypeDefaultAttribute-id-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="name" value="id" />
    <property name="label" value="Id" />
  </bean>
  <bean id="GroupTypeDefaultAttribute-attributeName" parent="GroupTypeDefaultAttribute-attributeName-parentBean" />

  <bean id="GroupTypeDefaultAttribute-attributeName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="500" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="attributeName" />
    <property name="label" value="Name" />
  </bean>
  <bean id="GroupTypeDefaultAttribute-description" parent="GroupTypeDefaultAttribute-description-parentBean" />

  <bean id="GroupTypeDefaultAttribute-description-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="4000" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="description" />
    <property name="label" value="Description" />
  </bean>
  <bean id="GroupTypeDefaultAttribute-attributeTypeId" parent="GroupTypeDefaultAttribute-attributeTypeId-parentBean" />

  <bean id="GroupTypeDefaultAttribute-attributeTypeId-parentBean" abstract="true" parent="AttributeType-id">
    <property name="shortLabel" value="Type" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:businessObjectClass="org.kuali.rice.kim.bo.AttributeType"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
            p:includeKeyInLabel="true"
            p:keyAttribute="id"
            p:labelAttribute="attributeTypeName" />
    </property>
    <property name="name" value="attributeTypeId" />
    <property name="label" value="Type" />
  </bean>
  <bean id="GroupTypeDefaultAttribute-groupTypeId" parent="GroupTypeDefaultAttribute-groupTypeId-parentBean" />

  <bean id="GroupTypeDefaultAttribute-groupTypeId-parentBean" abstract="true" parent="GroupType-id">
    <property name="shortLabel" value="Group Type" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:businessObjectClass="org.kuali.rice.kim.bo.GroupType"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
            p:includeKeyInLabel="true"
            p:keyAttribute="id"
            p:labelAttribute="name" />
    </property>
    <property name="name" value="groupTypeId" />
    <property name="label" value="Group Type" />
  </bean>
  <bean id="GroupTypeDefaultAttribute-required" parent="GroupTypeDefaultAttribute-required-parentBean" />

  <bean id="GroupTypeDefaultAttribute-required-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Required" />
    <property name="maxLength" value="3" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.IndicatorYNOnlyValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="name" value="required" />
    <property name="label" value="Required" />
  </bean>
  <bean id="GroupTypeDefaultAttribute-active" parent="GroupTypeDefaultAttribute-active-parentBean" />

  <bean id="GroupTypeDefaultAttribute-active-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Active" />
    <property name="maxLength" value="3" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.IndicatorYNOnlyValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="name" value="active" />
    <property name="label" value="Active" />
  </bean>
  <bean id="GroupTypeDefaultAttribute-versionNumber" parent="GroupTypeDefaultAttribute-versionNumber-parentBean" />

  <bean id="GroupTypeDefaultAttribute-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
  <bean id="GroupTypeDefaultAttribute-objectId" parent="GroupTypeDefaultAttribute-objectId-parentBean" />

  <bean id="GroupTypeDefaultAttribute-objectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
  </bean>
</beans>
