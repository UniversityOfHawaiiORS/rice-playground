<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "https://test.kuali.org/dtds/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.rice.kim.bo.EntityType">

        <inquiry>
            <title>EntityType Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="id" />
                <field attributeName="name" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>EntityType Lookup</title>

            <instructions>Lookup a EntityType</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="name" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="id" required="false" />
                <lookupField attributeName="name" required="false" />
                <lookupField attributeName="description" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="id" />
                <field attributeName="name" />
                <field attributeName="description" />
            </resultFields>
        </lookup>

        <titleAttribute>id</titleAttribute>

        <attributes>

            <attribute name="id" forceUppercase="true">
                <label>ID</label>
                <shortLabel>id</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="8" />
                </control>

			</attribute>

            <attribute name="name" forceUppercase="false">
                <label>EntityType Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>500</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="100" />
                </control>

			</attribute>
			
			<attribute name="description" forceUppercase="false">
                <label>EntityType Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>4000</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <textarea rows="10" cols="50" />
                </control>

			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
			<attributeReference name="objectId" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="objectId" />
        </attributes>
        <objectLabel>EntityType</objectLabel>
    </businessObject>
</dictionaryEntry>