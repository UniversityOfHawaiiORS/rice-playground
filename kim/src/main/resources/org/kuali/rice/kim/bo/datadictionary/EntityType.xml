<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="EntityType" parent="EntityType-parentBean" />

  <bean id="EntityType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.EntityType" />
    <property name="inquiryDefinition" >
      <ref bean="EntityType-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="EntityType-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="name" />
    <property name="objectLabel" value="Entity Type" />
    <property name="attributes" >
      <list>
        <ref bean="EntityType-id" />
        <ref bean="EntityType-name" />
        <ref bean="EntityType-description" />
        <ref bean="EntityType-versionNumber" />
        <ref bean="EntityType-objectId" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="EntityType-id" parent="EntityType-id-parentBean" />

  <bean id="EntityType-id-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="name" value="id" />
    <property name="label" value="Id" />
  </bean>
  <bean id="EntityType-name" parent="EntityType-name-parentBean" />

  <bean id="EntityType-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Type Name" />
    <property name="maxLength" value="500" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="name" />
    <property name="label" value="Type Name" />
  </bean>
  <bean id="EntityType-description" parent="EntityType-description-parentBean" />

  <bean id="EntityType-description-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="4000" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextareaControlDefinition"
            p:cols="50"
            p:rows="10" />
    </property>
    <property name="name" value="description" />
    <property name="label" value="Description" />
  </bean>
  <bean id="EntityType-versionNumber" parent="EntityType-versionNumber-parentBean" />

  <bean id="EntityType-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
  <bean id="EntityType-objectId" parent="EntityType-objectId-parentBean" />

  <bean id="EntityType-objectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="EntityType-inquiryDefinition" parent="EntityType-inquiryDefinition-parentBean" />

  <bean id="EntityType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="id" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
              <bean parent="FieldDefinition"
                    p:attributeName="description" />
            </list>
          </property>
          <property name="title" value="Information" />
        </bean>
      </list>
    </property>
    <property name="title" value="Entity Type Inquiry" />
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="EntityType-lookupDefinition" parent="EntityType-lookupDefinition-parentBean" />

  <bean id="EntityType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="instructions" value="Lookup an Entity Type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="id" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="id" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
      </list>
    </property>
    <property name="title" value="Entity Type Lookup" />
    <property name="menubar" value="&lt;a href=&quot;../kim/index.jsp&quot;&gt;KIM Home&lt;/a&gt;   &lt;a href=&quot;../index.html&quot;&gt;Main&lt;/a&gt;" />
  </bean>
</beans>
