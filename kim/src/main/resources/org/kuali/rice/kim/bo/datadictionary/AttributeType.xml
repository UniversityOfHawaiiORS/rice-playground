<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "https://test.kuali.org/dtds/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.rice.kim.bo.AttributeType">

        <inquiry>
            <title>Attribute Type Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="id" />
                <field attributeName="attributeTypeName" />
                <field attributeName="description" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Attribute Type Lookup</title>

            <instructions>Lookup a Attribute Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="attributeTypeName" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="id" required="false" />
                <lookupField attributeName="attributeTypeName" required="false" />
                <lookupField attributeName="description" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="id" />
                <field attributeName="attributeTypeName" />
               <field attributeName="description" />
            </resultFields>
        </lookup>

        <titleAttribute>id</titleAttribute>

        <attributes>

            <attribute name="id" forceUppercase="true">
                <label>ID</label>
                <shortLabel>id</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="8" />
                </control>

			</attribute>

            <attribute name="attributeTypeName" forceUppercase="true">
                <label>Attribute Type Name</label>
                <shortLabel>Type Name</shortLabel>

                <maxLength>100</maxLength>
                <validationPattern><anyCharacter allowWhitespace="false" /></validationPattern>

                <control>
                    <text size="500" />
                </control>

			</attribute>

            <attribute name="description" forceUppercase="false">
                <label>Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>100</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <text size="4000" />
                </control>

            </attribute>
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
        <objectLabel>Attribute Type</objectLabel>
    </businessObject>
</dictionaryEntry>
