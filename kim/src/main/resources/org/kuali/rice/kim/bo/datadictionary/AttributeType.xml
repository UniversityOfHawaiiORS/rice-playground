<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AttributeType" parent="AttributeType-parentBean" />

  <bean id="AttributeType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.AttributeType" />
    <property name="inquiryDefinition" >
      <ref bean="AttributeType-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="AttributeType-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="attributeTypeName" />
    <property name="objectLabel" value="Attribute Type" />
    <property name="attributes" >
      <list>
        <ref bean="AttributeType-id" />
        <ref bean="AttributeType-attributeTypeName" />
        <ref bean="AttributeType-description" />
        <ref bean="AttributeType-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="AttributeType-id" parent="AttributeType-id-parentBean" />

  <bean id="AttributeType-id-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="name" value="id" />
    <property name="label" value="Id" />
  </bean>
  <bean id="AttributeType-attributeTypeName" parent="AttributeType-attributeTypeName-parentBean" />

  <bean id="AttributeType-attributeTypeName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="500" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="attributeTypeName" />
    <property name="label" value="Name" />
  </bean>
  <bean id="AttributeType-description" parent="AttributeType-description-parentBean" />

  <bean id="AttributeType-description-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="4000" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="description" />
    <property name="label" value="Description" />
  </bean>
  <bean id="AttributeType-versionNumber" parent="AttributeType-versionNumber-parentBean" />

  <bean id="AttributeType-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="AttributeType-inquiryDefinition" parent="AttributeType-inquiryDefinition-parentBean" />

  <bean id="AttributeType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="id" />
              <bean parent="FieldDefinition"
                    p:attributeName="attributeTypeName" />
              <bean parent="FieldDefinition"
                    p:attributeName="description" />
            </list>
          </property>
          <property name="title" value="Header Text" />
        </bean>
      </list>
    </property>
    <property name="title" value="Attribute Type Inquiry" />
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="AttributeType-lookupDefinition" parent="AttributeType-lookupDefinition-parentBean" />

  <bean id="AttributeType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="instructions" value="Lookup a Attribute Type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="id" />
        <bean parent="FieldDefinition"
              p:attributeName="attributeTypeName" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="id" />
        <bean parent="FieldDefinition"
              p:attributeName="attributeTypeName" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
      </list>
    </property>
    <property name="title" value="Attribute Type Lookup" />
  </bean>
</beans>
