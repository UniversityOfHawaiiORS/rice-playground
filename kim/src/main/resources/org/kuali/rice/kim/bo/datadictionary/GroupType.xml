<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="GroupType" parent="GroupType-parentBean" />

  <bean id="GroupType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.GroupType" />
    <property name="inquiryDefinition" >
      <ref bean="GroupType-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="GroupType-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="name" />
    <property name="objectLabel" value="Group Type" />
    <property name="collections" >
      <list>
        <bean parent="CollectionDefinition"
              p:shortLabel="Default Attributes"
              p:summary="Group Type Default Attributes"
              p:elementLabel="Attribute"
              p:name="groupTypeDefaultAttributes"
              p:label="Group Type Default Attributes"
              p:description="Default attributes for this group type." />
      </list>
    </property>
    <property name="attributes" >
      <list>
        <ref bean="GroupType-id" />
        <ref bean="GroupType-name" />
        <ref bean="GroupType-description" />
        <ref bean="GroupType-workflowDocumentType" />
        <ref bean="GroupType-versionNumber" />
        <ref bean="GroupType-objectId" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="GroupType-id" parent="GroupType-id-parentBean" />

  <bean id="GroupType-id-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="name" value="id" />
    <property name="label" value="Id" />
  </bean>
  <bean id="GroupType-name" parent="GroupType-name-parentBean" />

  <bean id="GroupType-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="500" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="name" />
    <property name="label" value="Name" />
  </bean>
  <bean id="GroupType-description" parent="GroupType-description-parentBean" />

  <bean id="GroupType-description-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="4000" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="description" />
    <property name="label" value="Description" />
  </bean>
  <bean id="GroupType-workflowDocumentType" parent="GroupType-workflowDocumentType-parentBean" />

  <bean id="GroupType-workflowDocumentType-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Workflow Doc Type" />
    <property name="maxLength" value="500" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="workflowDocumentType" />
    <property name="label" value="Workflow Document Type" />
  </bean>
  <bean id="GroupType-versionNumber" parent="GroupType-versionNumber-parentBean" />

  <bean id="GroupType-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
  <bean id="GroupType-objectId" parent="GroupType-objectId-parentBean" />

  <bean id="GroupType-objectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="GroupType-inquiryDefinition" parent="GroupType-inquiryDefinition-parentBean" />

  <bean id="GroupType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="id" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
              <bean parent="FieldDefinition"
                    p:attributeName="description" />
              <bean parent="FieldDefinition"
                    p:attributeName="workflowDocumentType" />
            </list>
          </property>
          <property name="title" value="Group Type Information" />
        </bean>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="InquiryCollectionDefinition">
                <property name="attributeName" value="groupTypeDefaultAttributes" />
                <property name="businessObjectClass" value="org.kuali.rice.kim.bo.GroupTypeDefaultAttribute" />
                <property name="numberOfColumns" value="1" />
                <property name="inquiryFields" >
                  <list>
                    <bean parent="FieldDefinition"
                          p:attributeName="id" />
                    <bean parent="FieldDefinition"
                          p:attributeName="attributeTypeId" />
                    <bean parent="FieldDefinition"
                          p:attributeName="attributeName" />
                    <bean parent="FieldDefinition"
                          p:attributeName="description" />
                    <bean parent="FieldDefinition"
                          p:attributeName="required" />
                    <bean parent="FieldDefinition"
                          p:attributeName="active" />
                  </list>
                </property>
              </bean>
            </list>
          </property>
          <property name="title" value="Default Attributes" />
        </bean>
      </list>
    </property>
    <property name="title" value="Group Type Inquiry" />
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="GroupType-lookupDefinition" parent="GroupType-lookupDefinition-parentBean" />

  <bean id="GroupType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="instructions" value="Lookup a Group Type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="id" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
        <bean parent="FieldDefinition"
              p:attributeName="workflowDocumentType" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="id" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
        <bean parent="FieldDefinition"
              p:attributeName="workflowDocumentType" />
      </list>
    </property>
    <property name="title" value="Group Type Lookup" />
  </bean>
</beans>
