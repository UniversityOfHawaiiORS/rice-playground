<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="KIMEntityMaintenanceDocument" parent="KIMEntityMaintenanceDocument-parentBean" />

  <bean id="KIMEntityMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Entity" />
    
    <property name="maintainableClass" value="org.kuali.rice.kim.document.maintenance.EntityMaintainable" />
    
    <property name="maintainableSections" >
      <list>
        <ref bean="KIMEntityMaintenanceDocument-EditEntity" />
        <ref bean="KIMEntityMaintenanceDocument-EntityPrincipal" />
<!--    <ref bean="KIMEntityMaintenanceDocument-EntityAttributes" />  --> 
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>id</value>
      </list>
    </property>
    <property name="shortLabel" value="Entity" />
    <property name="summary" value="Entity Maintenance Document" />
    <property name="documentTypeName" value="KIMEntityMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="KEMD" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
          <property name="action" value="initiate" />
        </bean>
      </list>
    </property>
    <property name="label" value="Entity" />
    <property name="description" value="Maintenance document which allows you to create new Entity,
			or modify existing Entity." />
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="KIMEntityMaintenanceDocument-EditEntity" parent="KIMEntityMaintenanceDocument-EditEntity-parentBean" />

  <bean id="KIMEntityMaintenanceDocument-EditEntity-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="id"
              p:readOnly="true" 
              p:required="true"
              p:defaultValueFinderClass="org.kuali.rice.kim.lookup.valuefinder.NextEntityIdFinder" />
        <bean parent="MaintainableFieldDefinition"
              p:required="true"
              p:name="entityTypeId" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
    <property name="id" value="Edit Entity" />
    <property name="title" value="Edit Entity" />
  </bean>
  <bean id="KIMEntityMaintenanceDocument-EntityPrincipal" parent="KIMEntityMaintenanceDocument-EntityPrincipal-parentBean" />

  <bean id="KIMEntityMaintenanceDocument-EntityPrincipal-parentBean" abstract="true" parent="MaintainableSectionDefinition">
  	<property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="sourceClassName" value="org.kuali.rice.kim.bo.Principal" />
          <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Principal" />
          <property name="includeAddLine" value="false" />
          <property name="summaryTitle" value="Associated Principals" />
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:template="id"
                    p:name="id"
                    p:readOnly="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:template="name"
                    p:name="name"
                    p:readOnly="true" />
            </list>
          </property>
          <property name="name" value="principals" />
        </bean>
      </list>
    </property>
    <property name="id" value="Entity Principal" />
    <property name="title" value="Entity Principal" />
  </bean>
  <bean id="KIMEntityMaintenanceDocument-EntityAttributes" parent="KIMEntityMaintenanceDocument-EntityAttributes-parentBean" />

  <bean id="KIMEntityMaintenanceDocument-EntityAttributes-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="businessObjectClass" value="org.kuali.rice.kim.bo.EntityAttribute" />
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:template="id"
                    p:name="id"
                    p:readOnly="true" 
                    p:defaultValueFinderClass="org.kuali.rice.kim.lookup.valuefinder.NextEntityAttributeIdFinder" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:template="attributeTypeId"
                    p:name="attributeTypeId" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:template="namespaceId"
                    p:name="namespaceId" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:template="attributeName"
                    p:name="attributeName" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:template="value"
                    p:name="value" />
            </list>
          </property>
          <property name="name" value="entityAttributes" />
        </bean>
      </list>
    </property>
    <property name="id" value="Entity Attributes" />
    <property name="title" value="Entity Attributes" />
  </bean>
</beans>
