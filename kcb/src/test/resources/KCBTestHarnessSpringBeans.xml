<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<!-- this is a bootstrap Spring file that the unit tests use  -->
<beans>
    <bean id="config" class="org.kuali.rice.config.spring.ConfigFactoryBean">
        <property name="configLocations">
            <list>
                <value>classpath:META-INF/kcb-test-config.xml</value>
            </list>
        </property>
    </bean>

    <bean id="rice" class="org.kuali.rice.config.RiceConfigurer">
        <property name="messageEntity" value="KCB" />
        <property name="dataSource" ref="dataSource" />
        <property name="transactionManager" ref="atomikosTransactionManager" />
        <property name="userTransaction" ref="atomikosUserTransaction" />
        <property name="rootConfig" ref="config" />
        <property name="modules">
            <list>
                <bean class="edu.iu.uis.eden.messaging.config.KSBConfigurer">
                    <property name="serviceServletUrl" value="${serviceServletUrl}" />
                </bean>
                <bean class="org.kuali.rice.kcb.config.KCBConfigurer">
                    <property name="testMode" value="true"/>
                </bean>
                <bean class="org.kuali.notification.config.KENConfigurer">
                    <property name="testMode" value="true"/>
                </bean>
            </list>
        </property>
    </bean>
</beans>