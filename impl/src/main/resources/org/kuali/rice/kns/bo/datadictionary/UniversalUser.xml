<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="Person" parent="Person-parentBean" />

  <bean id="Person-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Person" />
    <property name="inquiryDefinition" >
      <ref bean="Person-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="Person-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="principalId" />
    <property name="attributes" >
      <list>
        <ref bean="Person-principalId" />
        <ref bean="Person-principalName" />
        <ref bean="Person-personPayrollIdentifier" />
        <ref bean="Person-personName" />
        <ref bean="Person-employeeStatusCode" />
        <ref bean="Person-employeeTypeCode" />
        <ref bean="Person-student" />
        <ref bean="Person-staff" />
        <ref bean="Person-faculty" />
        <ref bean="Person-affiliate" />
        <ref bean="Person-primaryDepartmentCode" />
        <ref bean="Person-personEmailAddress" />
        <ref bean="Person-personFirstName" />
        <ref bean="Person-personLastName" />
        <ref bean="Person-personMiddleName" />
        <ref bean="Person-personCampusAddress" />
        <ref bean="Person-personLocalPhoneNumber" />
        <ref bean="Person-personBaseSalaryAmount" />
        <ref bean="Person-personTaxIdentifier" />
        <ref bean="Person-personTaxIdentifierTypeCode" />
        <ref bean="Person-financialSystemsEncryptedPasswordText" />
        <ref bean="Person-campusCode" />
        <ref bean="Person-versionNumber" />
      </list>
    </property>
    <property name="objectLabel" value="Universal User" />
  </bean>

<!-- Attribute Definitions -->


  <bean id="Person-principalId" parent="Person-principalId-parentBean" />

  <bean id="Person-principalId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Univ User Id" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Universal User ID" />
    <property name="name" value="principalId" />
    <property name="label" value="Universal User ID" />
    <property name="description" value="Universal User ID" />
  </bean>
  <bean id="Person-principalName" parent="Person-principalName-parentBean" />

  <bean id="Person-principalName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="User ID" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="summary" value="User ID" />
    <property name="name" value="principalName" />
    <property name="label" value="User ID" />
    <property name="description" value="User ID" />
  </bean>
  <bean id="Person-personPayrollIdentifier" parent="Person-personPayrollIdentifier-parentBean" />

  <bean id="Person-personPayrollIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Empl ID" />
    <property name="maxLength" value="11" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="13" />
    </property>
    <property name="summary" value="Empl ID" />
    <property name="name" value="personPayrollIdentifier" />
    <property name="label" value="Employee ID" />
    <property name="description" value="This field lists the individualï¿½s HRMS Employee ID.  
					This is a unique 9-digit number assigned to each 
					employee as they are entered into the HRMS system." />
  </bean>
  <bean id="Person-personName" parent="Person-personName-parentBean" />

  <bean id="Person-personName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="80" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="Display Name" />
    <property name="name" value="name" />
    <property name="label" value="Display Name" />
    <property name="description" value="Name" />
  </bean>
  <bean id="Person-employeeStatusCode" parent="Person-employeeStatusCode-parentBean" />

  <bean id="Person-employeeStatusCode-parentBean" abstract="true" parent="EmployeeStatus-code">
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.rice.kns.lookup.keyvalues.EmployeeStatusValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="name" value="employeeStatusCode" />
  </bean>
  <bean id="Person-employeeTypeCode" parent="Person-employeeTypeCode-parentBean" />

  <bean id="Person-employeeTypeCode-parentBean" abstract="true" parent="EmployeeType-code">
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.rice.kns.lookup.keyvalues.EmployeeTypeValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="name" value="employeeTypeCode" />
  </bean>
  <bean id="Person-student" parent="Person-student-parentBean" />

  <bean id="Person-student-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean">
    <property name="shortLabel" value="Stdt?" />
    <property name="summary" value="Student?" />
    <property name="name" value="student" />
    <property name="label" value="Student?" />
    <property name="description" value="Is this person a student?" />
  </bean>
  <bean id="Person-staff" parent="Person-staff-parentBean" />

  <bean id="Person-staff-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean">
    <property name="shortLabel" value="Stf?" />
    <property name="summary" value="Staff?" />
    <property name="name" value="staff" />
    <property name="label" value="Staff?" />
    <property name="description" value="Is this person a staff member?" />
  </bean>
  <bean id="Person-faculty" parent="Person-faculty-parentBean" />

  <bean id="Person-faculty-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean">
    <property name="shortLabel" value="Fcty?" />
    <property name="summary" value="Faculty?" />
    <property name="name" value="faculty" />
    <property name="label" value="Faculty?" />
    <property name="description" value="Is this person a faculty member?" />
  </bean>
  <bean id="Person-affiliate" parent="Person-affiliate-parentBean" />

  <bean id="Person-affiliate-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean">
    <property name="shortLabel" value="Aflt?" />
    <property name="summary" value="Affiliate?" />
    <property name="name" value="affiliate" />
    <property name="label" value="Affiliate?" />
    <property name="description" value="Is this person an affiliate of the institution?" />
  </bean>
  <bean id="Person-primaryDepartmentCode" parent="Person-primaryDepartmentCode-parentBean" />

  <bean id="Person-primaryDepartmentCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="DeptId" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Chart-Org" />
    <property name="name" value="primaryDepartmentCode" />
    <property name="label" value="Department Id" />
    <property name="description" value="Chart-Org" />
  </bean>
  <bean id="Person-personEmailAddress" parent="Person-personEmailAddress-parentBean" />

  <bean id="Person-personEmailAddress-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Email" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="EmailAddressValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="summary" value="Email Address" />
    <property name="name" value="emailAddress" />
    <property name="label" value="Email Address" />
    <property name="description" value="Email Address" />
  </bean>
  <bean id="Person-personFirstName" parent="Person-personFirstName-parentBean" />

  <bean id="Person-personFirstName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="1st Name" />
    <property name="maxLength" value="20" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="20" />
    </property>
    <property name="summary" value="Person First Name" />
    <property name="name" value="firstName" />
    <property name="label" value="First Name" />
    <property name="description" value="Person First Name" />
  </bean>
  <bean id="Person-personLastName" parent="Person-personLastName-parentBean" />

  <bean id="Person-personLastName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Lst Name" />
    <property name="maxLength" value="20" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="20" />
    </property>
    <property name="summary" value="Person Last Name" />
    <property name="name" value="lastName" />
    <property name="label" value="Last Name" />
    <property name="description" value="Person Last Name" />
  </bean>
  <bean id="Person-personMiddleName" parent="Person-personMiddleName-parentBean" />

  <bean id="Person-personMiddleName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Mdl Name" />
    <property name="maxLength" value="20" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="20" />
    </property>
    <property name="summary" value="Person Last Name" />
    <property name="name" value="personMiddleName" />
    <property name="label" value="Middle Name" />
    <property name="description" value="Person Last Name" />
  </bean>
  <bean id="Person-personCampusAddress" parent="Person-personCampusAddress-parentBean" />

  <bean id="Person-personCampusAddress-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Cmp Addr" />
    <property name="maxLength" value="55" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="summary" value="Person Campus Address" />
    <property name="name" value="addressLine1" />
    <property name="label" value="Campus Address" />
    <property name="description" value="Person Campus Address" />
  </bean>
  <bean id="Person-personLocalPhoneNumber" parent="Person-personLocalPhoneNumber-parentBean" />

  <bean id="Person-personLocalPhoneNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Phn Nbr" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
    <property name="summary" value="Person Local Phone Number" />
    <property name="name" value="personLocalPhoneNumber" />
    <property name="label" value="Local Phone Number" />
    <property name="description" value="Person Local Phone Number" />
  </bean>
  <bean id="Person-personBaseSalaryAmount" parent="Person-personBaseSalaryAmount-parentBean" />

  <bean id="Person-personBaseSalaryAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Salary" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FixedPointValidationPattern"
            p:precision="19"
            p:scale="2" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Person Base Salary" />
    <property name="name" value="baseSalaryAmount" />
    <property name="label" value="Base Salary" />
    <property name="description" value="Person Base Salary" />
  </bean>
  <bean id="Person-personTaxIdentifier" parent="Person-personTaxIdentifier-parentBean" />

  <bean id="Person-personTaxIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Tax ID" />
    <property name="maxLength" value="9" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="displayMask" >
      <bean parent="Mask">
        <property name="maskFormatter" >
          <bean parent="MaskFormatterLiteral"
                p:literal="***************" />
        </property>
      </bean>
    </property>
    <property name="displayWorkgroup" value="UNIVERSAL_USER_MAINTAINERS" />
    <property name="summary" value="SSN" />
    <property name="name" value="personTaxIdentifier" />
    <property name="label" value="Tax Identifier" />
    <property name="description" value="Social Security Number" />
  </bean>
  <bean id="Person-personTaxIdentifierTypeCode" parent="Person-personTaxIdentifierTypeCode-parentBean" />

  <bean id="Person-personTaxIdentifierTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Tax ID Type" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.rice.kns.lookup.keyvalues.PersonTaxIdentifierTypeCodeValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="Tax Identifier Type" />
    <property name="name" value="personTaxIdentifierTypeCode" />
    <property name="label" value="Tax Identifier Type" />
    <property name="description" value="Tax Identifier Type" />
  </bean>
  <bean id="Person-financialSystemsEncryptedPasswordText" parent="Person-financialSystemsEncryptedPasswordText-parentBean" />

  <bean id="Person-financialSystemsEncryptedPasswordText-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Password" />
    <property name="maxLength" value="200" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="20" />
    </property>
    <property name="displayMask" >
      <bean parent="Mask">
        <property name="maskFormatter" >
          <bean parent="MaskFormatterLiteral"
                p:literal="***************" />
        </property>
      </bean>
    </property>
    <property name="displayWorkgroup" value="UNIVERSAL_USER_MAINTAINERS" />
    <property name="summary" value="Password" />
    <property name="name" value="financialSystemsEncryptedPasswordText" />
    <property name="label" value="Password" />
    <property name="description" value="Password" />
  </bean>
  <bean id="Person-campusCode" parent="Person-campusCode-parentBean" />

  <bean id="Person-campusCode-parentBean" abstract="true" parent="Campus-campusCode">
    <property name="required" value="false" />
    <property name="description" value="The code for the Campus of the user." />
  </bean>
  <bean id="Person-versionNumber" parent="Person-versionNumber-parentBean" />

  <bean id="Person-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="Person-inquiryDefinition" parent="Person-inquiryDefinition-parentBean" />

  <bean id="Person-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="principalName" />
              <bean parent="FieldDefinition"
                    p:attributeName="principalId"
                    p:noInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="personPayrollIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
              <bean parent="FieldDefinition"
                    p:attributeName="campusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="primaryDepartmentCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="staff" />
              <bean parent="FieldDefinition"
                    p:attributeName="faculty" />
              <bean parent="FieldDefinition"
                    p:attributeName="student" />
              <bean parent="FieldDefinition"
                    p:attributeName="affiliate" />
              <bean parent="FieldDefinition"
                    p:attributeName="employeeStatusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="employeeTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="emailAddress" />
            </list>
          </property>
          <property name="title" value="" />
        </bean>
      </list>
    </property>
    <property name="title" value="User Inquiry" />
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="Person-lookupDefinition" parent="Person-lookupDefinition-parentBean" />

  <bean id="Person-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="lookupableID" value="personLookupable" />
    <property name="instructions" value="Lookup a User" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>principalName</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="principalName" />
        <bean parent="FieldDefinition"
              p:attributeName="principalId" />
        <bean parent="FieldDefinition"
              p:attributeName="personPayrollIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="lastName" />
        <bean parent="FieldDefinition"
              p:attributeName="firstName" />
        <bean parent="FieldDefinition"
              p:attributeName="emailAddress" />
        <bean parent="FieldDefinition"
              p:attributeName="campusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="primaryDepartmentCode" />
        <bean parent="FieldDefinition"
              p:attributeName="staff" />
        <bean parent="FieldDefinition"
              p:attributeName="faculty" />
        <bean parent="FieldDefinition"
              p:attributeName="student" />
        <bean parent="FieldDefinition"
              p:attributeName="affiliate" />
        <bean parent="FieldDefinition"
              p:attributeName="employeeStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="employeeTypeCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="principalName" />
        <bean parent="FieldDefinition"
              p:attributeName="principalId" />
        <bean parent="FieldDefinition"
              p:attributeName="personPayrollIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="campusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="primaryDepartmentCode" />
        <bean parent="FieldDefinition"
              p:attributeName="employeeStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="employeeTypeCode" />
      </list>
    </property>
    <property name="title" value="User Lookup" />
  </bean>
</beans>

