<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RiceNamespace" parent="RiceNamespace-parentBean" />

  <bean id="RiceNamespace-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kns.bo.RiceNamespace" />
    <property name="inquiryDefinition" >
      <ref bean="RiceNamespace-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="RiceNamespace-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="name" />
    <property name="attributes" >
      <list>
        <ref bean="RiceNamespace-code" />
        <ref bean="RiceNamespace-name" />
        <ref bean="RiceNamespace-codeAndDescription" />
        <ref bean="RiceNamespace-applicationNamespaceCode" />
        <ref bean="RiceNamespace-active" />
        <ref bean="RiceNamespace-objectId" />
        <ref bean="RiceNamespace-versionNumber" />
      </list>
    </property>
    <property name="objectLabel" value="org.kuali.rice.kns.bo.RiceNamespace"/>
  </bean>

<!-- Attribute Definitions -->


  <bean id="RiceNamespace-code" parent="RiceNamespace-code-parentBean" />

  <bean id="RiceNamespace-code-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Nmspc Cd" />
    <property name="maxLength" value="20" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Code identifying the namespace." />
    <property name="name" value="code" />
    <property name="label" value="Namespace Code" />
    <property name="description" value="Code identifying the namespace." />
  </bean>
  <bean id="RiceNamespace-name" parent="RiceNamespace-name-parentBean" />

  <bean id="RiceNamespace-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Nmspc Nm" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="summary" value="The name of the namespace." />
    <property name="name" value="name" />
    <property name="label" value="Namespace Name" />
    <property name="description" value="The name of the namespace." />
  </bean>
  <bean id="RiceNamespace-codeAndDescription" parent="RiceNamespace-codeAndDescription-parentBean" />

  <bean id="RiceNamespace-codeAndDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Parm Nmspc" />
    <property name="maxLength" value="100" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="summary" value="The name of the namespace." />
    <property name="name" value="codeAndDescription" />
    <property name="label" value="Namespace" />
    <property name="description" value="The name of the namespace." />
  </bean>
  <bean id="RiceNamespace-applicationNamespaceCode" parent="RiceNamespace-applicationNamespaceCode-parentBean" />

  <bean id="RiceNamespace-applicationNamespaceCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Appl Nmspc" />
    <property name="maxLength" value="20" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="The code of the application namespace." />
    <property name="name" value="applicationNamespaceCode" />
    <property name="label" value="Application Namespace Code" />
    <property name="description" value="The code of the application namespace." />
  </bean>
  <bean id="RiceNamespace-active" parent="RiceNamespace-active-parentBean" />

  <bean id="RiceNamespace-active-parentBean" abstract="true" parent="AttributeReferenceDummy-activeIndicator">
    <property name="name" value="active" />
  </bean>
  <bean id="RiceNamespace-objectId" parent="RiceNamespace-objectId-parentBean" />

  <bean id="RiceNamespace-objectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
  </bean>
  <bean id="RiceNamespace-versionNumber" parent="RiceNamespace-versionNumber-parentBean" />

  <bean id="RiceNamespace-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="RiceNamespace-inquiryDefinition" parent="RiceNamespace-inquiryDefinition-parentBean" />

  <bean id="RiceNamespace-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="code" />
              <bean parent="FieldDefinition"
                    p:attributeName="name"
                    p:noInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="applicationNamespaceCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
          <property name="title" value="Namespace Information" />
        </bean>
      </list>
    </property>
    <property name="title" value="Namespace Inquiry" />
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="RiceNamespace-lookupDefinition" parent="RiceNamespace-lookupDefinition-parentBean" />

  <bean id="RiceNamespace-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="instructions" value="Look up a Namespace." />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>name</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code"
              p:noLookup="true" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="applicationNamespaceCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active"
              p:defaultValue="Y" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="applicationNamespaceCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="title" value="Namespace Lookup" />
  </bean>
</beans>
