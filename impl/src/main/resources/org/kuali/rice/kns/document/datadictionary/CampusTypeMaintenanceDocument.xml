<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="KualiCampusTypeMaintenanceDocument" parent="KualiCampusTypeMaintenanceDocument-parentBean"/>

  <bean id="KualiCampusTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kns.bo.CampusTypeImpl"/>
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl"/>
    <property name="maintainableSections">
      <list>
        <ref bean="KualiCampusTypeMaintenanceDocument-EditCampusType"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>campusTypeCode</value>
      </list>
    </property>



    <property name="documentTypeName" value="CampusTypeMaintenanceDocument"/>
    <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>

    <property name="authorizations">
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="authorizedGroups">
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
          <property name="action" value="initiate"/>
        </bean>
      </list>
    </property>


  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="KualiCampusTypeMaintenanceDocument-EditCampusType" parent="KualiCampusTypeMaintenanceDocument-EditCampusType-parentBean"/>

  <bean id="KualiCampusTypeMaintenanceDocument-EditCampusType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:required="true" p:name="campusTypeCode"/>
        <bean parent="MaintainableFieldDefinition" p:required="true" p:name="campusTypeName"/>
        <bean parent="MaintainableFieldDefinition" p:name="dataObjectMaintenanceCodeActiveIndicator"/>
        <bean parent="MaintainableFieldDefinition" p:name="versionNumber"/>
      </list>
    </property>
    <property name="id" value="Edit Campus Type"/>
    <property name="title" value="Edit Campus Type"/>
  </bean>
</beans>