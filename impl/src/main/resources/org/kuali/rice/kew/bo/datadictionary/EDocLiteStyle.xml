<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="EDocLiteStyle" parent="EDocLiteStyle-parentBean" />

  <bean id="EDocLiteStyle-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kew.edl.bo.EDocLiteStyle" />
    <property name="exporterClass" value="org.kuali.rice.kew.export.DataExporter" />
    <property name="objectLabel" value="Style"/>
    <property name="inquiryDefinition" >
      <ref bean="EDocLiteStyle-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="EDocLiteStyle-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="EDocLiteStyleId" />
    <property name="attributes" >
      <list>
        <ref bean="EDocLiteStyle-EDocLiteStyleId" />
        <ref bean="EDocLiteStyle-name" />
        <ref bean="EDocLiteStyle-xmlContent" />
        <ref bean="EDocLiteStyle-activeInd" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="EDocLiteStyle-EDocLiteStyleId" parent="EDocLiteStyle-EDocLiteStyleId-parentBean" />

  <bean id="EDocLiteStyle-EDocLiteStyleId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Id" />
    <property name="maxLength" value="30" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="30" />
    </property>
    <property name="name" value="EDocLiteStyleId" />
    <property name="label" value="Id" />
  </bean>

  <bean id="EDocLiteStyle-name" parent="EDocLiteStyle-name-parentBean" />

  <bean id="EDocLiteStyle-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="name" />
    <property name="maxLength" value="2000" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="30" />
    </property>
    <property name="name" value="name" />
    <property name="label" value="Style Name" />
  </bean> 
 
   <bean id="EDocLiteStyle-xmlContent" parent="EDocLiteStyle-xmlContent-parentBean" />

  <bean id="EDocLiteStyle-xmlContent-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="xmlContent" />
    <property name="maxLength" value="250" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="250" />
    </property>
    <property name="name" value="xmlContent" />
    <property name="label" value="XML Content" />
  </bean>
  <bean id="EDocLiteStyle-activeInd" parent="EDocLiteStyle-activeInd-parentBean" />

  <bean id="EDocLiteStyle-activeInd-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="activeInd" />
    <property name="maxLength" value="250" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="30" />
    </property>
    <property name="name" value="activeInd" />
    <property name="label" value="Active Indicator" />
  </bean>
   
 
  <!-- Business Object Inquiry Definition -->

  <bean id="EDocLiteStyle-inquiryDefinition" parent="EDocLiteStyle-inquiryDefinition-parentBean" />

  <bean id="EDocLiteStyle-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="inquirySections">
      <list>
     	<bean parent="InquirySectionDefinition">
          <property name="title" value="EdocLiteStyle" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="EDocLiteStyleId" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
              <bean parent="FieldDefinition"
                    p:attributeName="xmlContent" />
              <bean parent="FieldDefinition"
                    p:attributeName="activeInd"/>              
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

  <!-- Business Object Lookup Definition -->

  <bean id="EDocLiteStyle-lookupDefinition" parent="EDocLiteStyle-lookupDefinition-parentBean" />

  <bean id="EDocLiteStyle-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="instructions" value="Lookup a Rule Template" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>         
        <bean parent="FieldDefinition"
              p:attributeName="EDocLiteStyleId"
              p:forceLookup="true" />       
        <bean parent="FieldDefinition"
              p:attributeName="name"
              p:forceLookup="true" />
		<bean parent="FieldDefinition"
              p:attributeName="xmlContent" />	 	
     	<bean parent="FieldDefinition"
              p:attributeName="activeInd" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="EDocLiteStyleId" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="xmlContent" />
        <bean parent="FieldDefinition"
              p:attributeName="activeInd" />
	  </list>
    </property>
    <property name="title" value="EdocLiteStyle Lookup" />
  </bean>
</beans>
