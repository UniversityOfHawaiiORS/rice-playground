<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="KimRoleImpl" parent="KimRoleImpl-parentBean" />

  <bean id="KimRoleImpl-parentBean" abstract="true" parent="BusinessObjectEntry"
        p:businessObjectClass="org.kuali.rice.kim.bo.role.impl.KimRoleImpl"
        p:titleAttribute="roleId"
        p:objectLabel="Names"
        p:inquiryDefinition-ref="KimRoleImpl-inquiryDefinition"
        p:lookupDefinition-ref="KimRoleImpl-lookupDefinition" >
    <property name="attributes" >
      <list>
        <ref bean="KimRoleImpl-roleId" />
        <ref bean="KimRoleImpl-kimTypeId" />
        <ref bean="KimRoleImpl-roleName" />
        <ref bean="KimRoleImpl-namespaceCode" />
        <ref bean="KimRoleImpl-active" />
        <ref bean="KimRoleImpl-kimRoleType.name" />
      </list>
    </property>
    
    
        <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="kimRoleType" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="kimTypeId"
                    p:targetName="kimTypeId" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="kimRoleType.name"
                    p:targetName="name" />
            </list>
          </property>
        </bean>
      </list>
    </property>
    
    
  </bean>
 
   <bean id="KimRoleImpl-roleId" parent="KimRoleImpl-roleId-parentBean" />
  <bean id="KimRoleImpl-roleId-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="roleId"
        p:label="Role ID"
        p:shortLabel="Role ID"
        p:maxLength="10"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="10" />
    </property>
  </bean>
  
  <bean id="KimRoleImpl-kimTypeId" parent="KimRoleImpl-kimTypeId-parentBean" />
  <bean id="KimRoleImpl-kimTypeId-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimTypeId"
        p:label="Role Type"
        p:shortLabel="Role Type"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="10" />
    </property>
  </bean>

  <bean id="KimRoleImpl-roleName" parent="KimRoleImpl-roleName-parentBean" />
  <bean id="KimRoleImpl-roleName-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="roleName"
        p:label="Role Name"
        p:shortLabel="Role Name"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="10" />
    </property>
  </bean>
  
  
  <bean id="KimRoleImpl-namespaceCode" parent="KimRoleImpl-namespaceCode-parentBean" />
  <bean id="KimRoleImpl-namespaceCode-parentBean" parent="KimTypeImpl-namespaceCode" abstract="true"
        p:name="namespaceCode"
        p:label="Role Namespace"
        p:shortLabel="Role Namespace"
        p:required="false" 
        p:maxLength="40">
  </bean>

  <bean id="KimRoleImpl-active" parent="KimRoleImpl-active-parentBean" />
  <bean id="KimRoleImpl-active-parentBean" parent="KimTypeImpl-active" abstract="true" p:label="Active?"/>
  
  <bean id="KimRoleImpl-kimRoleType.name" parent="KimRoleImpl-kimRoleType.name-parentBean" />

  <bean id="KimRoleImpl-kimRoleType.name-parentBean" abstract="true" parent="KimTypeImpl-name">
    <property name="name" value="kimRoleType.name" />
    <property name="label" value="Type" />
    <property name="shortLabel" value="Type" />
  </bean>
  
  
  <bean id="KimRoleImpl-inquiryDefinition" parent="KimRoleImpl-inquiryDefinition-parentBean" />

  <bean id="KimRoleImpl-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition"
        p:title="Role Impl Inquiry" >
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition"
              p:title="Role Details"
              p:numberOfColumns="1" >
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition" p:attributeName="roleId" />
              <bean parent="FieldDefinition" p:attributeName="roleName" />
              <bean parent="FieldDefinition" p:attributeName="kimTypeId" />
        	  <bean parent="FieldDefinition" p:attributeName="namespaceCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

  <bean id="KimRoleImpl-lookupDefinition" parent="KimRoleImpl-lookupDefinition-parentBean" />

  <bean id="KimRoleImpl-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition"
        p:title="Role Lookup" 
        p:instructions="Lookup a Role" >
    <property name="lookupableID" value="roleLookupable" />
        
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>roleId</value>
          </list>
        </property>
        <property name="sortAscending" value="true" />
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition" p:attributeName="roleId" />
        <bean parent="FieldDefinition" p:attributeName="roleName" />
        <bean parent="FieldDefinition" p:attributeName="kimTypeId" />
        <bean parent="FieldDefinition" p:attributeName="namespaceCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition" p:attributeName="roleId" />
        <bean parent="FieldDefinition" p:attributeName="roleName" />
        <bean parent="FieldDefinition" p:attributeName="kimTypeId" />
        <bean parent="FieldDefinition" p:attributeName="namespaceCode" />
        <bean parent="FieldDefinition" p:attributeName="kimRoleType.name" />
      </list>
    </property>
  </bean>

</beans>
