<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="KimAttributeDataImpl" parent="KimAttributeDataImpl-parentBean" />

  <bean id="KimAttributeDataImpl-parentBean" abstract="true" parent="BusinessObjectEntry"
        p:businessObjectClass="org.kuali.rice.kim.bo.types.impl.KimAttributeDataImpl"
        p:titleAttribute="attributeDataId"
        p:objectLabel="Attribute Data"
        p:inquiryDefinition-ref="KimAttributeDataImpl-inquiryDefinition">
    <property name="attributes" >
      <list>
        <ref bean="KimAttributeDataImpl-kimAttributeId" />
        <ref bean="KimAttributeDataImpl-attributeName" />
        <ref bean="KimAttributeDataImpl-attributeValue" />
        <ref bean="KimAttributeDataImpl-namespaceCode" />
        <ref bean="KimAttributeDataImpl-attributeLabel" />
        <ref bean="KimAttributeDataImpl-active" />
        <ref bean="KimAttributeDataImpl-componentName" />
        <ref bean="KimAttributeDataImpl-applicationUrl" />
      </list>
    </property>
  </bean>

  <bean id="KimAttributeDataImpl-kimAttributeId" parent="KimAttributeDataImpl-kimAttributeId-parentBean" />
  <bean id="KimAttributeDataImpl-kimAttributeId-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttributeId"
        p:forceUppercase="true"
        p:label="Attribute Id"
        p:shortLabel="Attribute Id"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>
    
  <bean id="KimAttributeDataImpl-attributeName" parent="KimAttributeDataImpl-attributeName-parentBean" />
  <bean id="KimAttributeDataImpl-attributeName-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttribute.attributeName"
        p:label="Attribute Name"
        p:shortLabel="Attribute Name"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="KimAttributeDataImpl-attributeValue" parent="KimAttributeDataImpl-attributeValue-parentBean" />
  <bean id="KimAttributeDataImpl-attributeValue-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="attributeValue"
        p:label="Attribute Value"
        p:shortLabel="Attribute Value"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="KimAttributeDataImpl-namespaceCode" parent="KimAttributeDataImpl-namespaceCode-parentBean" />
  <bean id="KimAttributeDataImpl-namespaceCode-parentBean" parent="RoleImpl-namespaceCode" abstract="true"
        p:name="kimAttribute.namespaceCode"
        p:label="Attribute Namespace Code"
        p:shortLabel="Attribute Namespace Code">
  </bean>
  
  <bean id="KimAttributeDataImpl-attributeLabel" parent="KimAttributeDataImpl-attributeLabel-parentBean" />
  <bean id="KimAttributeDataImpl-attributeLabel-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttribute.attributeLabel"
        p:forceUppercase="true"
        p:label="Attribute Label"
        p:shortLabel="Attribute Label"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="KimAttributeDataImpl-active" parent="KimAttributeDataImpl-active-parentBean" />
  <bean id="KimAttributeDataImpl-active-parentBean" parent="AttributeReferenceDummy-activeIndicator" abstract="true"
  	    p:name="kimAttribute.active" />
  
  <bean id="KimAttributeDataImpl-componentName" parent="KimAttributeDataImpl-componentName-parentBean" />
  <bean id="KimAttributeDataImpl-componentName-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttribute.componentName"
        p:forceUppercase="true"
        p:label="Component Name"
        p:shortLabel="Component Name"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="KimAttributeDataImpl-applicationUrl" parent="KimAttributeDataImpl-applicationUrl-parentBean" />
  <bean id="KimAttributeDataImpl-applicationUrl-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttribute.applicationUrl"
        p:forceUppercase="true"
        p:label="Application Url"
        p:shortLabel="Application Url"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>  
  
  <bean id="KimAttributeDataImpl-inquiryDefinition" parent="KimAttributeDataImpl-inquiryDefinition-parentBean" />
  <bean id="KimAttributeDataImpl-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition"
        p:title="Kim Attribute Data Inquiry" >
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition"
              p:title="Kim Attribute Data"
              p:numberOfColumns="1" >
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition" p:attributeName="kimAttributeId" />
              <bean parent="FieldDefinition" p:attributeName="kimAttribute.attributeName" />
        	  <bean parent="FieldDefinition" p:attributeName="attributeValue" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.namespaceCode" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.attributeLabel" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.active" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.componentName" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.applicationUrl" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

</beans>