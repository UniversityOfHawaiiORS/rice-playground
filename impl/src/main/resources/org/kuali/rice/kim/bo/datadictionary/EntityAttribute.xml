<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="EntityAttribute" parent="EntityAttribute-parentBean" />

  <bean id="EntityAttribute-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.EntityAttribute" />
    <property name="titleAttribute" value="attributeName" />
    <property name="objectLabel" value="Entity Attribute" />
    <property name="attributes" >
      <list>
        <ref bean="EntityAttribute-id" />
        <ref bean="EntityAttribute-entityId" />
        <ref bean="EntityAttribute-attributeTypeId" />
        <ref bean="EntityAttribute-attributeName" />
        <ref bean="EntityAttribute-namespaceId" />
        <ref bean="EntityAttribute-value" />
        <ref bean="EntityAttribute-versionNumber" />
        <ref bean="EntityAttribute-objectId" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="EntityAttribute-id" parent="EntityAttribute-id-parentBean" />

  <bean id="EntityAttribute-id-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="name" value="id" />
    <property name="label" value="Id" />
  </bean>
  <bean id="EntityAttribute-entityId" parent="EntityAttribute-entityId-parentBean" />

  <bean id="EntityAttribute-entityId-parentBean" abstract="true" parent="Entity-id">
    <property name="shortLabel" value="Entity" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:businessObjectClass="org.kuali.rice.kim.bo.Entity"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
            p:includeKeyInLabel="true"
            p:keyAttribute="id"
            p:labelAttribute="entityTypeId" />
    </property>
    <property name="name" value="entityId" />
    <property name="label" value="Entity" />
  </bean>
  <bean id="EntityAttribute-attributeTypeId" parent="EntityAttribute-attributeTypeId-parentBean" />

  <bean id="EntityAttribute-attributeTypeId-parentBean" abstract="true" parent="AttributeType-id">
    <property name="shortLabel" value="Type" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:businessObjectClass="org.kuali.rice.kim.bo.AttributeType"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
            p:includeKeyInLabel="true"
            p:keyAttribute="id"
            p:labelAttribute="attributeTypeName" />
    </property>
    <property name="name" value="attributeTypeId" />
    <property name="label" value="Type" />
  </bean>
  <bean id="EntityAttribute-attributeName" parent="EntityAttribute-attributeName-parentBean" />

  <bean id="EntityAttribute-attributeName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="500" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="attributeName" />
    <property name="label" value="Name" />
  </bean>
  <bean id="EntityAttribute-namespaceId" parent="EntityAttribute-namespaceId-parentBean" />

  <bean id="EntityAttribute-namespaceId-parentBean" abstract="true" parent="Namespace-id">
    <property name="shortLabel" value="Namespace" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:businessObjectClass="org.kuali.rice.kim.bo.Namespace"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
            p:includeKeyInLabel="true"
            p:keyAttribute="id"
            p:labelAttribute="name" />
    </property>
    <property name="name" value="namespaceId" />
    <property name="label" value="Namespace" />
  </bean>
  <bean id="EntityAttribute-value" parent="EntityAttribute-value-parentBean" />

  <bean id="EntityAttribute-value-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Value" />
    <property name="maxLength" value="4000" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextareaControlDefinition"
            p:cols="40"
            p:rows="10" />
    </property>
    <property name="name" value="value" />
    <property name="label" value="Value" />
  </bean>
  <bean id="EntityAttribute-versionNumber" parent="EntityAttribute-versionNumber-parentBean" />

  <bean id="EntityAttribute-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
  <bean id="EntityAttribute-objectId" parent="EntityAttribute-objectId-parentBean" />

  <bean id="EntityAttribute-objectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
  </bean>
</beans>
