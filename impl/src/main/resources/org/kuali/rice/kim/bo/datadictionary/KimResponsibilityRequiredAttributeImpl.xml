<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="KimResponsibilityRequiredAttributeImpl" parent="KimResponsibilityRequiredAttributeImpl-parentBean" />

  <bean id="KimResponsibilityRequiredAttributeImpl-parentBean" abstract="true" parent="BusinessObjectEntry"
        p:businessObjectClass="org.kuali.rice.kim.bo.role.impl.KimResponsibilityRequiredAttributeImpl"
        p:titleAttribute="kimResponsibilityRequiredAttributeId"
        p:objectLabel="Kim Attribute Definition"
        p:inquiryDefinition-ref="KimResponsibilityRequiredAttributeImpl-inquiryDefinition" >
    <property name="attributes" >
      <list>
        <ref bean="KimResponsibilityRequiredAttributeImpl-kimResponsibilityRequiredAttributeId" />
        <ref bean="KimResponsibilityRequiredAttributeImpl-responsibilityId" />
        <ref bean="KimResponsibilityRequiredAttributeImpl-kimAttributeId" />
        <ref bean="KimResponsibilityRequiredAttributeImpl-active" />
        <ref bean="KimResponsibilityRequiredAttributeImpl-kimAttribute.attributeName" />
        <ref bean="KimResponsibilityRequiredAttributeImpl-kimAttribute.namespaceCode" />
        <ref bean="KimResponsibilityRequiredAttributeImpl-kimAttribute.attributeLabel" />
        <ref bean="KimResponsibilityRequiredAttributeImpl-kimAttribute.active" />
        <ref bean="KimResponsibilityRequiredAttributeImpl-kimAttribute.componentName" />
        <ref bean="KimResponsibilityRequiredAttributeImpl-kimAttribute.applicationUrl" />
      </list>
    </property>
  </bean>

  <bean id="KimResponsibilityRequiredAttributeImpl-kimResponsibilityRequiredAttributeId" parent="KimResponsibilityRequiredAttributeImpl-kimResponsibilityRequiredAttributeId-parentBean" />
  <bean id="KimResponsibilityRequiredAttributeImpl-kimResponsibilityRequiredAttributeId-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimResponsibilityRequiredAttributeId"
        p:forceUppercase="true"
        p:label="Responsibility Required Attribute Id"
        p:shortLabel="Responsibility Required Attribute Id"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>
    
  <bean id="KimResponsibilityRequiredAttributeImpl-responsibilityId" parent="KimResponsibilityRequiredAttributeImpl-responsibilityId-parentBean" />
  <bean id="KimResponsibilityRequiredAttributeImpl-responsibilityId-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="responsibilityId"
        p:label="Responsibility Id"
        p:shortLabel="Responsibility Id"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="KimResponsibilityRequiredAttributeImpl-kimAttributeId" parent="KimResponsibilityRequiredAttributeImpl-kimAttributeId-parentBean" />
  <bean id="KimResponsibilityRequiredAttributeImpl-kimAttributeId-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttributeId"
        p:forceUppercase="true"
        p:label="Attribute Id"
        p:shortLabel="Attribute Id"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="KimResponsibilityRequiredAttributeImpl-active" parent="KimResponsibilityRequiredAttributeImpl-active-parentBean" />
  <bean id="KimResponsibilityRequiredAttributeImpl-active-parentBean" parent="KimAttributeImpl-active" abstract="true"
  	    p:name="active" 
  	    p:label="Responsibility Required Active Indicator" />

  <bean id="KimResponsibilityRequiredAttributeImpl-kimAttribute.attributeName" parent="KimResponsibilityRequiredAttributeImpl-kimAttribute.attributeName-parentBean" />
  <bean id="KimResponsibilityRequiredAttributeImpl-kimAttribute.attributeName-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttribute.attributeName"
        p:label="Attribute Name"
        p:shortLabel="Attribute Name"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="KimResponsibilityRequiredAttributeImpl-kimAttribute.namespaceCode" parent="KimResponsibilityRequiredAttributeImpl-kimAttribute.namespaceCode-parentBean" />
  <bean id="KimResponsibilityRequiredAttributeImpl-kimAttribute.namespaceCode-parentBean" parent="RoleImpl-namespaceCode" abstract="true"
        p:name="kimAttribute.namespaceCode"
        p:label="Attribute Namespace Code"
        p:shortLabel="Attribute Namespace Code">
  </bean>
  
  <bean id="KimResponsibilityRequiredAttributeImpl-kimAttribute.attributeLabel" parent="KimResponsibilityRequiredAttributeImpl-kimAttribute.attributeLabel-parentBean" />
  <bean id="KimResponsibilityRequiredAttributeImpl-kimAttribute.attributeLabel-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttribute.attributeLabel"
        p:forceUppercase="true"
        p:label="Attribute Label"
        p:shortLabel="Attribute Label"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="KimResponsibilityRequiredAttributeImpl-kimAttribute.active" parent="KimResponsibilityRequiredAttributeImpl-kimAttribute.active-parentBean" />
  <bean id="KimResponsibilityRequiredAttributeImpl-kimAttribute.active-parentBean" parent="KimAttributeImpl-active" abstract="true"
  	    p:name="kimAttribute.active" 
  	    p:label="Attribute Active Indicator"/>
  
  <bean id="KimResponsibilityRequiredAttributeImpl-kimAttribute.componentName" parent="KimResponsibilityRequiredAttributeImpl-kimAttribute.componentName-parentBean" />
  <bean id="KimResponsibilityRequiredAttributeImpl-kimAttribute.componentName-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttribute.componentName"
        p:forceUppercase="true"
        p:label="Component Name"
        p:shortLabel="Component Name"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="KimResponsibilityRequiredAttributeImpl-kimAttribute.applicationUrl" parent="KimResponsibilityRequiredAttributeImpl-kimAttribute.applicationUrl-parentBean" />
  <bean id="KimResponsibilityRequiredAttributeImpl-kimAttribute.applicationUrl-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttribute.applicationUrl"
        p:forceUppercase="true"
        p:label="Application Url"
        p:shortLabel="Application Url"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>  
    
  <bean id="KimResponsibilityRequiredAttributeImpl-inquiryDefinition" parent="KimResponsibilityRequiredAttributeImpl-inquiryDefinition-parentBean" />
  <bean id="KimResponsibilityRequiredAttributeImpl-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition"
        p:title="Kim Attribute Data Inquiry" >
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition"
              p:title="Kim Attribute Data"
              p:numberOfColumns="1" >
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition" p:attributeName="kimResponsibilityRequiredAttributeId" />
              <bean parent="FieldDefinition" p:attributeName="responsibilityId" />
              <bean parent="FieldDefinition" p:attributeName="active" />
              <bean parent="FieldDefinition" p:attributeName="kimAttributeId" />
              <bean parent="FieldDefinition" p:attributeName="kimAttribute.attributeName" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.namespaceCode" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.attributeLabel" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.active" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.componentName" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.applicationUrl" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
  
</beans>