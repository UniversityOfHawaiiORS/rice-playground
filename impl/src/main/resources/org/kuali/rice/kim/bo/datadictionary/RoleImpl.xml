        p:label="Name Space"
        p:shortLabel="Name Space"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="10" />
      <!-- <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.rice.kns.lookup.keyvalues.ParameterNamespaceValuesFinder"
            p:includeKeyInLabel="false" 
            p:keyAttribute="code"
            p:labelAttribute="codeAndDescription" /> -->
