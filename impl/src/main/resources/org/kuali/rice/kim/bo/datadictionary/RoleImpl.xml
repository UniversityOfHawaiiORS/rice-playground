<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RoleImpl" parent="RoleImpl-parentBean" />

  <bean id="RoleImpl-parentBean" abstract="true" parent="BusinessObjectEntry"
        p:businessObjectClass="org.kuali.rice.kim.bo.impl.RoleImpl"
        p:titleAttribute="roleId"
        p:objectLabel="Names"
        p:inquiryDefinition-ref="RoleImpl-inquiryDefinition"
        p:lookupDefinition-ref="RoleImpl-lookupDefinition" >
    <property name="attributes" >
      <list>
        <ref bean="RoleImpl-roleId" />
        <ref bean="RoleImpl-kimTypeId" />
        <ref bean="RoleImpl-roleName" />
        <ref bean="RoleImpl-namespaceCode" />
        <ref bean="RoleImpl-principalName" />
        <ref bean="RoleImpl-groupNamespaceCode" />
        <ref bean="RoleImpl-groupName" />
        <ref bean="RoleImpl-permTmplNamespaceCode" />
        <ref bean="RoleImpl-permTmplName" />
        <ref bean="RoleImpl-permNamespaceCode" />
        <ref bean="RoleImpl-permName" />
        <ref bean="RoleImpl-respNamespaceCode" />
        <ref bean="RoleImpl-respName" />
        <ref bean="RoleImpl-respTmplNamespaceCode" />
        <ref bean="RoleImpl-respTmplName" />
      </list>
    </property>
    
        <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="kimRoleType" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="kimTypeId"
                    p:targetName="kimTypeId" />
            </list>
          </property>
        </bean>
      </list>
    </property>
        
  </bean>
 
 
 
   <bean id="RoleImpl-roleId" parent="RoleImpl-roleId-parentBean" />
  <bean id="RoleImpl-roleId-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="roleId"
        p:forceUppercase="true"
        p:label="Role"
        p:shortLabel="Role"
        p:maxLength="10"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="10" />
    </property>
  </bean>
  
  <bean id="RoleImpl-kimTypeId" parent="RoleImpl-kimTypeId-parentBean" />
  <bean id="RoleImpl-kimTypeId-parentBean" parent="AttributeDefinition" abstract="true"
