<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="KimAddressType" parent="KimAddressType-parentBean" />
  <bean id="KimAddressType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.reference.impl.AddressTypeImpl" />
    <property name="inquiryDefinition" >
      <ref bean="KimAddressType-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="KimAddressType-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Address Type" />
    <property name="attributes" >
      <list>
        <ref bean="KimAddressType-addressTypeCode" />
        <ref bean="KimAddressType-addressTypeName" />
        <ref bean="KimAddressType-active" />
        <ref bean="KimAddressType-displaySortCode" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->

  <bean id="KimAddressType-addressTypeCode" parent="KimAddressType-addressTypeCode-parentBean" />
  <bean id="KimAddressType-addressTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true"/>  
    <property name="name" value="code" />
    <property name="label" value="Address Type Code" />
    <property name="shortLabel" value="Type Code" />
    <property name="maxLength" value="2" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="The address type code." />
    <property name="description" value="The address type code." />
  </bean>

  <bean id="KimAddressType-addressTypeName" parent="KimAddressType-addressTypeName-parentBean" />
  <bean id="KimAddressType-addressTypeName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="name" />
    <property name="label" value="Address Type Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="10" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Descriptive text." />
    <property name="description" value="Descriptive text." />
  </bean>

  <bean id="KimAddressType-active" parent="KimAddressType-active-parentBean" />
  <bean id="KimAddressType-active-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean">
    <property name="name" value="active" />
  </bean>

  <bean id="KimAddressType-displaySortCode" parent="KimAddressType-displaySortCode-parentBean" />
  <bean id="KimAddressType-displaySortCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="displaySortCode" />
    <property name="label" value="Display Sort Code" />
    <property name="shortLabel" value="Sort" />
    <property name="maxLength" value="10" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Descriptive text." />
    <property name="description" value="Descriptive text." />
  </bean>

<!-- Business Object Inquiry Definition -->

  <bean id="KimAddressType-inquiryDefinition" parent="KimAddressType-inquiryDefinition-parentBean" />
  <bean id="KimAddressType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Address Type" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Address Type" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="code" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->

  <bean id="KimAddressType-lookupDefinition" parent="KimAddressType-lookupDefinition-parentBean" />
  <bean id="KimAddressType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Address Type Lookup" />
    <property name="instructions" value="Look up Address Types. Click &quot;Return Results&quot; to  them." />
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="active"
              p:defaultValue="Y" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="displaySortCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
