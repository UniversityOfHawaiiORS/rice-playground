<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="GroupDocumentQualifier" parent="GroupDocumentQualifier-parentBean" />
	<bean id="GroupDocumentQualifier-parentBean" abstract="true" parent="BusinessObjectEntry" 
        p:businessObjectClass="org.kuali.rice.kim.bo.ui.GroupDocumentQualifier"
		p:titleAttribute="attributeDataId"
        p:objectLabel="Group Qualifier">
		<property name="inquiryDefinition" >
			<ref bean="GroupDocumentQualifier-inquiryDefinition" />
		</property>
		<property name="lookupDefinition" >
			<ref bean="GroupDocumentQualifier-lookupDefinition" />
		</property>
		<property name="attributes" >
			<list>
				<ref bean="GroupDocumentQualifier-attributeDataId" />
				<ref bean="GroupDocumentQualifier-targetPrimaryKey" />
				<ref bean="GroupDocumentQualifier-kimTypeId" />
				<ref bean="GroupDocumentQualifier-kimAttributeId" />
				<ref bean="GroupDocumentQualifier-attributeValue" />
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->
	
	<bean id="GroupDocumentQualifier-attributeDataId" parent="GroupDocumentQualifier-attributeDataId-parentBean" />
	<bean id="GroupDocumentQualifier-attributeDataId-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="attributeDataId" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Attribute Data Identifier" />
		<property name="shortLabel" value="Attr Data Id" />
		<property name="maxLength" value="40" />
		<property name="validationPattern" >
			<bean parent="AlphaNumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Attr Data Id" />
		<property name="description" value="Attr Data Id" />
	</bean>

	<bean id="GroupDocumentQualifier-targetPrimaryKey" parent="GroupDocumentQualifier-targetPrimaryKey-parentBean" />
	<bean id="GroupDocumentQualifier-targetPrimaryKey-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="targetPrimaryKey" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Target Primary Key" />
		<property name="shortLabel" value="Target Primary Key" />
		<property name="maxLength" value="40" />
		<property name="validationPattern" >
			<bean parent="AlphaNumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Target Primary Key" />
		<property name="description" value="Target Primary Key" />
	</bean>

	<bean id="GroupDocumentQualifier-kimTypeId" parent="GroupDocumentQualifier-kimTypeId-parentBean" />
	<bean id="GroupDocumentQualifier-kimTypeId-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="kimTypeId" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Kim Type Identifier" />
		<property name="shortLabel" value="Kim Typ Id" />
		<property name="maxLength" value="40" />
		<property name="validationPattern" >
			<bean parent="AlphaNumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Kim Typ Id" />
		<property name="description" value="Kim Typ Id" />
	</bean>

	<bean id="GroupDocumentQualifier-kimAttributeId" parent="GroupDocumentQualifier-kimAttributeId-parentBean" />
	<bean id="GroupDocumentQualifier-kimAttributeId-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="kimAttributeId" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Kim Attribute Definition Identifier" />
		<property name="shortLabel" value="Kim Attr Defn Id" />
		<property name="maxLength" value="40" />
		<property name="validationPattern" >
			<bean parent="AlphaNumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Kim Attr Defn Id" />
		<property name="description" value="Kim Attr Defn Id" />
	</bean>

	<bean id="GroupDocumentQualifier-attributeValue" parent="GroupDocumentQualifier-attributeValue-parentBean" />
	<bean id="GroupDocumentQualifier-attributeValue-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="attributeValue" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Attr Val" />
		<property name="shortLabel" value="Attr Val" />
		<property name="maxLength" value="400" />
		<property name="validationPattern" >
			<bean parent="AnyCharacterValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Attr Val" />
		<property name="description" value="Attr Val" />
	</bean>

	<!-- Business Object Inquiry Definition -->
	<bean id="GroupDocumentQualifier-inquiryDefinition" parent="GroupDocumentQualifier-inquiryDefinition-parentBean" />
	<bean id="GroupDocumentQualifier-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
		<property name="title" value="Attribute Data Identifier" />
		<property name="inquirySections" >
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="title" value="Attribute Data Identifier" />
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields" >
						<list>
							<bean parent="FieldDefinition"
								p:attributeName="attributeDataId" 
								p:forceInquiry="true" />
							<bean parent="FieldDefinition"
								p:attributeName="targetPrimaryKey" />
							<bean parent="FieldDefinition"
								p:attributeName="kimTypeId" />
							<bean parent="FieldDefinition"
								p:attributeName="kimAttributeId" />
							<bean parent="FieldDefinition"
								p:attributeName="attributeValue" />
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>

	<!-- Business Object Lookup Definition -->
	<bean id="GroupDocumentQualifier-lookupDefinition" parent="GroupDocumentQualifier-lookupDefinition-parentBean" />
	<bean id="GroupDocumentQualifier-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
		<property name="title" value="Doc Group Qualifier Lookup" />
		<property name="instructions" value="Look up a Doc Group Qualifier" />
		<property name="lookupFields" >
			<list>
				<bean parent="FieldDefinition"
					p:attributeName="attributeDataId" 
					p:forceInquiry="true" />
				<bean parent="FieldDefinition"
					p:attributeName="targetPrimaryKey" />
				<bean parent="FieldDefinition"
					p:attributeName="kimTypeId" />
				<bean parent="FieldDefinition"
					p:attributeName="kimAttributeId" />
				<bean parent="FieldDefinition"
					p:attributeName="attributeValue" />
			</list>
		</property>
		<property name="resultFields" >
			<list>
				<bean parent="FieldDefinition"
					p:attributeName="attributeDataId" 
					p:forceInquiry="true" />
				<bean parent="FieldDefinition"
					p:attributeName="targetPrimaryKey" />
				<bean parent="FieldDefinition"
					p:attributeName="kimTypeId" />
				<bean parent="FieldDefinition"
					p:attributeName="kimAttributeId" />
				<bean parent="FieldDefinition"
					p:attributeName="attributeValue" />
			</list>
		</property>
	</bean>
</beans>
