<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="DocRoleMember" parent="DocRoleMember-parentBean" />

  <bean id="DocRoleMember-parentBean" abstract="true" parent="BusinessObjectEntry"
        p:businessObjectClass="org.kuali.rice.kim.bo.ui.KimDocumentRoleMember"
        p:titleAttribute="roleMemberId"
        p:objectLabel="Names"
        p:inquiryDefinition-ref="DocRoleMember-inquiryDefinition"
        p:lookupDefinition-ref="DocRoleMember-lookupDefinition" >
    <property name="attributes" >
      <list>
        <ref bean="DocRoleMember-roleMemberId" />
        <ref bean="DocRoleMember-memberId" />
        <!-- TODO: create entry for this  -->
        <ref bean="DocRoleMember-memberTypeCode" />
        <ref bean="DocRoleMember-memberName" />
        <ref bean="DocRoleMember-activeFromDate" />
        <ref bean="DocRoleMember-activeToDate" />
      </list>
    </property>
    
  </bean>
 
   <bean id="DocRoleMember-roleMemberId" parent="DocRoleMember-roleMemberId-parentBean" />
  <bean id="DocRoleMember-roleMemberId-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="roleMemberId"
        p:forceUppercase="true"
        p:label="Role Member Identifier"
        p:shortLabel="Role Member Identifier"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="10" />
    </property>
  </bean>
  
  <bean id="DocRoleMember-memberId" parent="DocRoleMember-memberId-parentBean" />
  <bean id="DocRoleMember-memberId-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="memberId"
        p:forceUppercase="true"
        p:label="Member Identifier"
        p:shortLabel="Member Identifier"
        p:maxLength="40"
        p:required="true" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="10" />
    </property>
  </bean>
    
  <bean id="DocRoleMember-memberTypeCode" parent="DocRoleMember-memberTypeCode-parentBean" />
  <bean id="DocRoleMember-memberTypeCode-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="memberTypeCode"
        p:forceUppercase="true"
        p:label="Member Type Code"
        p:shortLabel="Type Code"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="SelectControlDefinition" p:valuesFinderClass="org.kuali.rice.kim.bo.options.MemberTypeValuesFinder"/>
    </property>
  </bean>

  <bean id="DocRoleMember-memberName" parent="DocRoleMember-memberName-parentBean" />
  <bean id="DocRoleMember-memberName-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="memberName"
        p:forceUppercase="true"
        p:label="Member Name"
        p:shortLabel="Name"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="10" />
    </property>
  </bean>
            
  <bean id="DocRoleMember-activeFromDate" parent="DocRoleMember-activeFromDate-parentBean" />
  <bean id="DocRoleMember-activeFromDate-parentBean" abstract="true" parent="AttributeReferenceDummy-genericDate">
    <property name="name" value="activeFromDate" />
    <property name="label" value="Active From Date" />
    <property name="shortLabel" value="Active From Dt" />
    <property name="required" value="false" />
    <property name="summary" value="Start Date" />
    <property name="description" value="Start Date" />
  </bean>

  <bean id="DocRoleMember-activeToDate" parent="DocRoleMember-activeToDate-parentBean" />
  <bean id="DocRoleMember-activeToDate-parentBean" abstract="true" parent="AttributeReferenceDummy-genericDate">
    <property name="name" value="activeToDate" />
    <property name="label" value="Active To Date" />
    <property name="shortLabel" value="Active To Dt" />
    <property name="required" value="false" />
    <property name="summary" value="End Date" />
    <property name="description" value="End Date" />
  </bean>



  
  <bean id="DocRoleMember-inquiryDefinition" parent="DocRoleMember-inquiryDefinition-parentBean" />

  <bean id="DocRoleMember-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition"
        p:title="Person Group Inquiry" >
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition"
              p:title="Person Attributes"
              p:numberOfColumns="1" >
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition" p:attributeName="roleMemberId" />
              <bean parent="FieldDefinition" p:attributeName="memberId" />
              <bean parent="FieldDefinition" p:attributeName="activeFromDate" />
              <bean parent="FieldDefinition" p:attributeName="activeToDate" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

  <bean id="DocRoleMember-lookupDefinition" parent="DocRoleMember-lookupDefinition-parentBean" />

  <bean id="DocRoleMember-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition"
        p:title="Role Member Lookup" 
        p:instructions="Lookup a Role Member" >
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>roleMemberId</value>
          </list>
        </property>
        <property name="sortAscending" value="true" />
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
              <bean parent="FieldDefinition" p:attributeName="roleMemberId" />
              <bean parent="FieldDefinition" p:attributeName="memberId" />
              <bean parent="FieldDefinition" p:attributeName="activeFromDate" />
              <bean parent="FieldDefinition" p:attributeName="activeToDate" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
              <bean parent="FieldDefinition" p:attributeName="roleMemberId" />
              <bean parent="FieldDefinition" p:attributeName="memberId" />
              <bean parent="FieldDefinition" p:attributeName="activeFromDate" />
              <bean parent="FieldDefinition" p:attributeName="activeToDate" />
      </list>
    </property>
  </bean>

</beans>
