<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="KimResponsibilityAttributeDataImpl" parent="KimResponsibilityAttributeDataImpl-parentBean" />

  <bean id="KimResponsibilityAttributeDataImpl-parentBean" abstract="true" parent="BusinessObjectEntry"
        p:businessObjectClass="org.kuali.rice.kim.bo.role.impl.ResponsibilityAttributeDataImpl"
        p:titleAttribute="attributeDataId"
        p:objectLabel="Responsibility Attribute Data"
        p:inquiryDefinition-ref="KimResponsibilityAttributeDataImpl-inquiryDefinition">
	<property name="attributes" >
      <list>
        <ref bean="KimResponsibilityAttributeDataImpl-kimAttributeId" />
        <ref bean="KimResponsibilityAttributeDataImpl-attributeName" />
        <ref bean="KimResponsibilityAttributeDataImpl-attributeValue" />
        <ref bean="KimResponsibilityAttributeDataImpl-namespaceCode" />
        <ref bean="KimResponsibilityAttributeDataImpl-attributeLabel" />
        <ref bean="KimResponsibilityAttributeDataImpl-active" />
        <ref bean="KimResponsibilityAttributeDataImpl-componentName" />
        <ref bean="KimResponsibilityAttributeDataImpl-applicationUrl" />
      </list>
    </property>
  </bean>

  <bean id="KimResponsibilityAttributeDataImpl-kimAttributeId" parent="KimResponsibilityAttributeDataImpl-kimAttributeId-parentBean" />
  <bean id="KimResponsibilityAttributeDataImpl-kimAttributeId-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttributeId"
        p:forceUppercase="true"
        p:label="Attribute Id"
        p:shortLabel="Attribute Id"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>
    
  <bean id="KimResponsibilityAttributeDataImpl-attributeName" parent="KimResponsibilityAttributeDataImpl-attributeName-parentBean" />
  <bean id="KimResponsibilityAttributeDataImpl-attributeName-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttribute.attributeName"
        p:label="Attribute Name"
        p:shortLabel="Attribute Name"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="KimResponsibilityAttributeDataImpl-attributeValue" parent="KimResponsibilityAttributeDataImpl-attributeValue-parentBean" />
  <bean id="KimResponsibilityAttributeDataImpl-attributeValue-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="attributeValue"
        p:label="Attribute Value"
        p:shortLabel="Attribute Value"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="KimResponsibilityAttributeDataImpl-namespaceCode" parent="KimResponsibilityAttributeDataImpl-namespaceCode-parentBean" />
  <bean id="KimResponsibilityAttributeDataImpl-namespaceCode-parentBean" parent="KimRoleImpl-namespaceCode" abstract="true"
        p:name="kimAttribute.namespaceCode"
        p:label="Attribute Namespace Code"
        p:shortLabel="Attribute Namespace Code">
  </bean>
  
  <bean id="KimResponsibilityAttributeDataImpl-attributeLabel" parent="KimResponsibilityAttributeDataImpl-attributeLabel-parentBean" />
  <bean id="KimResponsibilityAttributeDataImpl-attributeLabel-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttribute.attributeLabel"
        p:forceUppercase="true"
        p:label="Attribute Label"
        p:shortLabel="Attribute Label"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="KimResponsibilityAttributeDataImpl-active" parent="KimResponsibilityAttributeDataImpl-active-parentBean" />
  <bean id="KimResponsibilityAttributeDataImpl-active-parentBean" parent="KimAttributeImpl-active" abstract="true"
  	    p:name="kimAttribute.active" />
  
  <bean id="KimResponsibilityAttributeDataImpl-componentName" parent="KimResponsibilityAttributeDataImpl-componentName-parentBean" />
  <bean id="KimResponsibilityAttributeDataImpl-componentName-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttribute.componentName"
        p:forceUppercase="true"
        p:label="Component Name"
        p:shortLabel="Component Name"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="KimResponsibilityAttributeDataImpl-applicationUrl" parent="KimResponsibilityAttributeDataImpl-applicationUrl-parentBean" />
  <bean id="KimResponsibilityAttributeDataImpl-applicationUrl-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttribute.applicationUrl"
        p:forceUppercase="true"
        p:label="Application Url"
        p:shortLabel="Application Url"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>  
  
  <bean id="KimResponsibilityAttributeDataImpl-inquiryDefinition" parent="KimResponsibilityAttributeDataImpl-inquiryDefinition-parentBean" />
  <bean id="KimResponsibilityAttributeDataImpl-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition"
        p:title="Kim Attribute Data Inquiry" >
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition"
              p:title="Kim Attribute Data"
              p:numberOfColumns="1" >
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition" p:attributeName="kimAttributeId" />
              <bean parent="FieldDefinition" p:attributeName="kimAttribute.attributeName" />
        	  <bean parent="FieldDefinition" p:attributeName="attributeValue" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.namespaceCode" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.attributeLabel" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.active" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.componentName" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.applicationUrl" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

</beans>