<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="Permission" parent="Permission-parentBean" />

  <bean id="Permission-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.Permission" />
     <property name="lookupDefinition" >
      <ref bean="Permission-lookupDefinition" />
    </property>
    <property name="inquiryDefinition" >
      <ref bean="Permission-inquiryDefinition" />
    </property>
    <property name="titleAttribute" value="name" />
    <property name="objectLabel" value="Permission" />
    <property name="attributes" >
      <list>
        <ref bean="Permission-id" />
        <ref bean="Permission-name" />
        <ref bean="Permission-description" />
        <ref bean="Permission-namespaceId" />
        <ref bean="Permission-versionNumber" />
        <ref bean="Permission-objectId" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->
  <bean id="Permission-id" parent="Permission-id-parentBean" />

  <bean id="Permission-id-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Permission Id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="name" value="id" />
    <property name="label" value="Id" />
  </bean>
  <bean id="Permission-name" parent="Permission-name-parentBean" />

  <bean id="Permission-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Permission Name" />
    <property name="maxLength" value="500" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="name" />
    <property name="label" value="Name" />
  </bean>
  <bean id="Permission-description" parent="Permission-description-parentBean" />

  <bean id="Permission-description-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Permission Description" />
    <property name="maxLength" value="4000" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextareaControlDefinition"
            p:cols="40"
            p:rows="10" />
    </property>
    <property name="name" value="description" />
    <property name="label" value="Description" />
  </bean>
  <bean id="Permission-namespaceId" parent="Permission-namespaceId-parentBean" />

  <bean id="Permission-namespaceId-parentBean" abstract="true" parent="Namespace-id">
    <property name="shortLabel" value="Namespace" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:businessObjectClass="org.kuali.rice.kim.bo.Namespace"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
            p:includeKeyInLabel="true"
            p:keyAttribute="id"
            p:labelAttribute="name" />
    </property>
    <property name="name" value="namespaceId" />
    <property name="label" value="Namespace" />
  </bean>
  <bean id="Permission-versionNumber" parent="Permission-versionNumber-parentBean" />

  <bean id="Permission-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
  <bean id="Permission-objectId" parent="Permission-objectId-parentBean" />

  <bean id="Permission-objectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
  </bean>
  
  <!-- Business Object Inquiry Definition -->
  <bean id="Permission-inquiryDefinition" parent="Permission-inquiryDefinition-parentBean" />

  <bean id="Permission-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="id" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
              <bean parent="FieldDefinition"
                    p:attributeName="description" />
            </list>
          </property>
          <property name="title" value="Permission Info" />
        </bean>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="namespace.id" />
              <bean parent="FieldDefinition"
                    p:attributeName="namespace.name" />
              <bean parent="FieldDefinition"
                    p:attributeName="namespace.description" />
            </list>
          </property>
          <property name="title" value="Associated Namespace" />
        </bean>
      </list>
    </property>
    <property name="title" value="Permission Inquiry" />
  </bean>

  <!-- Business Object Lookup Definition -->

  <bean id="Permission-lookupDefinition" parent="Permission-lookupDefinition-parentBean" />

  <bean id="Permission-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="instructions" value="Lookup a Permission" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="id" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="id" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
      </list>
    </property>
    <property name="title" value="Permission Lookup" />
    <property name="menubar" value="&lt;a href=&quot;../kim/index.jsp&quot;&gt;KIM Home&lt;/a&gt;   &lt;a href=&quot;../index.html&quot;&gt;Main&lt;/a&gt;" />
  </bean>
  
  
  
  
  
  
  
</beans>
