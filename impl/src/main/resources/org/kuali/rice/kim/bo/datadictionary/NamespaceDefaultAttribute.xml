<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="NamespaceDefaultAttribute" parent="NamespaceDefaultAttribute-parentBean" />

  <bean id="NamespaceDefaultAttribute-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.NamespaceDefaultAttribute" />
    <property name="objectLabel" value="Namespace Default Attribute" />
    <property name="attributes" >
      <list>
        <ref bean="NamespaceDefaultAttribute-id" />
        <ref bean="NamespaceDefaultAttribute-attributeName" />
        <ref bean="NamespaceDefaultAttribute-description" />
        <ref bean="NamespaceDefaultAttribute-attributeType.attributeTypeName" />
        <ref bean="NamespaceDefaultAttribute-namespace.name" />
        <ref bean="NamespaceDefaultAttribute-required" />
        <ref bean="NamespaceDefaultAttribute-active" />
        <ref bean="NamespaceDefaultAttribute-attributeTypeId" />
        <ref bean="NamespaceDefaultAttribute-namespaceId" />
        <ref bean="NamespaceDefaultAttribute-versionNumber" />
        <ref bean="NamespaceDefaultAttribute-objectId" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="NamespaceDefaultAttribute-id" parent="NamespaceDefaultAttribute-id-parentBean" />

  <bean id="NamespaceDefaultAttribute-id-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="name" value="id" />
    <property name="label" value="Id" />
  </bean>
  <bean id="NamespaceDefaultAttribute-attributeName" parent="NamespaceDefaultAttribute-attributeName-parentBean" />

  <bean id="NamespaceDefaultAttribute-attributeName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="500" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="attributeName" />
    <property name="label" value="Name" />
  </bean>
  <bean id="NamespaceDefaultAttribute-description" parent="NamespaceDefaultAttribute-description-parentBean" />

  <bean id="NamespaceDefaultAttribute-description-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="4000" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="description" />
    <property name="label" value="Description" />
  </bean>
  <bean id="NamespaceDefaultAttribute-attributeType.attributeTypeName" parent="NamespaceDefaultAttribute-attributeType.attributeTypeName-parentBean" />

  <bean id="NamespaceDefaultAttribute-attributeType.attributeTypeName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Type" />
    <property name="maxLength" value="4000" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="attributeType.attributeTypeName" />
    <property name="label" value="Type" />
  </bean>
  <bean id="NamespaceDefaultAttribute-namespace.name" parent="NamespaceDefaultAttribute-namespace.name-parentBean" />

  <bean id="NamespaceDefaultAttribute-namespace.name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Namespace" />
    <property name="maxLength" value="4000" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="namespace.name" />
    <property name="label" value="Namespace" />
  </bean>
  <bean id="NamespaceDefaultAttribute-required" parent="NamespaceDefaultAttribute-required-parentBean" />

  <bean id="NamespaceDefaultAttribute-required-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Required" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="name" value="required" />
    <property name="label" value="Required" />
  </bean>
  <bean id="NamespaceDefaultAttribute-active" parent="NamespaceDefaultAttribute-active-parentBean" />

  <bean id="NamespaceDefaultAttribute-active-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Active" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="name" value="active" />
    <property name="label" value="Active" />
  </bean>
  <bean id="NamespaceDefaultAttribute-attributeTypeId" parent="NamespaceDefaultAttribute-attributeTypeId-parentBean" />

  <bean id="NamespaceDefaultAttribute-attributeTypeId-parentBean" abstract="true" parent="AttributeType-id">
    <property name="shortLabel" value="Type" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:businessObjectClass="org.kuali.rice.kim.bo.AttributeType"
            p:valuesFinderClass="org.kuali.rice.kns.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
            p:includeKeyInLabel="true"
            p:keyAttribute="id"
            p:labelAttribute="attributeTypeName" />
    </property>
    <property name="name" value="attributeTypeId" />
    <property name="label" value="Type" />
  </bean>
  <bean id="NamespaceDefaultAttribute-namespaceId" parent="NamespaceDefaultAttribute-namespaceId-parentBean" />

  <bean id="NamespaceDefaultAttribute-namespaceId-parentBean" abstract="true" parent="Namespace-id">
    <property name="shortLabel" value="Group" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:businessObjectClass="org.kuali.rice.kim.bo.Namespace"
            p:valuesFinderClass="org.kuali.rice.kns.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
            p:includeKeyInLabel="true"
            p:keyAttribute="id"
            p:labelAttribute="name" />
    </property>
    <property name="name" value="namespaceId" />
    <property name="label" value="Associated Group" />
  </bean>
  <bean id="NamespaceDefaultAttribute-versionNumber" parent="NamespaceDefaultAttribute-versionNumber-parentBean" />

  <bean id="NamespaceDefaultAttribute-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
  <bean id="NamespaceDefaultAttribute-objectId" parent="NamespaceDefaultAttribute-objectId-parentBean" />

  <bean id="NamespaceDefaultAttribute-objectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
  </bean>
</beans>
