<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ResponsibilityAttributeDataImpl" parent="ResponsibilityAttributeDataImpl-parentBean" />

  <bean id="ResponsibilityAttributeDataImpl-parentBean" abstract="true" parent="BusinessObjectEntry"
        p:businessObjectClass="org.kuali.rice.kim.bo.role.impl.ResponsibilityAttributeDataImpl"
        p:titleAttribute="attributeDataId"
        p:objectLabel="Attribute Data"
        p:inquiryDefinition-ref="ResponsibilityAttributeDataImpl-inquiryDefinition">
    <property name="attributes" >
      <list>
        <ref bean="ResponsibilityAttributeDataImpl-kimAttributeId" />
        <ref bean="ResponsibilityAttributeDataImpl-attributeName" />
        <ref bean="ResponsibilityAttributeDataImpl-attributeValue" />
        <ref bean="ResponsibilityAttributeDataImpl-namespaceCode" />
        <ref bean="ResponsibilityAttributeDataImpl-attributeLabel" />
        <ref bean="ResponsibilityAttributeDataImpl-active" />
        <ref bean="ResponsibilityAttributeDataImpl-componentName" />
        <ref bean="ResponsibilityAttributeDataImpl-applicationUrl" />
      </list>
    </property>
  </bean>

  <bean id="ResponsibilityAttributeDataImpl-kimAttributeId" parent="ResponsibilityAttributeDataImpl-kimAttributeId-parentBean" />
  <bean id="ResponsibilityAttributeDataImpl-kimAttributeId-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttributeId"
        p:forceUppercase="true"
        p:label="Attribute Id"
        p:shortLabel="Attribute Id"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>
    
  <bean id="ResponsibilityAttributeDataImpl-attributeName" parent="ResponsibilityAttributeDataImpl-attributeName-parentBean" />
  <bean id="ResponsibilityAttributeDataImpl-attributeName-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttribute.attributeName"
        p:label="Attribute Name"
        p:shortLabel="Attribute Name"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="ResponsibilityAttributeDataImpl-attributeValue" parent="ResponsibilityAttributeDataImpl-attributeValue-parentBean" />
  <bean id="ResponsibilityAttributeDataImpl-attributeValue-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="attributeValue"
        p:label="Attribute Value"
        p:shortLabel="Attribute Value"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="ResponsibilityAttributeDataImpl-namespaceCode" parent="ResponsibilityAttributeDataImpl-namespaceCode-parentBean" />
  <bean id="ResponsibilityAttributeDataImpl-namespaceCode-parentBean" parent="RoleImpl-namespaceCode" abstract="true"
        p:name="kimAttribute.namespaceCode"
        p:label="Attribute Namespace Code"
        p:shortLabel="Attribute Namespace Code">
  </bean>
  
  <bean id="ResponsibilityAttributeDataImpl-attributeLabel" parent="ResponsibilityAttributeDataImpl-attributeLabel-parentBean" />
  <bean id="ResponsibilityAttributeDataImpl-attributeLabel-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttribute.attributeLabel"
        p:forceUppercase="true"
        p:label="Attribute Label"
        p:shortLabel="Attribute Label"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="ResponsibilityAttributeDataImpl-active" parent="ResponsibilityAttributeDataImpl-active-parentBean" />
  <bean id="ResponsibilityAttributeDataImpl-active-parentBean" parent="AttributeReferenceDummy-activeIndicator" abstract="true"
  	    p:name="kimAttribute.active" />
  
  <bean id="ResponsibilityAttributeDataImpl-componentName" parent="ResponsibilityAttributeDataImpl-componentName-parentBean" />
  <bean id="ResponsibilityAttributeDataImpl-componentName-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttribute.componentName"
        p:forceUppercase="true"
        p:label="Component Name"
        p:shortLabel="Component Name"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="ResponsibilityAttributeDataImpl-applicationUrl" parent="ResponsibilityAttributeDataImpl-applicationUrl-parentBean" />
  <bean id="ResponsibilityAttributeDataImpl-applicationUrl-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttribute.applicationUrl"
        p:forceUppercase="true"
        p:label="Application Url"
        p:shortLabel="Application Url"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>  
  
  <bean id="ResponsibilityAttributeDataImpl-inquiryDefinition" parent="ResponsibilityAttributeDataImpl-inquiryDefinition-parentBean" />
  <bean id="ResponsibilityAttributeDataImpl-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition"
        p:title="Kim Attribute Data Inquiry" >
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition"
              p:title="Kim Attribute Data"
              p:numberOfColumns="1" >
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition" p:attributeName="kimAttributeId" />
              <bean parent="FieldDefinition" p:attributeName="kimAttribute.attributeName" />
        	  <bean parent="FieldDefinition" p:attributeName="attributeValue" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.namespaceCode" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.attributeLabel" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.active" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.componentName" />
        	  <bean parent="FieldDefinition" p:attributeName="kimAttribute.applicationUrl" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

</beans>