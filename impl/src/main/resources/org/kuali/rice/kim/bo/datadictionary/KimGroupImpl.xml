<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="KimGroupImpl" parent="KimGroupImpl-parentBean" />

  <bean id="KimGroupImpl-parentBean" abstract="true" parent="BusinessObjectEntry"
        p:businessObjectClass="org.kuali.rice.kim.bo.group.impl.KimGroupImpl"
        p:titleAttribute="groupId"
        p:objectLabel="Group"
        p:inquiryDefinition-ref="KimGroupImpl-inquiryDefinition"
        p:lookupDefinition-ref="KimGroupImpl-lookupDefinition" >

    <property name="attributes" >
      <list>
        <ref bean="KimGroupImpl-groupId" />
        <ref bean="KimGroupImpl-groupName" />
        <ref bean="KimGroupImpl-namespaceCode" />
        <ref bean="KimGroupImpl-kimTypeId" />
        <ref bean="KimGroupImpl-kimTypeName" />
        <ref bean="KimGroupImpl-kimTypeNamespaceCode" />
      </list>
    </property>
    <property name="collections" >
      <list>
        <bean parent="CollectionDefinition"
              p:name="members"
              p:label="Assigned Principals"
              p:shortLabel="Assigned Principals"
              p:summary="Assigned Principals"
              p:description="Assigned Principals" />
        <bean parent="CollectionDefinition"
              p:name="memberPersons"
              p:label="Assigned Principals"
              p:shortLabel="Assigned Principals"
              p:summary="Assigned Principals"
              p:description="Assigned Principals" />
        <bean parent="CollectionDefinition"
              p:name="memberGroups"
              p:label="Assigned Groups"
              p:shortLabel="Assigned Groups"
              p:summary="Assigned Groups"
              p:description="Assigned Groups" />
      </list>
    </property>
    <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="kimTypeImpl"/>
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="kimTypeId"
                    p:targetName="kimTypeId" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="kimTypeImpl.name"
                    p:targetName="name" />
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="kimTypeImpl.namespaceCode"
                    p:targetName="namespaceCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

  <bean id="KimGroupImpl-groupId" parent="KimGroupImpl-groupId-parentBean" />
  <bean id="KimGroupImpl-groupId-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="groupId"
        p:forceUppercase="true"
        p:label="Group Id"
        p:shortLabel="Group Id"
        p:maxLength="10"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="10" />
    </property>
  </bean>

  <bean id="KimGroupImpl-kimTypeId" parent="KimGroupImpl-kimTypeId-parentBean" />
  <bean id="KimGroupImpl-kimTypeId-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimTypeId"
        p:forceUppercase="true"
        p:label="Group Type"
        p:shortLabel="Group Type"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="10" />
    </property>
  </bean>

  <bean id="KimGroupImpl-kimTypeName" parent="KimGroupImpl-kimTypeName-parentBean" />
  <bean id="KimGroupImpl-kimTypeName-parentBean" abstract="true" parent="KimTypeImpl-name">
    <property name="name" value="kimTypeImpl.name" />
    <property name="label" value="Group Type Name" />
  </bean>

  <bean id="KimGroupImpl-kimTypeNamespaceCode" parent="KimGroupImpl-kimTypeNamespaceCode-parentBean" />
  <bean id="KimGroupImpl-kimTypeNamespaceCode-parentBean" abstract="true" parent="KimTypeImpl-namespaceCode">
    <property name="name" value="kimTypeImpl.namespaceCode" />
    <property name="label" value="Group Type Name Space" />
  </bean>

  <bean id="KimGroupImpl-groupName" parent="KimGroupImpl-groupName-parentBean" />
  <bean id="KimGroupImpl-groupName-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="groupName"
        p:forceUppercase="false"
        p:label="Group Name"
        p:shortLabel="Group Name"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="10" />
    </property>
  </bean>
<!--
  <bean id="KimGroupImpl-principalName" parent="KimGroupImpl-principalName-parentBean" />
  <bean id="KimGroupImpl-principalName-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="members.groupMemberId"
        p:forceUppercase="true"
        p:label="Principal Name"
        p:shortLabel="Principal Name"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="10" />
    </property>
  </bean>
-->
  <bean id="KimGroupImpl-namespaceCode" parent="KimGroupImpl-namespaceCode-parentBean" />
  <bean id="KimGroupImpl-namespaceCode-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="namespaceCode"
        p:forceUppercase="true"
        p:label="Name Space"
        p:shortLabel="Name Space"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="10" />
    </property>
  </bean>

  <bean id="KimGroupImpl-inquiryDefinition" parent="KimGroupImpl-inquiryDefinition-parentBean" />

  <bean id="KimGroupImpl-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Group" />
    <property name="inquirableClass" value="org.kuali.rice.kim.inquiry.GroupInquirable"/>
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition"
              p:title="Group"
              p:numberOfColumns="1" >
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition" p:attributeName="groupId" />
              <bean parent="FieldDefinition" p:attributeName="namespaceCode" />
              <bean parent="FieldDefinition" p:attributeName="groupName" />
              <!-- <bean parent="FieldDefinition" p:attributeName="kimTypeId" />-->
              <bean parent="FieldDefinition" p:attributeName="kimTypeImpl.namespaceCode" p:noInquiry="true" />
		      <bean parent="FieldDefinition" p:attributeName="kimTypeImpl.name" />
            </list>
          </property>
        </bean>
		<bean parent="InquirySectionDefinition">
          <property name="title" value="Assigned Groups"/>
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="InquiryCollectionDefinition">
                <property name="attributeName" value="memberGroups" />
                <property name="businessObjectClass" value="org.kuali.rice.kim.bo.group.impl.KimGroupImpl" />
                <property name="numberOfColumns" value="1" />
                <property name="inquiryFields" >
                  <list>
                      <bean parent="FieldDefinition" p:attributeName="groupId" />
				      <bean parent="FieldDefinition" p:attributeName="namespaceCode" />
				      <bean parent="FieldDefinition" p:attributeName="groupName" />
				      <!-- <bean parent="FieldDefinition" p:attributeName="kimTypeId" />-->
				      <bean parent="FieldDefinition" p:attributeName="kimTypeImpl.namespaceCode" p:noInquiry="true" />
				      <bean parent="FieldDefinition" p:attributeName="kimTypeImpl.name" />
                  </list>
                </property>
                <property name="summaryFields" >
                  <list>
                      <bean parent="FieldDefinition" p:attributeName="namespaceCode" />
				      <bean parent="FieldDefinition" p:attributeName="groupName" />
                  </list>
                </property>
              </bean>
            </list>
          </property>
        </bean>

        <bean parent="InquirySectionDefinition">
          <property name="title" value="Assigned Principals"/>
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="InquiryCollectionDefinition">
                <property name="attributeName" value="memberPersons" />
                <property name="businessObjectClass" value="org.kuali.rice.kim.bo.impl.PersonImpl" />
                <property name="numberOfColumns" value="1" />
                <property name="inquiryFields" >
                  <list>
                      <bean parent="FieldDefinition" p:attributeName="principalName" />
                      <bean parent="FieldDefinition" p:attributeName="name" />
		              <bean parent="FieldDefinition" p:attributeName="firstName" />
		              <bean parent="FieldDefinition" p:attributeName="middleName" />
		              <bean parent="FieldDefinition" p:attributeName="lastName" />
		              <bean parent="FieldDefinition" p:attributeName="principalId" />
                  </list>
                </property>
                <property name="summaryFields" >
                  <list>
                      <bean parent="FieldDefinition" p:attributeName="principalName" />
                  </list>
                </property>
              </bean>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

  <bean id="KimGroupImpl-lookupDefinition" parent="KimGroupImpl-lookupDefinition-parentBean" />

  <bean id="KimGroupImpl-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition"
        p:title="Group Lookup"
        p:instructions="Lookup a Group" >
    <property name="lookupableID" value="groupLookupable" />

    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>groupId</value>
          </list>
        </property>
        <property name="sortAscending" value="true" />
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition" p:attributeName="groupId" />
        <bean parent="FieldDefinition" p:attributeName="groupName" />
        <bean parent="FieldDefinition" p:attributeName="kimTypeId" />
        <bean parent="FieldDefinition" p:attributeName="namespaceCode" />
        <!--
             Note that kimTypeId and PrincipalName fields
             are overwritten/created in GroupLookupableHelperServiceImpl
        -->
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition" p:attributeName="groupId" />
        <bean parent="FieldDefinition" p:attributeName="namespaceCode" />
        <bean parent="FieldDefinition" p:attributeName="groupName" />
        <bean parent="FieldDefinition" p:attributeName="kimTypeImpl.name" />
      <!--  <bean parent="FieldDefinition" p:attributeName="principalName" /> -->
      </list>
    </property>
  </bean>

</beans>
