<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="KimAttributeImpl" parent="KimAttributeImpl-parentBean" />

  <bean id="KimAttributeImpl-parentBean" abstract="true" parent="BusinessObjectEntry"
        p:businessObjectClass="org.kuali.rice.kim.bo.types.impl.KimAttributeImpl"
        p:titleAttribute="kimAttributeId"
        p:objectLabel="Kim Attribute Definition">
    <property name="attributes" >
      <list>
        <ref bean="KimAttributeImpl-kimAttributeId" />
        <ref bean="KimAttributeImpl-attributeName" />
        <ref bean="KimAttributeImpl-namespaceCode" />
        <ref bean="KimAttributeImpl-attributeLabel" />
        <ref bean="KimAttributeImpl-active" />
        <ref bean="KimAttributeImpl-componentName" />
        <ref bean="KimAttributeImpl-applicationUrl" />
      </list>
    </property>
  </bean>

  <bean id="KimAttributeImpl-kimAttributeId" parent="KimAttributeImpl-kimAttributeId-parentBean" />
  <bean id="KimAttributeImpl-kimAttributeId-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="kimAttributeId"
        p:forceUppercase="true"
        p:label="Attribute Id"
        p:shortLabel="Attribute Id"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>
    
  <bean id="KimAttributeImpl-attributeName" parent="KimAttributeImpl-attributeName-parentBean" />
  <bean id="KimAttributeImpl-attributeName-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="attributeName"
        p:label="Attribute Name"
        p:shortLabel="Attribute Name"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="KimAttributeImpl-namespaceCode" parent="KimAttributeImpl-namespaceCode-parentBean" />
  <bean id="KimAttributeImpl-namespaceCode-parentBean" parent="KimRoleImpl-namespaceCode" abstract="true"
        p:name="namespaceCode"
        p:label="Attribute Namespace Code"
        p:shortLabel="Attribute Namespace Code">
  </bean>
  
  <bean id="KimAttributeImpl-attributeLabel" parent="KimAttributeImpl-attributeLabel-parentBean" />
  <bean id="KimAttributeImpl-attributeLabel-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="attributeLabel"
        p:forceUppercase="true"
        p:label="Attribute Label"
        p:shortLabel="Attribute Label"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="KimAttributeImpl-active" parent="KimAttributeImpl-active-parentBean" />
  <bean id="KimAttributeImpl-active-parentBean" parent="AttributeReferenceDummy-genericBoolean" abstract="true" 
	  p:name="active"
	  p:label="Active Indicator"
	  p:shortLabel="Active Ind" />
  
  <bean id="KimAttributeImpl-componentName" parent="KimAttributeImpl-componentName-parentBean" />
  <bean id="KimAttributeImpl-componentName-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="componentName"
        p:forceUppercase="true"
        p:label="Component Name"
        p:shortLabel="Component Name"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>

  <bean id="KimAttributeImpl-applicationUrl" parent="KimAttributeImpl-applicationUrl-parentBean" />
  <bean id="KimAttributeImpl-applicationUrl-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="applicationUrl"
        p:forceUppercase="true"
        p:label="Application Url"
        p:shortLabel="Application Url"
        p:maxLength="40"
        p:required="false" >
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="40" />
    </property>
  </bean>  
  
</beans>