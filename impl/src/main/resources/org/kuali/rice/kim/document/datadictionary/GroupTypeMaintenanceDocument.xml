<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="KIMGroupTypeMaintenanceDocument" parent="KIMGroupTypeMaintenanceDocument-parentBean" />

  <bean id="KIMGroupTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kim.bo.GroupType" />
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="KIMGroupTypeMaintenanceDocument-GroupTypeInformation" />
        <ref bean="KIMGroupTypeMaintenanceDocument-GroupTypeDefaultAttributes" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>id</value>
      </list>
    </property>
    <property name="shortLabel" value="Group Type" />
    <property name="summary" value="Group Type Maintenance Document" />
    <property name="documentTypeName" value="KIMGroupTypeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="KGTM" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="authorizedGroups" >
            <set>
              <value>Kuali Identity Managers</value>
            </set>
          </property>
          <property name="action" value="initiate" />
        </bean>
      </list>
    </property>
    <property name="label" value="Group Type Maintenance Document" />
    <property name="description" value="This document is used to create or update Group Types." />
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="KIMGroupTypeMaintenanceDocument-GroupTypeInformation" parent="KIMGroupTypeMaintenanceDocument-GroupTypeInformation-parentBean" />

  <bean id="KIMGroupTypeMaintenanceDocument-GroupTypeInformation-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="id"
              p:readOnly="true" 
              p:required="true"
              p:defaultValueFinderClass="org.kuali.rice.kim.lookup.valuefinder.NextGroupTypeIdFinder" />
        <bean parent="MaintainableFieldDefinition"
              p:required="true"
              p:name="name" />
        <bean parent="MaintainableFieldDefinition"
              p:required="true"
              p:name="description" />
        <bean parent="MaintainableFieldDefinition"
              p:required="true"
              p:name="workflowDocumentType" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
    <property name="id" value="Group Type Information" />
    <property name="title" value="Group Type Information" />
  </bean>
  <bean id="KIMGroupTypeMaintenanceDocument-GroupTypeDefaultAttributes" parent="KIMGroupTypeMaintenanceDocument-GroupTypeDefaultAttributes-parentBean" />

  <bean id="KIMGroupTypeMaintenanceDocument-GroupTypeDefaultAttributes-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="businessObjectClass" value="org.kuali.rice.kim.bo.GroupTypeDefaultAttribute" />
          <property name="alwaysAllowCollectionDeletion" value="true" />
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
		            p:name="id"
		            p:readOnly="true" 
		            p:required="true"
		            p:defaultValueFinderClass="org.kuali.rice.kim.lookup.valuefinder.NextGroupTypeDefaultAttributeIdFinder" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="attributeTypeId" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="attributeName" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="description" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="required" />
              <bean parent="MaintainableFieldDefinition"
                    p:required="true"
                    p:name="active" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="versionNumber" />
            </list>
          </property>
          <property name="name" value="groupTypeDefaultAttributes" />
        </bean>
      </list>
    </property>
    <property name="id" value="Group Type Default Attributes" />
    <property name="title" value="Group Type Default Attributes" />
  </bean>
</beans>
