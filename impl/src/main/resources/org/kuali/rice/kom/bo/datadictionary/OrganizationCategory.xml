<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="OrganizationCategory" parent="OrganizationCategory-parentBean" />

  <bean id="OrganizationCategory-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kom.bo.OrganizationCategory" />
    <property name="inquiryDefinition" >
      <ref bean="OrganizationCategory-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="OrganizationCategory-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="id" />
    <property name="objectLabel" value="Organization Category" />
    <property name="attributes" >
      <list>
        <ref bean="OrganizationCategory-id" />
        <ref bean="OrganizationCategory-name" />
        <ref bean="OrganizationCategory-versionNumber" />
        <ref bean="OrganizationCategory-objectId" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="OrganizationCategory-id" parent="OrganizationCategory-id-parentBean" />

  <bean id="OrganizationCategory-id-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="name" value="id" />
    <property name="label" value="ID" />
  </bean>
  <bean id="OrganizationCategory-name" parent="OrganizationCategory-name-parentBean" />

  <bean id="OrganizationCategory-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="255" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="100" />
    </property>
    <property name="name" value="name" />
    <property name="label" value="Organization Category Name" />
  </bean>
  <bean id="OrganizationCategory-versionNumber" parent="OrganizationCategory-versionNumber-parentBean" />

  <bean id="OrganizationCategory-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
  <bean id="OrganizationCategory-objectId" parent="OrganizationCategory-objectId-parentBean" />

  <bean id="OrganizationCategory-objectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="OrganizationCategory-inquiryDefinition" parent="OrganizationCategory-inquiryDefinition-parentBean" />

  <bean id="OrganizationCategory-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="id" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
            </list>
          </property>
          <property name="title" value="" />
        </bean>
      </list>
    </property>
    <property name="title" value="Organization Category Inquiry" />
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="OrganizationCategory-lookupDefinition" parent="OrganizationCategory-lookupDefinition-parentBean" />

  <bean id="OrganizationCategory-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="instructions" value="Lookup a Organization Category" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="id" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="id" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
      </list>
    </property>
    <property name="title" value="Organization Category Lookup" />
  </bean>
</beans>
