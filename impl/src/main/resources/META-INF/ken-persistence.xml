<?xml version="1.0" encoding="UTF-8"?>
<persistence 
    version="1.0" 
    xmlns="http://java.sun.com/xml/ns/persistence" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  
  <persistence-unit name="ken-unit" transaction-type="JTA">
    <mapping-file>META-INF/ken-orm.xml</mapping-file>
    <class>org.kuali.rice.ken.bo.Notification</class>
    <class>org.kuali.rice.ken.bo.NotificationChannel</class>
    <class>org.kuali.rice.ken.bo.NotificationContentType</class>
    <class>org.kuali.rice.ken.bo.NotificationPriority</class>
    <class>org.kuali.rice.ken.bo.NotificationProducer</class>
    <class>org.kuali.rice.ken.bo.NotificationChannelReviewer</class>
    <class>org.kuali.rice.ken.bo.NotificationRecipient</class>
    <class>org.kuali.rice.ken.bo.NotificationRecipientList</class>
    <class>org.kuali.rice.ken.bo.NotificationSender</class>
    <class>org.kuali.rice.ken.bo.UserChannelSubscription</class>
    <class>org.kuali.rice.ken.bo.NotificationMessageDelivery</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
  </persistence-unit>

</persistence>
