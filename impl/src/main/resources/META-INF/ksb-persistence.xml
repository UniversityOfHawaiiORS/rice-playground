<?xml version="1.0" encoding="UTF-8"?>
<persistence 
    version="1.0" 
    xmlns="http://java.sun.com/xml/ns/persistence" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  
  <persistence-unit name="ksb-unit" transaction-type="JTA">
    <mapping-file>META-INF/ksb-orm.xml</mapping-file>
    <class>edu.iu.uis.eden.messaging.PersistedMessage</class>
    <class>edu.iu.uis.eden.messaging.PersistedMassagePayload</class>
    <class>edu.iu.uis.eden.messaging.ServiceInfo</class>
    <class>edu.iu.uis.eden.messaging.bam.BAMTargetEntry</class>
    <class>edu.iu.uis.eden.messaging.bam.BAMParam</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
  </persistence-unit>

</persistence>
