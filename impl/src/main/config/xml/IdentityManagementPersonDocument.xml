<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  
   <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">

<!--  for KIM Person/role/group document -->
        <documentType>
            <name>IdentityManagementPersonDocument</name>
            <description>Create Person Document</description>
            <label>Person Document</label>
            <postProcessorName>org.kuali.rice.kns.workflow.postprocessor.KualiPostProcessor</postProcessorName>
            <superUserWorkgroupName>WorkflowAdmin</superUserWorkgroupName>
            <blanketApproveWorkgroupName>WorkflowAdmin</blanketApproveWorkgroupName>
            <defaultExceptionWorkgroupName>WorkflowAdmin</defaultExceptionWorkgroupName>
            <docHandler>${application.url}/identityManagementPersonDocument.do?methodToCall=docHandler</docHandler>
            <routePaths>
                <routePath>
					<start name="Adhoc Routing" nextNode="Kuali Identity Managers Group Approval" />
					<requests name="Kuali Identity Managers Group Approval" />
                </routePath>
            </routePaths>
            <routeNodes>
		        <start name="Adhoc Routing">
		          <activationType>P</activationType>
		          <mandatoryRoute>false</mandatoryRoute>
		          <finalApproval>false</finalApproval>
		        </start>
		        <requests name="Kuali Identity Managers Group Approval">
		            <activationType>S</activationType>
		            <ruleSelector>Named</ruleSelector>
		            <ruleName>KIMPersonDocumentApproval</ruleName>
		        </requests>
            </routeNodes>
        </documentType>

<!-- end KIM document -->
  </documentTypes>
  
  
  <rules xmlns="ns:workflow/Rule" xsi:schemaLocation="ns:workflow/Rule resource:Rule">
    <rule>
       <name>KIMPersonDocumentApproval</name>
       <documentType>IdentityManagementPersonDocument</documentType>
       <description>This rule requires an approval from the KIM Kuali Identity Managers workgroup.</description>
       <ignorePrevious>true</ignorePrevious>
       <responsibilities>
	       <responsibility>
	          <workgroup>Kuali Identity Managers</workgroup>
	       </responsibility>
       </responsibilities>
    </rule>


</rules>


  
  
</data>
