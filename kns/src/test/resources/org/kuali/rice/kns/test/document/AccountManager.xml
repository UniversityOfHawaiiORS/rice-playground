<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "https://test.kuali.org/dtds/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.rice.kns.test.document.bo.AccountManager">

        <inquiry>
            <title>Account Manager Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="amId" forceInquiry="true" />
                <field attributeName="userName" forceInquiry="true" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Account Manager Lookup</title>

            <instructions>Look up an Account Manager</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="userName" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="amId" required="false" />
                <lookupField attributeName="userName" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="amId" forceInquiry="true" />
                <field attributeName="userName" forceInquiry="true"/>
            </resultFields>
            <resultSetLimit>90</resultSetLimit>
        </lookup>

        <titleAttribute>amId</titleAttribute>

        <attributes>

            <attribute name="amId">
                <label>Account Manager Id</label>
                <shortLabel>Account Manager Id</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>Account Manager Id</summary>
                <description>Account Manager Id</description>
			</attribute>

            <attribute name="userName" forceUppercase="false">
                <label>Account Manager User Name</label>
                <shortLabel>Account Manager User Name</shortLabel>

                <maxLength>30</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="30" />
                </control>

                <summary>Account Manager User Name</summary>
                <description>Account Manager User Name</description>
			</attribute>
		</attributes>
    </businessObject>

</dictionaryEntry>