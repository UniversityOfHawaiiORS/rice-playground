<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AccountManagerMaintenanceDocument" parent="AccountManagerMaintenanceDocument-parentBean" />

  <bean id="AccountManagerMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kns.test.document.bo.AccountManager" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="AccountManagerMaintenanceDocument-EditAccountManager" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>userName</value>
      </list>
    </property>
    <property name="shortLabel" value="AccountManagerMaintenanceDocument" />
    <property name="summary" value="Account Manager Maintenance Document" />
    <property name="documentTypeName" value="AccountManagerMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="AMMD" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="authorizedGroups" >
            <set>
              <value>WorkflowAdmin</value>
            </set>
          </property>
          <property name="action" value="initiate" />
        </bean>
      </list>
    </property>
    <property name="label" value="Account Manager Maintenance Document" />
    <property name="description" value="Document used to create or update Account Manager" />
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="AccountManagerMaintenanceDocument-EditAccountManager" parent="AccountManagerMaintenanceDocument-EditAccountManager-parentBean" />

  <bean id="AccountManagerMaintenanceDocument-EditAccountManager-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="amId"
              p:readOnly="true" />
        <bean parent="MaintainableFieldDefinition"
              p:required="true"
              p:name="userName" />
      </list>
    </property>
    <property name="id" value="Edit Account Manager" />
    <property name="title" value="Edit Account Manager" />
  </bean>
</beans>
