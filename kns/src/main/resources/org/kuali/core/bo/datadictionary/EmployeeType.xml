<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="EmployeeType" parent="EmployeeType-parentBean" />

  <bean id="EmployeeType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.core.bo.EmployeeType" />
    <property name="inquiryDefinition" >
      <ref bean="EmployeeType-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="EmployeeType-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="code" />
    <property name="attributes" >
      <list>
        <ref bean="EmployeeType-code" />
        <ref bean="EmployeeType-name" />
        <ref bean="EmployeeType-rowActiveIndicator" />
        <ref bean="EmployeeType-versionNumber" />
        <ref bean="EmployeeType-objectId" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="EmployeeType-code" parent="EmployeeType-code-parentBean" />

  <bean id="EmployeeType-code-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AlphaValidationPattern"
            p:exactLength="1" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="1" />
    </property>
    <property name="summary" value="Employee Type Code" />
    <property name="name" value="code" />
    <property name="label" value="Employee Type Code" />
    <property name="description" value="Employee Type Code" />
  </bean>
  <bean id="EmployeeType-name" parent="EmployeeType-name-parentBean" />

  <bean id="EmployeeType-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="description" />
    <property name="maxLength" value="30" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="32" />
    </property>
    <property name="summary" value="Employee Type Description" />
    <property name="name" value="name" />
    <property name="label" value="Employee Type Description" />
    <property name="description" value="The description of the Employee Type." />
  </bean>
  <bean id="EmployeeType-rowActiveIndicator" parent="EmployeeType-rowActiveIndicator-parentBean" />

  <bean id="EmployeeType-rowActiveIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Active Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="Active Indicator" />
    <property name="name" value="rowActiveIndicator" />
    <property name="label" value="Active Indicator" />
    <property name="description" value="Employee Type Active Indicator" />
  </bean>
  <bean id="EmployeeType-versionNumber" parent="EmployeeType-versionNumber-parentBean" />

  <bean id="EmployeeType-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
  <bean id="EmployeeType-objectId" parent="EmployeeType-objectId-parentBean" />

  <bean id="EmployeeType-objectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="EmployeeType-inquiryDefinition" parent="EmployeeType-inquiryDefinition-parentBean" />

  <bean id="EmployeeType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="code" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
              <bean parent="FieldDefinition"
                    p:attributeName="rowActiveIndicator" />
            </list>
          </property>
          <property name="title" value="" />
        </bean>
      </list>
    </property>
    <property name="title" value="Employee Type Inquiry" />
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="EmployeeType-lookupDefinition" parent="EmployeeType-lookupDefinition-parentBean" />

  <bean id="EmployeeType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="instructions" value="Lookup an Employee Type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="sortAscending" value="false" />
        <property name="attributeNames" >
          <list>
            <value>code</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="rowActiveIndicator" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code"
              p:noInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="rowActiveIndicator" />
      </list>
    </property>
    <property name="title" value="Employee Type Lookup" />
  </bean>
</beans>
