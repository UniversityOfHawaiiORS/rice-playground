<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="DocumentType" parent="DocumentType-parentBean" />

  <bean id="DocumentType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.core.bo.DocumentType" />
    <property name="inquiryDefinition" >
      <ref bean="DocumentType-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="DocumentType-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="financialDocumentTypeCode" />
    <property name="attributes" >
      <list>
        <ref bean="DocumentType-financialDocumentTypeCode" />
        <ref bean="DocumentType-financialDocumentGroupCode" />
        <ref bean="DocumentType-financialDocumentName" />
        <ref bean="DocumentType-finEliminationsEligibilityIndicator" />
        <ref bean="DocumentType-finDocumentTypeActiveIndicator" />
        <ref bean="DocumentType-finDocumentRoutingRuleCode" />
        <ref bean="DocumentType-finDocAutomaticApprovalDays" />
        <ref bean="DocumentType-financialDocumentBalancedIndicator" />
        <ref bean="DocumentType-transactionScrubberOffsetGenerationIndicator" />
        <ref bean="DocumentType-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="DocumentType-financialDocumentTypeCode" parent="DocumentType-financialDocumentTypeCode-parentBean" />

  <bean id="DocumentType-financialDocumentTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Doc Type Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="Document Type Code" />
    <property name="name" value="financialDocumentTypeCode" />
    <property name="label" value="Document Type Code" />
    <property name="description" value="The code for the business function of all documents of this type." />
  </bean>
  <bean id="DocumentType-financialDocumentGroupCode" parent="DocumentType-financialDocumentGroupCode-parentBean" />

  <bean id="DocumentType-financialDocumentGroupCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Doc Group Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="financialDocumentGroupCode" />
    <property name="name" value="financialDocumentGroupCode" />
    <property name="label" value="Document Group Code" />
    <property name="description" value="financialDocumentGroupCode description..." />
  </bean>
  <bean id="DocumentType-financialDocumentName" parent="DocumentType-financialDocumentName-parentBean" />

  <bean id="DocumentType-financialDocumentName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Doc Name" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
    <property name="summary" value="financialDocumentName" />
    <property name="name" value="financialDocumentName" />
    <property name="label" value="Document Name" />
    <property name="description" value="financialDocumentName Description..." />
  </bean>
  <bean id="DocumentType-finEliminationsEligibilityIndicator" parent="DocumentType-finEliminationsEligibilityIndicator-parentBean" />

  <bean id="DocumentType-finEliminationsEligibilityIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Elim Eligible" />
    <property name="maxLength" value="1" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="finEliminationsEligibilityIndicator" />
    <property name="name" value="finEliminationsEligibilityIndicator" />
    <property name="label" value="Eliminations Eligibility" />
    <property name="description" value="finEliminationsEligibilityIndicator Description..." />
  </bean>
  <bean id="DocumentType-finDocumentTypeActiveIndicator" parent="DocumentType-finDocumentTypeActiveIndicator-parentBean" />

  <bean id="DocumentType-finDocumentTypeActiveIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Active" />
    <property name="maxLength" value="1" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="finDocumentTypeActiveIndicator" />
    <property name="name" value="finDocumentTypeActiveIndicator" />
    <property name="label" value="Document Type Active" />
    <property name="description" value="finDocumentTypeActiveIndicator Description..." />
  </bean>
  <bean id="DocumentType-finDocumentRoutingRuleCode" parent="DocumentType-finDocumentRoutingRuleCode-parentBean" />

  <bean id="DocumentType-finDocumentRoutingRuleCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Rule Code" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
    <property name="summary" value="finDocumentRoutingRuleCode" />
    <property name="name" value="finDocumentRoutingRuleCode" />
    <property name="label" value="Routing Rule Code" />
    <property name="description" value="finDocumentRoutingRuleCode description..." />
  </bean>
  <bean id="DocumentType-finDocAutomaticApprovalDays" parent="DocumentType-finDocAutomaticApprovalDays-parentBean" />

  <bean id="DocumentType-finDocAutomaticApprovalDays-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Auto Approve Days" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="7" />
    </property>
    <property name="summary" value="finDocAutomaticApprovalDays" />
    <property name="name" value="finDocAutomaticApprovalDays" />
    <property name="label" value="Automatic Approval Days" />
    <property name="description" value="finDocAutomaticApprovalDays ..." />
  </bean>
  <bean id="DocumentType-financialDocumentBalancedIndicator" parent="DocumentType-financialDocumentBalancedIndicator-parentBean" />

  <bean id="DocumentType-financialDocumentBalancedIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Balanced" />
    <property name="maxLength" value="1" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="financialDocumentBalancedIndicator" />
    <property name="name" value="financialDocumentBalancedIndicator" />
    <property name="label" value="Document Balanced" />
    <property name="description" value="financialDocumentBalancedIndicator Description..." />
  </bean>
  <bean id="DocumentType-transactionScrubberOffsetGenerationIndicator" parent="DocumentType-transactionScrubberOffsetGenerationIndicator-parentBean" />

  <bean id="DocumentType-transactionScrubberOffsetGenerationIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Scrubber Offset" />
    <property name="maxLength" value="1" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="transactionScrubberOffsetGenerationIndicator" />
    <property name="name" value="transactionScrubberOffsetGenerationIndicator" />
    <property name="label" value="Transaction Scrubber Offset Generation" />
    <property name="description" value="transactionScrubberOffsetGenerationIndicator Description..." />
  </bean>
  <bean id="DocumentType-versionNumber" parent="DocumentType-versionNumber-parentBean" />

  <bean id="DocumentType-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="DocumentType-inquiryDefinition" parent="DocumentType-inquiryDefinition-parentBean" />

  <bean id="DocumentType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentGroupCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentName" />
              <bean parent="FieldDefinition"
                    p:attributeName="finEliminationsEligibilityIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="finDocumentTypeActiveIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="finDocumentRoutingRuleCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="finDocAutomaticApprovalDays" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentBalancedIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="transactionScrubberOffsetGenerationIndicator" />
            </list>
          </property>
          <property name="title" value="Header Text" />
        </bean>
      </list>
    </property>
    <property name="title" value="Document Type Inquiry" />
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="DocumentType-lookupDefinition" parent="DocumentType-lookupDefinition-parentBean" />

  <bean id="DocumentType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="instructions" value="Lookup a Document Type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>financialDocumentTypeCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentGroupCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentName" />
        <bean parent="FieldDefinition"
              p:attributeName="finEliminationsEligibilityIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="finDocumentTypeActiveIndicator"
              p:defaultValue="Y" />
        <bean parent="FieldDefinition"
              p:attributeName="finDocumentRoutingRuleCode" />
        <bean parent="FieldDefinition"
              p:attributeName="finDocAutomaticApprovalDays" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentBalancedIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="transactionScrubberOffsetGenerationIndicator" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentGroupCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentName" />
        <bean parent="FieldDefinition"
              p:attributeName="finEliminationsEligibilityIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="finDocumentTypeActiveIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="finDocumentRoutingRuleCode" />
        <bean parent="FieldDefinition"
              p:attributeName="finDocAutomaticApprovalDays" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentBalancedIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="transactionScrubberOffsetGenerationIndicator" />
      </list>
    </property>
    <property name="title" value="Document Type Lookup" />
  </bean>
</beans>
