<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="DocumentGroup" parent="DocumentGroup-parentBean" />

  <bean id="DocumentGroup-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.core.bo.DocumentGroup" />
    <property name="inquiryDefinition" >
      <ref bean="DocumentGroup-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="DocumentGroup-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="DocumentGroup-financialDocumentClassCode" />
        <ref bean="DocumentGroup-financialDocumentGroupCode" />
        <ref bean="DocumentGroup-financialDocumentGroupName" />
        <ref bean="DocumentGroup-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="DocumentGroup-financialDocumentClassCode" parent="DocumentGroup-financialDocumentClassCode-parentBean" />

  <bean id="DocumentGroup-financialDocumentClassCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="financialDocumentClassCode" />
    <property name="name" value="financialDocumentClassCode" />
    <property name="label" value="Financial Document Class Code" />
    <property name="description" value="financialDocumentClassCode description..." />
  </bean>
  <bean id="DocumentGroup-financialDocumentGroupCode" parent="DocumentGroup-financialDocumentGroupCode-parentBean" />

  <bean id="DocumentGroup-financialDocumentGroupCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="financialDocumentGroupCode" />
    <property name="name" value="financialDocumentGroupCode" />
    <property name="label" value="Financial Document Group Code" />
    <property name="description" value="financialDocumentGroupCode description..." />
  </bean>
  <bean id="DocumentGroup-financialDocumentGroupName" parent="DocumentGroup-financialDocumentGroupName-parentBean" />

  <bean id="DocumentGroup-financialDocumentGroupName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
    <property name="summary" value="financialDocumentGroupName" />
    <property name="name" value="financialDocumentGroupName" />
    <property name="label" value="Financial Document Group Name" />
    <property name="description" value="financialDocumentGroupName description..." />
  </bean>
  <bean id="DocumentGroup-versionNumber" parent="DocumentGroup-versionNumber-parentBean" />

  <bean id="DocumentGroup-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="DocumentGroup-inquiryDefinition" parent="DocumentGroup-inquiryDefinition-parentBean" />

  <bean id="DocumentGroup-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentClassCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentGroupCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentGroupName" />
            </list>
          </property>
          <property name="title" value="Header Text" />
        </bean>
      </list>
    </property>
    <property name="title" value="Document Group Inquiry" />
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="DocumentGroup-lookupDefinition" parent="DocumentGroup-lookupDefinition-parentBean" />

  <bean id="DocumentGroup-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="instructions" value="Look up a Document Group" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentClassCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentGroupCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentGroupName" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentClassCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentGroupCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentGroupName" />
      </list>
    </property>
    <property name="title" value="Document Group Lookup" />
  </bean>
</beans>
