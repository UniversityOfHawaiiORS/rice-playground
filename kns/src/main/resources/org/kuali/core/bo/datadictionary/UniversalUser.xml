            p:includeKeyInLabel="false" />
    </property>
    <property name="name" value="employeeStatusCode" />
  </bean>
  <bean id="UniversalUser-employeeTypeCode" parent="UniversalUser-employeeTypeCode-parentBean" />

  <bean id="UniversalUser-employeeTypeCode-parentBean" abstract="true" parent="EmployeeType-code">
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.EmployeeTypeValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="name" value="employeeTypeCode" />
  </bean>
  <bean id="UniversalUser-student" parent="UniversalUser-student-parentBean" />

  <bean id="UniversalUser-student-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean">
    <property name="shortLabel" value="Stdt?" />
    <property name="summary" value="Student?" />
    <property name="name" value="student" />
    <property name="label" value="Student?" />
    <property name="description" value="Is this person a student?" />
  </bean>
  <bean id="UniversalUser-staff" parent="UniversalUser-staff-parentBean" />

  <bean id="UniversalUser-staff-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean">
    <property name="shortLabel" value="Stf?" />
    <property name="summary" value="Staff?" />
    <property name="name" value="staff" />
    <property name="label" value="Staff?" />
    <property name="description" value="Is this person a staff member?" />
  </bean>
  <bean id="UniversalUser-faculty" parent="UniversalUser-faculty-parentBean" />

  <bean id="UniversalUser-faculty-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean">
    <property name="shortLabel" value="Fcty?" />
    <property name="summary" value="Faculty?" />
    <property name="name" value="faculty" />
    <property name="label" value="Faculty?" />
    <property name="description" value="Is this person a faculty member?" />
  </bean>
  <bean id="UniversalUser-affiliate" parent="UniversalUser-affiliate-parentBean" />

  <bean id="UniversalUser-affiliate-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean">
    <property name="shortLabel" value="Aflt?" />
    <property name="summary" value="Affiliate?" />
    <property name="name" value="affiliate" />
    <property name="label" value="Affiliate?" />
    <property name="description" value="Is this person an affiliate of the institution?" />
  </bean>
  <bean id="UniversalUser-primaryDepartmentCode" parent="UniversalUser-primaryDepartmentCode-parentBean" />

  <bean id="UniversalUser-primaryDepartmentCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="DeptId" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Chart-Org" />
    <property name="name" value="primaryDepartmentCode" />
    <property name="label" value="Department Id" />
    <property name="description" value="Chart-Org" />
  </bean>
  <bean id="UniversalUser-personEmailAddress" parent="UniversalUser-personEmailAddress-parentBean" />

  <bean id="UniversalUser-personEmailAddress-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Email" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="EmailAddressValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="summary" value="Email Address" />
    <property name="name" value="personEmailAddress" />
    <property name="label" value="Email Address" />
    <property name="description" value="Email Address" />
  </bean>
  <bean id="UniversalUser-personFirstName" parent="UniversalUser-personFirstName-parentBean" />

  <bean id="UniversalUser-personFirstName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="1st Name" />
    <property name="maxLength" value="20" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="20" />
    </property>
    <property name="summary" value="Person First Name" />
    <property name="name" value="personFirstName" />
    <property name="label" value="First Name" />
    <property name="description" value="Person First Name" />
  </bean>
  <bean id="UniversalUser-personLastName" parent="UniversalUser-personLastName-parentBean" />

  <bean id="UniversalUser-personLastName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Lst Name" />
    <property name="maxLength" value="20" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="20" />
    </property>
    <property name="summary" value="Person Last Name" />
    <property name="name" value="personLastName" />
    <property name="label" value="Last Name" />
    <property name="description" value="Person Last Name" />
  </bean>
  <bean id="UniversalUser-personMiddleName" parent="UniversalUser-personMiddleName-parentBean" />

  <bean id="UniversalUser-personMiddleName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Mdl Name" />
    <property name="maxLength" value="20" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="20" />
    </property>
    <property name="summary" value="Person Last Name" />
    <property name="name" value="personMiddleName" />
    <property name="label" value="Middle Name" />
    <property name="description" value="Person Last Name" />
  </bean>
  <bean id="UniversalUser-personCampusAddress" parent="UniversalUser-personCampusAddress-parentBean" />

  <bean id="UniversalUser-personCampusAddress-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Cmp Addr" />
    <property name="maxLength" value="55" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="summary" value="Person Campus Address" />
    <property name="name" value="personCampusAddress" />
    <property name="label" value="Campus Address" />
    <property name="description" value="Person Campus Address" />
  </bean>
  <bean id="UniversalUser-personLocalPhoneNumber" parent="UniversalUser-personLocalPhoneNumber-parentBean" />

  <bean id="UniversalUser-personLocalPhoneNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Phn Nbr" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
    <property name="summary" value="Person Local Phone Number" />
    <property name="name" value="personLocalPhoneNumber" />
    <property name="label" value="Local Phone Number" />
    <property name="description" value="Person Local Phone Number" />
  </bean>
  <bean id="UniversalUser-personBaseSalaryAmount" parent="UniversalUser-personBaseSalaryAmount-parentBean" />

  <bean id="UniversalUser-personBaseSalaryAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Salary" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FixedPointValidationPattern"
            p:precision="19"
            p:scale="2" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Person Base Salary" />
    <property name="name" value="personBaseSalaryAmount" />
    <property name="label" value="Base Salary" />
    <property name="description" value="Person Base Salary" />
  </bean>
  <bean id="UniversalUser-personTaxIdentifier" parent="UniversalUser-personTaxIdentifier-parentBean" />

  <bean id="UniversalUser-personTaxIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Tax ID" />
    <property name="maxLength" value="9" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="displayMask" >
      <bean parent="Mask">
        <property name="maskFormatter" >
          <bean parent="MaskFormatterLiteral"
                p:literal="***************" />
        </property>
      </bean>
    </property>
    <property name="displayWorkgroup" value="UNIVERSAL_USER_MAINTAINERS" />
    <property name="summary" value="SSN" />
    <property name="name" value="personTaxIdentifier" />
    <property name="label" value="Tax Identifier" />
    <property name="description" value="Social Security Number" />
  </bean>
  <bean id="UniversalUser-personTaxIdentifierTypeCode" parent="UniversalUser-personTaxIdentifierTypeCode-parentBean" />

  <bean id="UniversalUser-personTaxIdentifierTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Tax ID Type" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.PersonTaxIdentifierTypeCodeValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="Tax Identifier Type" />
    <property name="name" value="personTaxIdentifierTypeCode" />
    <property name="label" value="Tax Identifier Type" />
    <property name="description" value="Tax Identifier Type" />
  </bean>
  <bean id="UniversalUser-financialSystemsEncryptedPasswordText" parent="UniversalUser-financialSystemsEncryptedPasswordText-parentBean" />

  <bean id="UniversalUser-financialSystemsEncryptedPasswordText-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Password" />
    <property name="maxLength" value="200" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="20" />
    </property>
    <property name="displayMask" >
      <bean parent="Mask">
        <property name="maskFormatter" >
          <bean parent="MaskFormatterLiteral"
                p:literal="***************" />
        </property>
      </bean>
    </property>
    <property name="displayWorkgroup" value="UNIVERSAL_USER_MAINTAINERS" />
    <property name="summary" value="Password" />
    <property name="name" value="financialSystemsEncryptedPasswordText" />
    <property name="label" value="Password" />
    <property name="description" value="Password" />
  </bean>
  <bean id="UniversalUser-campusCode" parent="UniversalUser-campusCode-parentBean" />

  <bean id="UniversalUser-campusCode-parentBean" abstract="true" parent="Campus-campusCode">
    <property name="required" value="false" />
    <property name="description" value="The code for the Campus of the user." />
  </bean>
  <bean id="UniversalUser-activeForAnyModule" parent="UniversalUser-activeForAnyModule-parentBean" />

  <bean id="UniversalUser-activeForAnyModule-parentBean" abstract="true" parent="AttributeReferenceDummy-activeIndicator">
    <property name="shortLabel" value="ActiveKualiUser" />
    <property name="summary" value="&amp;nbsp;" />
    <property name="name" value="activeForAnyModule" />
    <property name="label" value="Active Kuali User" />
    <property name="description" value="Indicates whether or not this UniversalUser is also an active user for any module of the application." />
  </bean>
  <bean id="UniversalUser-activeModuleCodeString" parent="UniversalUser-activeModuleCodeString-parentBean" />

  <bean id="UniversalUser-activeModuleCodeString-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Modules" />
    <property name="maxLength" value="200" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.InstalledModulesValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="List of module codes for which this person is an active user." />
    <property name="name" value="activeModuleCodeString" />
    <property name="label" value="Active Module List" />
    <property name="description" value="A comma-delimited list representing the modules to which this UniversalUser has access." />
  </bean>
  <bean id="UniversalUser-versionNumber" parent="UniversalUser-versionNumber-parentBean" />

  <bean id="UniversalUser-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="UniversalUser-inquiryDefinition" parent="UniversalUser-inquiryDefinition-parentBean" />

  <bean id="UniversalUser-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="personUserIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="personUniversalIdentifier"
                    p:noInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="personPayrollIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="personName" />
              <bean parent="FieldDefinition"
                    p:attributeName="campusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="primaryDepartmentCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="staff" />
              <bean parent="FieldDefinition"
                    p:attributeName="faculty" />
              <bean parent="FieldDefinition"
                    p:attributeName="student" />
              <bean parent="FieldDefinition"
                    p:attributeName="affiliate" />
              <bean parent="FieldDefinition"
                    p:attributeName="employeeStatusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="employeeTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="personEmailAddress" />
              <bean parent="FieldDefinition"
                    p:attributeName="activeModuleCodeString" />
            </list>
          </property>
          <property name="title" value="Header Text" />
        </bean>
      </list>
    </property>
    <property name="title" value="User Inquiry" />
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="UniversalUser-lookupDefinition" parent="UniversalUser-lookupDefinition-parentBean" />

  <bean id="UniversalUser-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="lookupableID" value="universalUserLookupable" />
    <property name="instructions" value="Lookup a User" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>personUserIdentifier</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="personUserIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="personPayrollIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="personLastName" />
        <bean parent="FieldDefinition"
              p:attributeName="personFirstName" />
        <bean parent="FieldDefinition"
              p:attributeName="personEmailAddress" />
        <bean parent="FieldDefinition"
              p:attributeName="campusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="primaryDepartmentCode" />
        <bean parent="FieldDefinition"
              p:attributeName="staff" />
        <bean parent="FieldDefinition"
              p:attributeName="faculty" />
        <bean parent="FieldDefinition"
              p:attributeName="student" />
        <bean parent="FieldDefinition"
              p:attributeName="affiliate" />
        <bean parent="FieldDefinition"
              p:attributeName="employeeStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="employeeTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="activeModuleCodeString" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="personUserIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="personPayrollIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="personName" />
        <bean parent="FieldDefinition"
              p:attributeName="campusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="primaryDepartmentCode" />
        <bean parent="FieldDefinition"
              p:attributeName="employeeStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="employeeTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="activeModuleCodeString" />
      </list>
    </property>
    <property name="title" value="User Lookup" />
  </bean>
</beans>
