<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="UniversalUser" parent="UniversalUser-parentBean" />

  <bean id="UniversalUser-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.core.bo.user.UniversalUser" />
    <property name="inquiryDefinition" >
      <ref bean="UniversalUser-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="UniversalUser-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="personUniversalIdentifier" />
    <property name="attributes" >
      <list>
        <ref bean="UniversalUser-personUniversalIdentifier" />
        <ref bean="UniversalUser-personUserIdentifier" />
        <ref bean="UniversalUser-personPayrollIdentifier" />
        <ref bean="UniversalUser-personName" />
        <ref bean="UniversalUser-employeeStatusCode" />
        <ref bean="UniversalUser-employeeTypeCode" />
        <ref bean="UniversalUser-student" />
        <ref bean="UniversalUser-staff" />
        <ref bean="UniversalUser-faculty" />
        <ref bean="UniversalUser-affiliate" />
        <ref bean="UniversalUser-primaryDepartmentCode" />
        <ref bean="UniversalUser-personEmailAddress" />
        <ref bean="UniversalUser-personFirstName" />
        <ref bean="UniversalUser-personLastName" />
        <ref bean="UniversalUser-personMiddleName" />
        <ref bean="UniversalUser-personCampusAddress" />
        <ref bean="UniversalUser-personLocalPhoneNumber" />
        <ref bean="UniversalUser-personBaseSalaryAmount" />
        <ref bean="UniversalUser-personTaxIdentifier" />
        <ref bean="UniversalUser-personTaxIdentifierTypeCode" />
        <ref bean="UniversalUser-financialSystemsEncryptedPasswordText" />
        <ref bean="UniversalUser-campusCode" />
        <ref bean="UniversalUser-activeForAnyModule" />
        <ref bean="UniversalUser-activeModuleCodeString" />
        <ref bean="UniversalUser-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="UniversalUser-personUniversalIdentifier" parent="UniversalUser-personUniversalIdentifier-parentBean" />

  <bean id="UniversalUser-personUniversalIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Univ User Id" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Universal User ID" />
    <property name="name" value="personUniversalIdentifier" />
    <property name="label" value="Universal User ID" />
    <property name="description" value="Universal User ID" />
  </bean>
  <bean id="UniversalUser-personUserIdentifier" parent="UniversalUser-personUserIdentifier-parentBean" />

  <bean id="UniversalUser-personUserIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="User ID" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="summary" value="User ID" />
    <property name="name" value="personUserIdentifier" />
    <property name="label" value="User ID" />
    <property name="description" value="User ID" />
  </bean>
  <bean id="UniversalUser-personPayrollIdentifier" parent="UniversalUser-personPayrollIdentifier-parentBean" />

  <bean id="UniversalUser-personPayrollIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Empl ID" />
    <property name="maxLength" value="11" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="13" />
    </property>
    <property name="summary" value="Empl ID" />
    <property name="name" value="personPayrollIdentifier" />
    <property name="label" value="Employee ID" />
    <property name="description" value="This field lists the individualï¿½s HRMS Employee ID.  
					This is a unique 9-digit number assigned to each 
					employee as they are entered into the HRMS system." />
  </bean>
  <bean id="UniversalUser-personName" parent="UniversalUser-personName-parentBean" />

  <bean id="UniversalUser-personName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="80" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="Display Name" />
    <property name="name" value="personName" />
    <property name="label" value="Display Name" />
    <property name="description" value="Name" />
  </bean>
  <bean id="UniversalUser-employeeStatusCode" parent="UniversalUser-employeeStatusCode-parentBean" />

  <bean id="UniversalUser-employeeStatusCode-parentBean" abstract="true" parent="EmployeeStatus-code">
