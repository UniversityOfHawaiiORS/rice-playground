<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="EmployeeStatus" parent="EmployeeStatus-parentBean" />

  <bean id="EmployeeStatus-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.core.bo.EmployeeStatus" />
    <property name="inquiryDefinition" >
      <ref bean="EmployeeStatus-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="EmployeeStatus-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="code" />
    <property name="attributes" >
      <list>
        <ref bean="EmployeeStatus-code" />
        <ref bean="EmployeeStatus-name" />
        <ref bean="EmployeeStatus-rowActiveIndicator" />
        <ref bean="EmployeeStatus-versionNumber" />
        <ref bean="EmployeeStatus-objectId" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="EmployeeStatus-code" parent="EmployeeStatus-code-parentBean" />

  <bean id="EmployeeStatus-code-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AlphaValidationPattern"
            p:exactLength="1" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="1" />
    </property>
    <property name="summary" value="Employee Status Code" />
    <property name="name" value="code" />
    <property name="label" value="Employee Status Code" />
    <property name="description" value="Employee Status Code" />
  </bean>
  <bean id="EmployeeStatus-name" parent="EmployeeStatus-name-parentBean" />

  <bean id="EmployeeStatus-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="description" />
    <property name="maxLength" value="30" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="32" />
    </property>
    <property name="summary" value="Employee Status Description" />
    <property name="name" value="name" />
    <property name="label" value="Employee Status Description" />
    <property name="description" value="The description of the Employee Status." />
  </bean>
  <bean id="EmployeeStatus-rowActiveIndicator" parent="EmployeeStatus-rowActiveIndicator-parentBean" />

  <bean id="EmployeeStatus-rowActiveIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Active Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="Active Indicator" />
    <property name="name" value="rowActiveIndicator" />
    <property name="label" value="Active Indicator" />
    <property name="description" value="Employee Status Active Indicator" />
  </bean>
  <bean id="EmployeeStatus-versionNumber" parent="EmployeeStatus-versionNumber-parentBean" />

  <bean id="EmployeeStatus-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
  <bean id="EmployeeStatus-objectId" parent="EmployeeStatus-objectId-parentBean" />

  <bean id="EmployeeStatus-objectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="EmployeeStatus-inquiryDefinition" parent="EmployeeStatus-inquiryDefinition-parentBean" />

  <bean id="EmployeeStatus-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="code" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
              <bean parent="FieldDefinition"
                    p:attributeName="rowActiveIndicator" />
            </list>
          </property>
          <property name="title" value="Header Text" />
        </bean>
      </list>
    </property>
    <property name="title" value="Employee Status Inquiry" />
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="EmployeeStatus-lookupDefinition" parent="EmployeeStatus-lookupDefinition-parentBean" />

  <bean id="EmployeeStatus-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="instructions" value="Lookup an Employee Status Code" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="sortAscending" value="false" />
        <property name="attributeNames" >
          <list>
            <value>code</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="rowActiveIndicator" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code"
              p:noInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="rowActiveIndicator" />
      </list>
    </property>
    <property name="title" value="Employee Status Lookup" />
  </bean>
</beans>
